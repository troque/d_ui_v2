eActual, setPageActual] = useState(1);\r\n    const [getModalState, setModalState] = useState({ title: \"\", message: \"\", show: false, button: false });\r\n\r\n    // Constantes de botones\r\n    const [getRoutes, setRoutes] = useState({\r\n        crear_registro: \"/Cargos/Add\",\r\n        consultar_registros: \"/Cargos\",\r\n        muestra_atras: false,\r\n        id_etapa: global.Constants.ETAPAS.NINGUNA,\r\n        muestra_inactivos: true,\r\n        modulo: \"ADMIN_Otros\",\r\n    });\r\n\r\n    // Metodo encargado de setear los valores inactivos\r\n    const handleCallback = (childData) => {\r\n        setEstadoLista(childData);\r\n        cargarCargos(childData);\r\n    }\r\n\r\n    // Columnas de la tabla\r\n    const columns = [\r\n        {\r\n            name: 'NOMBRE',\r\n            selector: genero => genero.attributes.nombre,\r\n            sortable: true,\r\n            width: \"50%\"\r\n        },\r\n        {\r\n            name: 'ESTADO',\r\n            selector: genero => (genero.attributes.estado == \"1\" ? 'ACTIVO' : 'INACTIVO'),\r\n            sortable: true,\r\n            width: \"10%\"\r\n        },\r\n        {\r\n            name: 'ACCIONES',\r\n            cell: row =>\r\n                <div>\r\n                    <Link to={`${row.id}`}>\r\n                        <button type=\"button\" className=\"btn btn btn-primary\" title='Editar'>\r\n                            <i className=\"fa fa-fw fa-edit\"></i>\r\n                        </button>\r\n                    </Link>\r\n                </div>,\r\n            width: \"10%\"\r\n        }\r\n    ];\r\n\r\n    // Se carga la informacion\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n\r\n            // Se llama el metodo encargado de cargar la informacion\r\n            cargarCargos(1);\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n    const cargarCargos = (estado) => {\r\n        window.showSpinner(true);\r\n        GenericApi.getAllGeneric('getCargos/'+estado).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setCargosLista(datos);\r\n                    window.showSpinner(false);\r\n                }\r\n                else{\r\n                    window.showSpinner(false);\r\n                    setModalState({ title: \"LISTA DE NIVEL JERÁRQUICO :: ERROR\", message: datos.error.toString(), show: true, alert: global.Constants.TIPO_ALERTA.ERROR, button: true });\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const handlePageChange = page => {\r\n        setPageActual(page);\r\n    }\r\n\r\n    const handlePerRowsChange = async (newPerPage, page) => {\r\n        setPerPage(newPerPage);\r\n        setPageActual(page);\r\n    }\r\n\r\n    return (\r\n        <div>\r\n\r\n            <Spinner />\r\n            <ModalInfo data={getModalState} />\r\n\r\n            <div className=\"w2d_block let\">\r\n                <nav aria-label=\"breadcrumb\">\r\n                    <ol className=\"breadcrumb breadcrumb-alt push\">\r\n                        <li className=\"breadcrumb-item\"> <small>Administración</small></li>\r\n                        <li className=\"breadcrumb-item\"> <small>Otros</small></li>\r\n                        <li className=\"breadcrumb-item\"> <small>Nivel Jerárquico</small></li>\r\n                    </ol>\r\n                </nav>\r\n            </div>\r\n\r\n            <div className=\"block block-themed\">\r\n                <div className=\"block-header\">\r\n                    <h3 className=\"block-title\">ADMINISTRACIÓN :: NIVELES JERÁRQUICOS {getEstadoLista == global.Constants.ESTADOS.INACTIVO ? \"INACTIVOS\" : \"ACTIVOS\"}</h3>\r\n\r\n                </div>\r\n                <div className=\"block-content\">\r\n                    <div className='row'>\r\n                        <div className='col-md-3'>\r\n                            <div className=\"form-group \">\r\n                                <input type=\"text\" id=\"search\" name=\"search\" value={getSeach} onChange={e => setSeach(e.target.value)} onKeyPress={(e) => { e.key === 'Enter' && e.preventDefault(); }} className=\"form-control border border-success\" placeholder=\"Buscar\" />\r\n                            </div>\r\n                        </div>\r\n                        <ListaBotones getRoutes={getRoutes} parentCallback={handleCallback} id=\"botonesNavegacion\" name=\"botonesNavegacion\" />\r\n\r\n                    </div>\r\n                    <div className=\"row\">\r\n                        <div className=\"col-sm-12\">\r\n\r\n                            <DataTable className=\"table table-bordered table-striped table-vcenter js-dataTable-full\"\r\n                                columns={columns}\r\n                                data={getCargosLista.data.filter((suggestion) => {\r\n                                    if (getSeach === \"\") {\r\n                                        if (suggestion.attributes.estado == getEstadoLista) {\r\n                                            return suggestion;\r\n                                        }\r\n                                    } else if (\r\n                                        ((suggestion.id\r\n                                            + quitarAcentos(suggestion.attributes.nombre)\r\n                                            + (suggestion.attributes.estado == \"1\" ? 'Activo' : 'Inactivo')).toLowerCase().includes(quitarAcentos(getSeach.toLowerCase()))\r\n                                            && (suggestion.attributes.estado == getEstadoLista))) {\r\n                                        return suggestion;\r\n                                    }\r\n                                })}\r\n                                perPage={perPage}\r\n                                page={pageActual}\r\n                                pagination\r\n                                noDataComponent=\"Sin datos\"\r\n                                paginationTotalRows={getCargosLista.data.length}\r\n                                onChangePage={handlePageChange}\r\n                                onChangeRowsPerPage={handlePerRowsChange}\r\n                                defaultSortFieldId=\"Nombre\"\r\n                                striped\r\n                                paginationComponentOptions={{ rowsPerPageText: 'Filas por página' }}\r\n                                defaultSortAsc={false}\r\n                            />\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default CargosLista;"]}}