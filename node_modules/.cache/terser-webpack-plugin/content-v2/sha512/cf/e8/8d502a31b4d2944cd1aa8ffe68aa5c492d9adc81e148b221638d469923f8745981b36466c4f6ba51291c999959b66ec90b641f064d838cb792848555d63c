ckbox' ? target.checked : target.value;\r\n        const name = target.name;\r\n\r\n        if (name == \"nombre\") {\r\n            setNombre(value);\r\n        }\r\n        if (name == \"apellido\") {\r\n            setApellido(value);\r\n        }\r\n\r\n    }\r\n\r\n    const cargarPendientes = (page, perPage) => {\r\n\r\n        const data = {\r\n            \"data\": {\r\n                \"type\": \"pendietes\",\r\n                \"attributes\": {\r\n                    \"fecha\": '',\r\n                    'per_page': perPage,\r\n                    'current_page': page,\r\n                    \"usuario_actual\": usuario\r\n                }\r\n            }\r\n        }\r\n        GenericApi.getByDataGeneric('mis-pendientes-filter-actual', data).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setListaPendientes(datos)\r\n                    window.showSpinner(false);\r\n                }\r\n                else {\r\n                    setModalState({ title: \"Traslado de casos :: Administración\", message: datos.error.toString(), show: true, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                    window.showSpinner(false);\r\n                }\r\n\r\n            }\r\n        )\r\n\r\n    }\r\n\r\n    const enviarDatos = (datos) => {\r\n\r\n    }\r\n\r\n    const handlePageChange = page => {\r\n        setPageActual(page);\r\n    }\r\n\r\n    const handlePerRowsChange = async (newPerPage, page) => {\r\n        setPerPage(newPerPage);\r\n        setPageActual(page);\r\n\r\n    }\r\n    const handleChange = (state) => {\r\n        setSelectedData(state.selectedRows);\r\n    };\r\n\r\n\r\n    const buscarUsuario = () => {\r\n        window.showSpinner(true);\r\n\r\n        try {\r\n\r\n            if (getApellido || getNombre) {\r\n                let data;\r\n\r\n                data = {\r\n\r\n                    \"data\": {\r\n                        \"type\": \"user\",\r\n                        \"