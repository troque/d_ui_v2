ar</button>\r\n                                    <Link to={`/RamasProceso/`} state={{ from: from }} >\r\n                                        <button type=\"button\" className=\"btn btn-danger\"><i className=\"fas fa-window-close\"></i> Cancelar</button>\r\n                                    </Link>\r\n                                </div>\r\n\r\n\r\n                            </div>\r\n                        </Form>\r\n                    )}\r\n                </Formik>\r\n            </>\r\n        );\r\n    }\r\n\r\n    const enviarDatosEvalucion = (datos) => {\r\n        window.showSpinner(true);\r\n        const data = {\r\n            \"data\": {\r\n                \"type\": 'evaluacion',\r\n                \"attributes\": {\r\n                    \"id_proceso_disciplinario\": procesoDisciplinarioId,\r\n                    \"estado\": getSelectedConcepto,\r\n                    \"resultado_evaluacion\": datos.ReclasiEvaluacion,\r\n                    \"justificacion\": datos.DetalEvaluacion,\r\n                }\r\n            }\r\n        }\r\n        console.log(JSON.stringify(data));\r\n\r\n        GenericApi.addGeneric('evaluacion-crear', data).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    console.log(datos.usuario_remitido);\r\n                    if (datos.usuario_remitido) {\r\n                        /*setExitoApiTextsCustom(\r\n                            {\r\n                                titulo: 'Asignación del proceso disciplinario',\r\n                                cuerpo: 'El proceso disciplinario es asignado a ' + datos.usuario_remitido,\r\n                                boton: 'Aceptar',\r\n                                destino: '/MisPendientes/',\r\n                            }\r\n                        );*/\r\n                        setModalState({ title: \"SINPROC No \"+radicado+\" :: Evaluación del radicado\", message: 'El proceso disciplinario es asignado a ' + datos.usuari