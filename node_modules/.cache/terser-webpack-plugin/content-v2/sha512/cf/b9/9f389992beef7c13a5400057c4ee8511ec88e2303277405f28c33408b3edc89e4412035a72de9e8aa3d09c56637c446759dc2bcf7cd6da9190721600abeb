ito-cierre-etapa\" tabIndex=\"-1\" role=\"dialog\" aria-labelledby=\"modal-block-popout\" aria-hidden=\"true\">\r\n                <div className=\"modal-dialog modal-dialog-popout\" role=\"document\">\r\n                    <div className=\"modal-content\">\r\n                        <div className=\"block block-themed block-transparent mb-0\">\r\n                            <div className=\"block-header bg-primary-dark\">\r\n                                <h3 className=\"block-title\">{props?.success?.titulo}</h3>\r\n                                <div className=\"block-options\">\r\n                                    <button type=\"button\" className=\"btn-block-option\" data-dismiss=\"modal\" aria-label=\"Close\">\r\n                                        <i className=\"fa fa-fw fa-times\"></i>\r\n                                    </button>\r\n                                </div>\r\n                            </div>\r\n                            <div className=\"block-content\">\r\n                                <p>{props?.success?.cuerpo}</p>\r\n                            </div>\r\n                            <div className=\"block-content block-content-full text-right bg-light\">\r\n                                <button type=\"button\" className=\"btn btn-sm btn-primary\" data-dismiss=\"modal\" onClick={() => handleClicAceptar()}>{props?.success?.boton}</button>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </>\r\n    );\r\n}\r\n\r\nexport default InfoExitoCierreEtapa;\r\n","import { ErrorMessage, Field, Form, Formik } from 'formik';\r\nimport React, { useEffect, useState } from 'react';\r\nimport { Navigate } from \"react-router-dom\";\r\nimport Spinner from '../../Utils/Spinner';\r\nimport ParametrosMasApi from '../../Api/Services/ParametrosMasApi';\r\nimport { Link, useLocation } from 'react-router-dom';\r\nimport 'react-datetime/css/react-datetime.css';\r\nimport 'moment/locale/es';\r\nimport '../../Utils/Constants';\r\nimport GenericApi from '../../Api/Services/GenericApi';\r\nimport DataTable from 'react-data-table-component';\r\nimport ModalGen from '../../Utils/Modals/ModalGeneric';\r\nimport InfoExitoCierreEtapa from '../../Utils/InfoExitoCierreEtapa';\r\n\r\nfunction EvaluacionQuejaForm() {\r\n\r\n    const [getListaTipoConducta, setListaTipoConducta] = useState({ data: {} });\r\n    const [countTextArea, setCountTextArea] = useState(0);\r\n    const [getMaximoTextArea, setMaximoTextArea] = useState(0);\r\n    const [getMinimoTextArea, setMinimoTextArea] = useState(0);\r\n    const location = useLocation();\r\n    const [perPage, setPerPage] = useState(process.env.PAGINATION_PER_PAGE);\r\n    const [pageActual, setPageActual] = useState(1);\r\n    const [getSelectedConcepto, setSelectedConcepto] = useState(\"\");\r\n    const [getDatosEvaluacion, setDatosEvaluacion] = useState({ data: [], links: [], meta: [] });\r\n    const [getListaResultadoEvaluacion, setListaResultadoEvaluacion] = useState({ data: {} });\r\n    const [getModalState, setModalState] = useState({ title: \"\", message: \"\", show: false });\r\n\r\n    const { from } = location.state;\r\n    let procesoDisciplinarioId = from.procesoDisciplinarioId;\r\n    let radicado = from.radicado;\r\n\r\n    const columns = [\r\n        {\r\n            name: 'Fecha de Registro',\r\n\r\n            cell: evaluacion => <div>{evaluacion.attributes.created_at}</div>,\r\n            selector: evaluacion => evaluacion.attributes.created_at,\r\n            sortable: true,\r\n            width: \"15%\"\r\n        },\r\n        {\r\n            name: 'Tipo de Conducta',\r\n            cell: evaluacion => (evaluacion.attributes.tipo_conducta ? evaluacion.attributes.tipo_conducta.conducta_nombre : \"\"),\r\n            selector: evaluacion => (evaluacion.attributes.tipo_conducta ? evaluacion.attributes.tipo_conducta.conducta_nombre : \"\"),\