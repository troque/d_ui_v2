_dependencia\": getTextoTransaccion2,\r\n                            \"texto_enviar_a_jefe_de_la_dependencia\": getTextoTransaccion3,\r\n                            \"texto_enviar_a_otra_dependencia\": getTextoTransaccion4,\r\n                            \"texto_regresar_proceso_al_ultimo_usuario\": getTextoTransaccion5,\r\n                            \"impedimento\": getImpedimentoSeleccionado,\r\n                            \"comisorio\": getComisorioSeleccionado,\r\n                            \"excluyente\": getExcluyenteSeleccionado\r\n                        }\r\n                    }\r\n                }\r\n            }\r\n\r\n        } else {\r\n            data = {\r\n                \"data\": {\r\n                    \"type\": \"mas_actuaciones\",\r\n                    \"attributes\": {\r\n                        \"nombre_actuacion\": getNombreActuacion,\r\n                        \"nombre_plantilla\": from.attributes.nombre_plantilla,\r\n                        \"id_etapa\": getEtapaSeleccionada,\r\n                        \"estado\": getEstadoSeleccionada,\r\n                        \"id_etapa_despues_aprobacion\": getEtapaCambiarSeleccionada ? getEtapaCambiarSeleccionada : null,\r\n                        \"despues_aprobacion_listar_actuacion\": getListarActuacionesSeleccionada,\r\n                        \"nombre_plantilla_manual\": from.attributes.nombre_plantilla_manual,\r\n                        \"fileBase64\": null,\r\n                        \"fileBase64_manual\": null,\r\n                        \"texto_dejar_en_mis_pendientes\": getTextoTransaccion1,\r\n                        \"texto_enviar_a_alguien_de_mi_dependencia\": getTextoTransaccion2,\r\n                        \"texto_enviar_a_jefe_de_la_dependencia\": getTextoTransaccion3,\r\n                        \"texto_enviar_a_otra_dependencia\": getTextoTransaccion4,\r\n                        \"texto_regresar_proceso_al_ultimo_usuario\": getTextoTransaccion5,\r\n                        \"impedimento\": getImpedimentoSeleccionado,\r\n                        \"comisorio\": getComisorioSeleccionado,\r\n                        \"excluyente\": getExcluyenteSeleccionado\r\n                    }\r\n                }\r\n            }\r\n        }\r\n\r\n        console.log(data);\r\n        GenericApi.updateGeneric('mas_actuaciones', id, data).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setModalState({ title: \"Actuacin :: Actualizacin con xito\", message: 'La Actuacion: ' + valores.nombreActuacion + ' fue actualizada con xito ', show: true, redirect: '/ActuacionesAdministracion', alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                }\r\n                else {\r\n                    setModalState({ title: \"Actuacin :: Error de actualizacin\", message: datos.error.toString(), show: true, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                }\r\n\r\n            }\r\n        )\r\n    }\r\n\r\n    const obtenemosEtapas = () => {\r\n        GenericApi.getAllGeneric('mas-etapa').then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setListaEtapas(datos);\r\n                    setRespuestaEstapas(true);\r\n                    setRespuestaEstapasCambiar(true);\r\n                    window.showSpinner(false);\r\n                } else {\r\n                    console.log(\"error\");\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const selectEtapa = () => {\r\n\r\n        return (\r\n            getListaEtapas.data.map((etapa, i) => {\r\n                if (etapa.id >= 3) {\r\n                    return (\r\n                        <option key={etapa.id} value={etapa.id}>{etapa.attributes.nombre}</option>\r\n                    )\r\n                }\r\n            })\r\n        )\r\n    }\r\n\r\n    const selectEtapaCambiar = () => {\r\n\r\n        return (\r\n            getListaEtapas.data.map((etapa, i) => {\r\n                if (etapa.id >= 3) {\r\n                    return (\r\n                        <option key={etapa.id} value={etapa.id}>{etapa.attributes.nombre}</option>\r\n                    )\r\n                }\r\n            })\r\n        )\r\n    }\r\n\r\n    const handleInputChangeArchivos = (e, index) => {\r\n\r\n        const { name, files } = e.target;\r\n        if (files.length > 0) {\r\n            const extension = getFileExtension(files[0].name);\r\n\r\n            if (extension == \"docx\") {\r\n\r\n                const list = [...inputListArchivos];\r\n                list[index][name] = files[0];\r\n\r\n                if (files[0]) {\r\n                    list[index][name] = files[0];\r\n                } else {\r\n                    list[index][name] = '';\r\n                    list[index].filebase64 = '';\r\n                    list[index].size = 0;\r\n                    list[index].ext = \"\";\r\n                    setInputListArchivos(list);\r\n                }\r\n\r\n                // Conversion a Base64\r\n                Array.from(e.target.files).forEach(archivo => {\r\n                    var reader = new FileReader();\r\n                    reader.readAsDataURL(archivo);\r\n                    reader.onload = function () {\r\n                        var arrayAuxiliar = [];\r\n                        var base64 = reader.result;\r\n                        arrayAuxiliar = base64.split(',');\r\n                        list[index].filebase64 = arrayAuxiliar[1];\r\n                        list[index].ext = extension;\r\n                        setInputListArchivos(list);\r\n                    }\r\n                })\r\n                obtenerPesoTotalArchivos(list);\r\n            } else {\r\n                const list = [...inputListArchivos];\r\n                list[index][name] = files[0];\r\n                list[index].filebase64 = '';\r\n                list[index].size = 0;\r\n                list[index][name] = '';\r\n                setInputListArchivos(list);\r\n                setErrorApi(\"El archivo seleccionado no tiene un formato permitido. Debe ser *.docx\")\r\n                window.showModal(1)\r\n            }\r\n        } else {\r\n            const list = [...inputListArchivos];\r\n            list[index][name] = '';\r\n            list[index].filebase64 = '';\r\n            list[index].size = 0;\r\n            setInputListArchivos(list);\r\n            obtenerPesoTotalArchivos(list);\r\n        }\r\n\r\n    }\r\n\r\n    const handleInputChangeArchivosManual = (e, index) => {\r\n\r\n        const { name, files } = e.target;\r\n        if (files.length > 0) {\r\n            const extension = getFileExtension(files[0].name);\r\n            if (extension == \"docx\") {\r\n\r\n                const list = [...inputListArchivos];\r\n                list[index][name] = files[0];\r\n\r\n                if (files[0]) {\r\n                    list[index][name] = files[0];\r\n                } else {\r\n                    list[index][name] = '';\r\n                    list[index].filebase64 = '';\r\n                    list[index].size = 0;\r\n                    list[index].ext = \"\";\r\n\r\n                    setInputListArchivosManual(list);\r\n                }\r\n\r\n                // Conversion a Base64\r\n                Array.from(e.target.files).forEach(archivo => {\r\n                    var reader = new FileReader();\r\n                    reader.readAsDataURL(archivo);\r\n                    reader.onload = function () {\r\n                        var arrayAuxiliar = [];\r\n                        var base64 = reader.result;\r\n                        arrayAuxiliar = base64.split(',');\r\n                        list[index].filebase64 = arrayAuxiliar[1];\r\n                        list[index].ext = extension;\r\n                        setInputListArchivosManual(list);\r\n                    }\r\n                })\r\n                obtenerPesoTotalArchivosManual(list);\r\n            } else {\r\n                const list = [...inputListArchivos];\r\n                list[index][name] = files[0];\r\n                list[index].filebase64 = '';\r\n                list[index].size = 0;\r\n                list[index][name] = '';\r\n                setInputListArchivosManual(list);\r\n                setErrorApi(\"El archivo seleccionado no tiene un formato permitido. Debe ser *.docx\")\r\n                window.showModal(1)\r\n            }\r\n        } else {\r\n            const list = [...inputListArchivos];\r\n            list[index][name] = '';\r\n            list[index].filebase64 = '';\r\n            list[index].size = 0;\r\n            setInputListArchivosManual(list);\r\n            obtenerPesoTotalArchivosManual(list);\r\n        }\r\n\r\n    }\r\n\r\n    function getFileExtension(filename) {\r\n        return filename.slice((filename.lastIndexOf(\".\") - 1 >>> 0) + 2);\r\n    }\r\n\r\n    function obtenerPesoTotalArchivos(list) {\r\n        let peso = 0;\r\n        list.forEach(\r\n            (archivo, index) => {\r\n                if (archivo.archivo.size) {\r\n                    peso += archivo.archivo.size;\r\n                }\r\n            }\r\n        )\r\n\r\n        if (peso > 15000000) {\r\n            setErrorApi('El peso/tamao de los todos los adjuntos superan los 15 Mb pertmitidos para el registro, verifique e tamao y elimine algunos adjuntos, (Si desea enviar mas archivos realice otro proceso de registro de Soportes del radicado)')\r\n            window.showModal(1)\r\n        }\r\n\r\n        setPesoTotalArchivos(peso);\r\n    }\r\n\r\n    function obtenerPesoTotalArchivosManual(list) {\r\n        let peso = 0;\r\n        list.forEach(\r\n            (archivo, index) => {\r\n                if (archivo.archivo.size) {\r\n                    peso += archivo.archivo.size;\r\n                }\r\n            }\r\n        )\r\n\r\n        if (peso > 15000000) {\r\n            setErrorApi('El peso/tamao de los todos los adjuntos superan los 15 Mb pertmitidos para el registro, verifique e tamao y elimine algunos adjuntos, (Si desea enviar mas archivos realice otro proceso de registro de Soportes del radicado)')\r\n            window.showModal(1)\r\n        }\r\n\r\n        setPesoTotalArchivosManual(peso);\r\n    }\r\n\r\n    function formatBytes(bytes, decimals = 3) {\r\n        if (bytes === undefined) return '0 Bytes';\r\n        if (bytes === 0) return '0 Bytes';\r\n        const k = 1024;\r\n        const dm = decimals < 0 ? 0 : decimals;\r\n        const sizes = ['Bytes', 'KB', 'MB', 'GB', 'TB', 'PB', 'EB', 'ZB', 'YB'];\r\n        const i = Math.floor(Math.log(bytes) / Math.log(k));\r\n        return parseFloat((bytes / Math.pow(k, i)).toFixed(dm)) + ' ' + sizes[i];\r\n    }\r\n\r\n    const selectChangeMasActuacion = (value) => {\r\n        setMasActuacionId(value);\r\n    }\r\n\r\n    const handleClicArchivo = () => {\r\n        try {\r\n            window.showSpinner(true);\r\n\r\n            const data = {\r\n                \"data\": {\r\n                    \"type\": \"mas_actuaciones\",\r\n                    \"attributes\": {\r\n                        \"nombre_actuacion\": \"\",\r\n                        \"nombre_plantilla\": \"\",\r\n                        \"id_etapa\": \"\",\r\n                        \"estado\": \"\",\r\n                        \"id_etapa_despues_aprobacion\": \"\",\r\n                        \"despues_aprobacion_listar_actuacion\": \"\",\r\n                        \"fileBase64\": \"\"\r\n                    }\r\n                }\r\n            }\r\n\r\n            GenericApi.getByDataGeneric('mas_actuaciones/plantilla/' + from.id, data).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        downloadBase64File(datos.content_type, datos.base_64, datos.file_name);\r\n                    }\r\n                    else {\r\n\r\n                    }\r\n                    window.showSpinner(false);\r\n                }\r\n            )\r\n\r\n        } catch (error) {\r\n            console.error(error);\r\n        }\r\n    };\r\n\r\n    const handleClicArchivoManual = () => {\r\n        try {\r\n            window.showSpinner(true);\r\n\r\n            const data = {\r\n                \"data\": {\r\n                    \"type\": \"mas_actuaciones\",\r\n                    \"attributes\": {\r\n                        \"nombre_actuacion\": \"\",\r\n                        \"nombre_plantilla\": \"\",\r\n                        \"id_etapa\": \"\",\r\n                        \"estado\": \"\",\r\n                        \"id_etapa_despues_aprobacion\": \"\",\r\n                        \"despues_aprobacion_listar_actuacion\": \"\",\r\n                        \"nombre_plantilla_manual\": \"\",\r\n                        \"fileBase64\": \"\",\r\n                        \"fileBase64_manual\": \"\"\r\n                    }\r\n                }\r\n            }\r\n\r\n            GenericApi.getByDataGeneric('mas_actuaciones/manual/' + from.id, data).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        downloadBase64FileManual(datos.content_type, datos.base_64, datos.file_name);\r\n                    }\r\n                    else {\r\n\r\n                    }\r\n                    window.showSpinner(false);\r\n                }\r\n            )\r\n\r\n        } catch (error) {\r\n            console.error(error);\r\n        }\r\n    };\r\n\r\n    function downloadBase64File(contentType, base64Data, fileName) {\r\n        const linkSource = `data:${contentType};base64,${base64Data}`;\r\n        const downloadLink = document.createElement(\"a\");\r\n        downloadLink.href = linkSource;\r\n        downloadLink.download = fileName;\r\n        downloadLink.click();\r\n    }\r\n\r\n    function downloadBase64FileManual(contentType, base64Data, fileName) {\r\n        const linkSource = `data:${contentType};base64,${base64Data}`;\r\n        const downloadLink = document.createElement(\"a\");\r\n        downloadLink.href = linkSource;\r\n        downloadLink.download = fileName;\r\n        downloadLink.click();\r\n    }\r\n\r\n    return (\r\n        <>\r\n            {<Spinner />}\r\n            {<InfoErrorApi error={errorApi} />}\r\n            {<ModalGen data={getModalState} />}\r\n            <Formik\r\n                initialValues={{\r\n                    nombreActuacion: '',\r\n                    plantilla: '',\r\n                    etapa: '',\r\n                    estado: '',\r\n                    cambiarEtapa: '',\r\n                    listarActuaciones: '',\r\n                    archivo: '',\r\n                    dejarEnMisPendientes: '',\r\n                    enviarAAlguienDeMiDependencia: '',\r\n                    enviarAJefeDeLaDependencia: '',\r\n                    enviarAOtraDependencia: '',\r\n                    regresarProcesoAlUltimoUsuario: '',\r\n                    impedimento: '',\r\n                    comisorio: '',\r\n                    excluyente: ''\r\n                }}\r\n                enableReinitialize\r\n                validate={(valores) => {\r\n                    let errores = {}\r\n                    valores.id_actuacion = getMasActuacionId;\r\n                    if (getNombreActuacion == \"\") {\r\n                        errores.nombreActuacion = 'Campo requerido';\r\n                    }\r\n                    let archivo = inputListArchivos[0].filebase64 != \"\" ? inputListArchivos[0].archivo.name : from ? from.attributes.nombre_plantilla : \"\";\r\n                    if (archivo == \"\") {\r\n                        errores.archivo = 'Campo requerido';\r\n                    }\r\n                    if (getEtapaSeleccionada == \"\") {\r\n                        errores.etapa = 'Campo requerido';\r\n                    }\r\n                    if (getEstadoSeleccionada == \"\") {\r\n                        errores.estado = 'Campo requerido';\r\n                    }\r\n                    if (getListarActuacionesSeleccionada == \"\" &&\r\n                        (getImpedimentoSeleccionado != 1 && getComisorioSeleccionado != 1)) {\r\n                        errores.listarActuaciones = 'Campo requerido';\r\n                    }\r\n                    if (getImpedimentoSeleccionado == \"\") {\r\n                        errores.impedimento = 'Campo requerido';\r\n                    }\r\n                    if (getComisorioSeleccionado == \"\") {\r\n                        errores.comisorio = 'Campo requerido';\r\n                    }\r\n                    if (getExcluyenteSeleccionado == \"\") {\r\n                        errores.excluyente = 'Campo requerido';\r\n                    }\r\n\r\n                    //validacion text area 1\r\n                    if (countTextArea1 > getMaximoTextArea) {\r\n                        errores.dejarEnMisPendientes = 'La descripcin debe tener como maximo ' + getMaximoTextArea + ' caracteres';\r\n                    }\r\n                    if (getTextoTransaccion1) {\r\n                        if (containsSpecialChars(getTextoTransaccion1))\r\n                            errores.dejarEnMisPendientes = 'Tiene caracteres invlidos';\r\n                    }\r\n\r\n                    //validacion text area 2\r\n                    if (countTextArea2 > getMaximoTextArea) {\r\n                        errores.enviarAAlguienDeMiDependencia = 'La descripcin debe tener como maximo ' + getMaximoTextArea + ' caracteres';\r\n                    }\r\n                    if (getTextoTransaccion2) {\r\n                        if (containsSpecialChars(getTextoTransaccion2))\r\n                            errores.enviarAAlguienDeMiDependencia = 'Tiene caracteres invlidos';\r\n                    }\r\n\r\n                    //validacion text area 3\r\n                    if (countTextArea3 > getMaximoTextArea) {\r\n                        errores.enviarAJefeDeLaDependencia = 'La descripcin debe tener como maximo ' + getMaximoTextArea + ' caracteres';\r\n                    }\r\n                    if (getTextoTransaccion3) {\r\n                        if (containsSpecialChars(getTextoTransaccion3))\r\n                            errores.enviarAJefeDeLaDependencia = 'Tiene caracteres invlidos';\r\n                    }\r\n\r\n                    //validacion text area 4\r\n                    if (countTextArea4 > getMaximoTextArea) {\r\n                        errores.enviarAOtraDependencia = 'La descripcin debe tener como maximo ' + getMaximoTextArea + ' caracteres';\r\n                    }\r\n                    if (getTextoTransaccion2) {\r\n                        if (containsSpecialChars(getTextoTransaccion4))\r\n                            errores.enviarAOtraDependencia = 'Tiene caracteres invlidos';\r\n                    }\r\n\r\n                    //validacion text area 5\r\n                    if (countTextArea5 > getMaximoTextArea) {\r\n                        errores.regresarProcesoAlUltimoUsuario = 'La descripcin debe tener como maximo ' + getMaximoTextArea + ' caracteres';\r\n                    }\r\n                    if (getTextoTransaccion2) {\r\n                        if (containsSpecialChars(getTextoTransaccion5))\r\n                            errores.regresarProcesoAlUltimoUsuario = 'Tiene caracteres invlidos';\r\n                    }\r\n\r\n                    console.log(\"errores -> \", errores);\r\n\r\n                    return errores\r\n                }}\r\n                onSubmit={(valores, { resetForm }) => {\r\n\r\n                    if (from != null) {\r\n                        actualizarDatos(valores);\r\n                    } else {\r\n                        enviarDatos(valores);\r\n                    }\r\n                }}\r\n            >\r\n\r\n                {({ errors }) => (\r\n                    <Form>\r\n\r\n                        <div className=\"block block-themed\">\r\n                            <div className=\"block-header\">\r\n                                <h3 className=\"block-title\">Datos bsicos maestra actuacin</h3>\r\n                            </div>\r\n                            <div className=\"block-content\">\r\n\r\n                                <div className=\"row\">\r\n\r\n                                    <div className=\"col-md-6\">\r\n                                        <div className=\"form-group\">\r\n                                            <label htmlFor=\"nombreActuacion\">Nombre actuacin <span className=\"text-danger\">*</span></label>\r\n                                            <Field as=\"input\" type=\"text\" className=\"form-control\" id=\"nombreActuacion\" name=\"nombreActuacion\" value={from != null ? getNombreActuacion : null} onChange={changeNombreActuacion}></Field>\r\n                                            <ErrorMessage name=\"nombreActuacion\" component={() => (<span className=\"text-danger\">{errors.nombreActuacion}</span>)} />\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                    {\r\n                                        getMasActuacionId > 0 ?\r\n                                            inputListArchivos.map((x, i) => {\r\n                                                return (\r\n                                                    <div className=\"col-md-6\" key={i}>\r\n                                                        <div className=\"form-group\">\r\n                                                            <div className='row'>\r\n                                                                <div className='col-md-12'>\r\n                                                                    <label>Plantilla (Word)<span className=\"text-danger\">* </span></label>\r\n                                                                </div>\r\n                                                                <div className='col-md-12' style={{ marginLeft: '13px' }}>\r\n                                                                    <div className='row'>\r\n                                                                        <div className='col-md-10'>\r\n                                                                            <label className=\"custom-file-label\" htmlFor=\"example-file-input-custom\" data-toggle=\"custom-file-input\">{x.archivo.name != null ? x.archivo.name : getNombrePlantilla}</label>\r\n                                                                            <input className=\"custom-file-input\" data-toggle=\"custom-file-input\" type=\"file\" name=\"archivo\" onChange={e => handleInputChangeArchivos(e, i)} />\r\n                                                                            <label>Peso del archivo: {formatBytes(x.archivo.size)} </label>\r\n                                                                            <ErrorMessage name=\"archivo\" component={() => (<span className=\"text-danger\">{errors.archivo}</span>)} />\r\n                                                                        </div>\r\n                                                                        {from != null ?\r\n                                                                            <div className='col-2'>\r\n                                                                                <button type='button' title='Descargar documento' className='btn btn-sm btn-primary' onClick={() => handleClicArchivo()}>\r\n                                                                                    <i className='fas fa-download'></i>\r\n                                                                                </button>\r\n                                                                            </div>\r\n                                                                            : \"\"}\r\n\r\n                                                                    </div>\r\n                                                                </div>\r\n                                                            </div>\r\n                                                        </div>\r\n                                                    </div>\r\n                                                );\r\n                                            })\r\n                                            : null\r\n                                    }\r\n\r\n                                    <div className=\"col-md-3\">\r\n                                        <div className=\"form-group\">\r\n                                            <label htmlFor=\"impedimento\">Es impedimento?<span className=\"text-danger\">*</span></label>\r\n                                            <Field as=\"select\" className=\"form-control\" id=\"impedimento\" name=\"impedimento\" value={getImpedimentoSeleccionado} onChange={selectChangeImpedimentos}>\r\n                                                <option value=\"\">Selecciona</option>\r\n                                                <option value=\"1\" selected>Si</option>\r\n                                                <option value=\"0\">No</option>\r\n                                            </Field>\r\n                                            <ErrorMessage name=\"impedimento\" component={() => (<span className=\"text-danger\">{errors.impedimento}</span>)} />\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                    <div className=\"col-md-3\">\r\n                                        <div className=\"form-group\">\r\n                                            <label htmlFor=\"comisorio\">Es auto comisorio? <span className=\"text-danger\">*</span></label>\r\n                                            <Field as=\"select\" className=\"form-control\" id=\"comisorio\" name=\"comisorio\" value={getComisorioSeleccionado} onChange={selectChangeComisorio}>\r\n                                                <option value=\"\">Selecciona</option>\r\n                                                <option value=\"1\" selected>Si</option>\r\n                                                <option value=\"0\">No</option>\r\n                                            </Field>\r\n                                            <ErrorMessage name=\"comisorio\" component={() => (<span className=\"text-danger\">{errors.comisorio}</span>)} />\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                    <div className=\"col-md-6\">\r\n                                        <div className=\"form-group\">\r\n                                            <label htmlFor=\"excluyente\">Es excluyente? Hasta que no se apruebe, no se pueden crear ms actuaciones<span className=\"text-danger\">*</span></label>\r\n                                            <Field as=\"select\" className=\"form-control\" id=\"excluyente\" name=\"excluyente\" value={getExcluyenteSeleccionado} onChange={selectChangeExcluyente}>\r\n                                                <option value=\"\">Selecciona</option>\r\n                                                <option value=\"1\" selected>Si</option>\r\n                                                <option value=\"0\">No</option>\r\n                                            </Field>\r\n                                            <ErrorMessage name=\"excluyente\" component={() => (<span className=\"text-danger\">{errors.excluyente}</span>)} />\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                    <div className=\"col-md-3\">\r\n                                        <div className=\"form-group\">\r\n                                    