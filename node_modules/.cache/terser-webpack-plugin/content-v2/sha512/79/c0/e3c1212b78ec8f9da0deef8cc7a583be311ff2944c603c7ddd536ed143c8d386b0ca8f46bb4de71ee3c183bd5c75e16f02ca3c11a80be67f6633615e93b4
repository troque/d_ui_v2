GenericApi from '../../Api/Services/GenericApi';\r\nimport Spinner from '../../Utils/Spinner';\r\nimport { Navigate } from \"react-router-dom\";\r\nimport DataTable from 'react-data-table-component';\r\nimport ListaBotones from '../../Utils/ListaBotones';\r\nimport '../../Utils/Constants';\r\nimport { useLocation } from 'react-router-dom';\r\n\r\nfunction ActuacionesLista() {\r\n\r\n    const [errorApi, setErrorApi] = useState('');\r\n    const [isRedirect, setIsRedirect] = useState(false);\r\n    const [getActuacionesListaSearch, setActuacionesListaSearch] = useState({ data: [], links: [], meta: [] });\r\n    const [getSeach, setSeach] = useState('');\r\n    const paganationPerPages = global.Constants.DATA_TABLE.PAGINATION_PER_PAGE;\r\n    const [perPage, setPerPage] = useState(global.Constants.DATA_TABLE.PAGINATION_PER_PAGE);\r\n    const [pageActual, setPageActual] = useState(1);\r\n    const [getEstadoLista, setEstadoLista] = useState('');\r\n    const [getNombreEtapaSeleccionada, setNombreEtapaSeleccionada] = useState('');\r\n\r\n    const Documento = [];\r\n\r\n    const [getMostrarBotonAgregar, setMostrarBotonAgregar] = useState(null);\r\n\r\n    const location = useLocation()\r\n    const { from, selected_id_etapa } = location.state\r\n\r\n    let radicado = from.radicado;\r\n    let procesoDisciplinarioId = from.procesoDisciplinarioId;\r\n    let idEtapa = from.idEtapa >= 3 ? from.idEtapa : 3;\r\n    let estado = 1;\r\n\r\n    const [getRoutes, setRoutes] = useState({\r\n        crear_registro: \"/ActuacionesForm/\",\r\n        consultar_registros: \"/ActuacionesLista/\",\r\n        ver_detalle: \"/ActuacionesVer/\" + procesoDisciplinarioId + \"/\" + selected_id_etapa + \"/\" + estado,\r\n        muestra_atras: true,\r\n        id_etapa: global.Constants.ETAPAS.NINGUNA,\r\n        selected_id_etapa: selected_id_etapa,\r\n    });\r\n\r\n    const handleCallback = (childData) => {\r\n        try {\r\n            setEstadoLista(childData == global.Constants.ESTADOS.INACTIVO ? \"Inactivos\" : \"Activos\")\r\n            cargarActuaciones(1, paganationPerPages, childData);\r\n        } catch (error) {\r\n\r\n        }\r\n    }\r\n\r\n    const columns = [\r\n        {\r\n\r\n            name: 'Actuacin',\r\n            cell: actuaciones => actuaciones.attributes.nombre_actuacion,\r\n            selector: actuaciones => actuaciones.attributes.nombre_actuacion,\r\n            sortable: true,\r\n            width: \"20%\"\r\n        },\r\n        {\r\n\r\n            name: 'Informacin registro',\r\n            cell: actuaciones =>\r\n                <div className=\"mt-2 mb-2 d-none d-sm-table-cell\">\r\n                    <div className='row'>\r\n                        <div className=\"col-10\">\r\n                            <small><strong>Usuario:</strong> {actuaciones.attributes.usuario.nombre + \" \" + actuaciones.attributes.usuario.apellido + \" (\" + actuaciones.attributes.id_dependencia.nombre + \")\"}</small><br />\r\n                            <small><strong>Fecha de registro:</strong> {actuaciones.attributes.created_at}</small><br />\r\n                        </div>\r\n                    </div>\r\n                </div>,\r\n            selector: actuaciones => actuaciones.attributes.created_user,\r\n            sortable: true,\r\n            width: \"20%\"\r\n        },\r\n        {\r\n            name: 'Informacin Aprobacin/Rechazo',\r\n            cell: actuaciones =>\r\n                < div className=\"mt-2 mb-2 d-none d-sm-table-cell\" >\r\n                    <div className='row'>\r\n                        <div className=\"col-10\">\r\n                            <small><strong>Usuario:</strong> {actuaciones.attributes.usuario_accion == \"\" ? \"-\" : actuaciones.attributes.usuario_accion}</small><br />\r\n                            <small><strong>Fecha de registro:</strong> {actuaciones.attributes.updated_at}</small><br />\r\n                        </div>\r\n                    </div>\r\n                </div >,\r\n            selector: actuaciones => actuaciones.attributes.usuario_accion,\r\n            sortable: true,\r\n            width: \"24%\"\r\n        },\r\n        {\r\n            name: 'Estado',\r\n            cell: actuaciones => actuaciones.attributes.nombre_estado_actuacion,\r\n            selector: actuaciones => actuaciones.attributes.nombre_estado_actuacion,\r\n            sortable: true,\r\n            width: \"10%\"\r\n        },\r\n        {\r\n            name: 'Auto',\r\n            cell: actuaciones => MostrarBotonDescargaAuto,\r\n            sortable: true,\r\n            width: \"10%\"\r\n        },\r\n        {\r\n            name: 'Detalle',\r\n            cell: actuaciones =>\r\n                <div className='row'>\r\n                    <div className='col-4'>\r\n                        <Link to={getRoutes.ver_detalle} state={{ from: from, selected_id_etapa: selected_id_etapa, id: actuaciones.id, nombre: actuaciones.attributes.nombre_actuacion, estadoActualActuacion: actuaciones.attributes.nombre_estado_actuacion, tipoActuacion: actuaciones.attributes.tipo_actuacion }} >\r\n                            <button type='button' title='Ver actuacin' className='btn btn-sm btn-primary' data-toggle=\"modal\" ><i className=\"fas fa-search\"></i></button>\r\n                        </Link>\r\n                    </div>\r\n                </div >,\r\n            width: \"10%\"\r\n        }\r\n    ];\r\n\r\n\r\n    const handleClicArchivo = (id_documento, extension, nombre_documento) => {\r\n\r\n        try {\r\n            window.showSpinner(true);\r\n\r\n            // Se genera la peticion para descargar el archivo\r\n            GenericApi.getGeneric(\"archivo-actuaciones/get-documento/\" + id_documento + \"/\" + extension).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        downloadBase64File(datos.content_type, datos.base_64, nombre_documento);\r\n                    } else {\r\n                        setErrorApi(datos.error.toString())\r\n                        window.showModal(1)\r\n                    }\r\n                    window.showSpinner(false);\r\n                }\r\n            )\r\n        } catch (error) {\r\n            console.error(error);\r\n        }\r\n    };\r\n\r\n    function downloadBase64File(contentType, base64Data, fileName) {\r\n        const linkSource = `data:${contentType};base64,${base64Data}`;\r\n        const downloadLink = document.createElement(\"a\");\r\n        downloadLink.href = linkSource;\r\n        downloadLink.download = fileName;\r\n        downloadLink.click();\r\n    }\r\n\r\n    const redirectToRoutes = () => {\r\n        return <Navigate to={`/ActuacionesLista/`} state={{ from: from, selected_id_etapa: selected_id_etapa }} />;\r\n    }\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n            window.showSpinner(true);\r\n            setEstadoLista(\"Activos\");\r\n            cargarActuaciones(1, paganationPerPages, global.Constants.ESTADOS.ACTIVO);\r\n            obtenerDatosDeLaEtapaSeleccionada();\r\n            \r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n    const  traerAutos = (actuaciones) => {\r\n        actuaciones.data.forEach(element => {\r\n            if(element.attributes.auto != null){\r\n                GenericApi.getGeneric(\"archivo-actuaciones/get-archivo-actuaciones-by-uuid/\" + element.id).then(\r\n                    datos => {\r\n                        if (!datos.error) {\r\n                            if (datos.data.length > 0) {\r\n                                datos.data.forEach(element2 => {\r\n                                    if(element2.attributes.id_tipo_archivo == 2){\r\n                                        Documento.push({\r\n                                            id:element2.attributes.id, \r\n                                            extension:element2.attributes.extension, \r\n                            