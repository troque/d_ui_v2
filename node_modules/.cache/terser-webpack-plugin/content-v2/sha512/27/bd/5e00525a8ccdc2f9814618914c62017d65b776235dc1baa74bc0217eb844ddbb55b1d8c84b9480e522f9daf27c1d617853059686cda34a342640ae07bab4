                      fechaDependencia: ''\r\n                        }}\r\n                        enableReinitialize\r\n                        validate={(valores) => {\r\n                            setCountTextArea(valores.antecedenteSirius.length)\r\n                            let errores = {}\r\n\r\n \r\n\r\n                            if (!valores.antecedenteSirius) {\r\n                                errores.antecedenteSirius = 'Debe escribir los antecedentes'\r\n                            }\r\n                            else if (valores.antecedenteSirius.length <= getMinimoTextArea) {\r\n                                errores.antecedenteSirius = 'Los antecedentes deben tener almenos ' + getMinimoTextArea + ' caracteres'\r\n                            }\r\n\r\n                            if (!valores.radicadoSirius) {\r\n                                errores.radicadoSirius = 'Debe escribir el radicado de la entidad'\r\n                            }\r\n\r\n                            if (!valores.fechaRadicadoCordis && !fechaRadicadoCordis) {\r\n                                errores.fechaRadicadoCordis = 'Debe ingresar la fecha del radicado'\r\n                            }\r\n\r\n                            if (!valores.fechaDependencia && !fechaDependenciaSirius) {\r\n                                errores.fechaDependencia = 'Debe ingresar la fecha de ingreso de la dependencia'\r\n                            }\r\n\r\n                            return errores\r\n                        }}\r\n                        onSubmit={(valores, { resetForm }) => {\r\n                            enviarDatos(valores)\r\n                        }}\r\n                    >\r\n                        {({ errors }) => (\r\n                            <Form>\r\n                                <div className=\"row\">\r\n                                    <div className=\"col-md-12\">\r\n                                        <div className=\"text-center\">\r\n   