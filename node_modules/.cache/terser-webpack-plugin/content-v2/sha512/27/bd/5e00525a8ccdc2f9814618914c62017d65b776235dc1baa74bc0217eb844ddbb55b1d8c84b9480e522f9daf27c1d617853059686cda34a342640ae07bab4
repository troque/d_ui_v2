',\r\n                            numAutoDesglose: '',\r\n                            dependenciaOrigenDesglose: ''\r\n                        }}\r\n                        enableReinitialize\r\n                        validate={(valores) => {\r\n\r\n                            setCountTextArea(valores.antecedentesDesglose.length)\r\n                            let errores = {}\r\n\r\n\r\n                            if (!valores.antecedentesDesglose) {\r\n                                errores.antecedentesDesglose = 'Debe escribir los antecedentes'\r\n                            }\r\n                            else if (valores.antecedentesDesglose.length <= getMinimoTextArea) {\r\n                                errores.antecedentesDesglose = 'Los antecedentes deben tener almenos ' + getMinimoTextArea + ' caracteres'\r\n                            }\r\n\r\n                            if (!valores.fechaIngresoDesglose && !fechaDependenciaDesglose) {\r\n\r\n                                errores.fechaIngresoDesglose = 'Debe ingresar la fecha de ingreso'\r\n                            }\r\n\r\n                            if (!valores.vigenciaAutoDesglose && !getFechaVigenciaAutoDesglose) {\r\n                                errores.vigenciaAutoDesglose = 'Debe ingresar la vigencia de auto'\r\n                            }\r\n\r\n                            if (!valores.fechaAutoDesglose && !fechaAutoaDesglose) {\r\n                                errores.fechaAutoDesglose = 'Debe ingresar la fecha de auto'\r\n                            }\r\n\r\n\r\n                            if (!valores.numAutoDesglose) {\r\n                                errores.numAutoDesglose = 'Debe ingresar el numero de auto'\r\n                            }\r\n                            else if (valores.numAutoDesglose.length < 4) {\r\n                                errores.numAutoDesglose = 'Debe ingresar almenos 4 caracteres del numero de auto'\r\n                            }\r\n     