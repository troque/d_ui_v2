\r\n        }\r\n        console.log(JSON.stringify(data));\r\n        GenericApi.addGeneric('evaluacion', data).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    window.showSpinner(false);\r\n                    if (datos.usuario_remitido) {\r\n                        setExitoApiTextsCustom(\r\n                            {\r\n                                titulo: 'Asignación del proceso disciplinario',\r\n                                cuerpo: 'El proceso disciplinario es asignado a ' + datos.usuario_remitido,\r\n                                boton: 'Aceptar',\r\n                                destino: '/MisPendientes/',\r\n                                from: from\r\n                            }\r\n                        );\r\n                        window.showModal(7);\r\n                        //setIsRedirect(true);\r\n                    }\r\n                    else {\r\n                        window.showModal(1);\r\n                        window.showSpinner(false);\r\n                    }\r\n\r\n\r\n                }\r\n                else {\r\n                    setErrorApi(datos.error.toString());\r\n                    window.showModal(1);\r\n                    window.showSpinner(false);\r\n                }\r\n\r\n            }\r\n        )\r\n\r\n\r\n\r\n    }\r\n\r\n    const componentEvaluacionForm = () => {\r\n        return (\r\n            <>\r\n\r\n\r\n                {<InfoErrorApi error={errorApi} />}\r\n                {<InfoExitoCierreEtapa success={getExitoApiTextsCustom} />}\r\n                {<Spinner />}\r\n                <Formik\r\n                    initialValues={{\r\n                        ResulEvaluacion: '',\r\n                        NoticiaPrio: '',\r\n                        tipoConducta: '',\r\n                        Justificacion: ''\r\n                    }}\r\n                    validate={(valores) => {\r\n                        let errores = {};\r\n          