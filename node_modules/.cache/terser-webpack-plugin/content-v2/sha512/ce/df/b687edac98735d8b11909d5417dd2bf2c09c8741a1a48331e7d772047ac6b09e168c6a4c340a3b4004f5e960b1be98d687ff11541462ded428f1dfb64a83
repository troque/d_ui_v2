    getValorVigencia != \"\" ||\r\n        getValorEstadoDelExpediente != \"\" ||\r\n        getValorUbicacionDelExpediente != \"\" ||\r\n        getValorNombreDisciplinado != \"\" ||\r\n        getValorIdentificacionDisciplinado != \"\" ||\r\n        getValorAsuntoDelExpediente != \"\" ||\r\n        getValorSector != \"\" ||\r\n        getValorNombreEntidad != \"\" ||\r\n        getValorNombreQuejoso != \"\" ||\r\n        getValorIdentificacionQuejoso != \"\" ||\r\n        getValorTipoQuejoso != \"\" ||\r\n        getValorEtapaDelExpediente != \"\" ||\r\n        getValorDelegada != \"\" \r\n        ){\r\n            let procesoDisciplinarioId = from != \"\" ? from.procesoDisciplinarioId : \"\" ;\r\n            let data = {\r\n                \"data\":{\r\n                    \"type\":\"buscador\",\r\n                    \"attributes\": {\r\n                        \"procesoDisciplinarioId\": procesoDisciplinarioId,\r\n                        \"n_expediente\": getValorExpediente,\r\n                        \"Vigencia\": getValorVigencia,\r\n                        \"estado_del_expediente\": getValorEstadoDelExpediente,\r\n                        \"ubicacion_del_expediente\": getValorUbicacionDelExpediente,\r\n                        \"nombre_disciplinado\": getValorNombreDisciplinado,\r\n                        \"identificacion_disciplinado\": getValorIdentificacionDisciplinado,\r\n                        \"asunto_del_expediente\": getValorAsuntoDelExpediente,\r\n                        \"sector\": getValorSector,\r\n                        \"nombre_entidad\": getValorNombreEntidad,\r\n                        \"nombre_quejoso\": getValorNombreQuejoso,\r\n                        \"identificacion_quejoso\": getValorIdentificacionQuejoso,\r\n                        \"tipo_quejoso\": getValorTipoQuejoso,\r\n                        \"etapa_del_expediente\": getValorEtapaDelExpediente,\r\n                        \"delegada\": getValorDelegada\r\n                    }\r\n                }\r\n            }\r\n    \r\n            GenericApi.getByDataGeneric('buscador-general', data).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        if(datos.data.length > 0){\r\n                            setListaPendientes(datos);\r\n                            window.showSpinner(false);\r\n                            setModalState({ title: \"Buscador de expediente :: Alerta\", message: \"Busqueda exitosa se encontraron \"+datos.data.length+\" existencias\", show: true, redirect: null, alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                        }else{\r\n                            window.showSpinner(false);\r\n                            setModalState({ title: \"Buscador de expediente :: Alerta\", message: \"No se han encontrado coincidencias\", show: true, redirect: null, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                        }\r\n                        \r\n                        \r\n                    }\r\n                    else {\r\n                        window.showSpinner(false);\r\n                        // console.log(\"Error en la consulta\");\r\n                    }\r\n                }\r\n            )\r\n\r\n        }else{\r\n            setModalState({ title: \"Buscador de expediente :: Alerta\", message: \"No se envio ningun dato\", show: true, redirect: null, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n        }   \r\n        \r\n\r\n        \r\n        \r\n    }\r\n\r\n    const handlePageChange = page => {\r\n        setPageActual(page);\r\n    }\r\n\r\n    const handlePerRowsChange = async (newPerPage, page) => {\r\n        setPerPage(newPerPage);\r\n        setPageActual(page);\r\n    }\r\n\r\n    let columns = [\r\n        {\r\n            name: 'Dependencia',\r\n            selector: pendiente => pendiente.attributes.Dependencia,\r\n            sortable: true,\r\n            width: \"10%\"\r\n        },\r\n        {\r\n            n