roll', 'display': 'block' }}>\r\n                                                    <table className=\"table table-bordered table-striped table-vcenter js-dataTable-full\">\r\n                                                        <thead>\r\n                                                            <tr>\r\n                                                                <th>Seleccionar</th>\r\n                                                                <th>Solo aparecer en</th>\r\n                                                            </tr>\r\n                                                        </thead>\r\n                                                        <tbody >\r\n\r\n                                                            {getListaAccesosAsociados ? listarAccessoAsociados() : null}\r\n\r\n                                                        </tbody>\r\n                                                    </table>\r\n                                                </div>\r\n                                            </div>\r\n\r\n\r\n\r\n\r\n                                        </div>\r\n                                        <div className=\"block-content block-content-full text-right\">\r\n                                            <button type=\"submit\" className=\"btn btn-primary\">\r\n                                                <i className=\"fa fa-fw fa-save\"></i> Actualizar\r\n                                            </button>\r\n                                        </div>\r\n\r\n                                    </div>\r\n\r\n\r\n                                </div>\r\n\r\n\r\n                            </div>\r\n                        </Form>\r\n                    )}\r\n                </Formik>\r\n            </>\r\n        )\r\n    }\r\n\r\n    return (\r\n\r\n        <>\r\n            {isRedirect ? redirectToRoutes() : null}\r\n            {<InfoErrorApi error={errorApi} />}\r\n            {<InfoExi