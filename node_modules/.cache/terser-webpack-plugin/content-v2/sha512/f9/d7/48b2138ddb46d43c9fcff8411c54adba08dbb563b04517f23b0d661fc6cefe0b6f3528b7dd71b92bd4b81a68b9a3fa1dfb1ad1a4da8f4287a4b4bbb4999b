            }\r\n                else {\r\n                    setErrorApi(datos.error.toString())\r\n                    window.showModal()\r\n                }\r\n            }\r\n        )\r\n\r\n    }\r\n\r\n    //Funciones\r\n    const handleClicSoporteRadicado = (id_etapa, id_fase, es_soporte) => {\r\n        from.id_etapa = id_etapa;\r\n        from.id_fase = id_fase;\r\n        changeValueEsFuncion(es_soporte);\r\n    };\r\n\r\n    function changeValueEsFuncion(es_funcion) {\r\n        from.es_soporte = es_funcion;\r\n    }\r\n\r\n    const cerrarEtapa = (datos, etapa) => {\r\n\r\n        let data\r\n\r\n        data = {\r\n            \"data\": {\r\n                \"type\": \"cerrar_etapa\",\r\n                \"attributes\": {\r\n                    \"id_proceso_disciplinario\": procesoDisciplinarioId,\r\n                    \"id_etapa\": datos.inputEtapa,\r\n                    \"descripcion\": \"Cierre de etapa\",\r\n                    \"created_user\": getUser().nombre,\r\n                }\r\n            }\r\n        }\r\n        CierreEtapaApi.addCierreEtapa(data, etapa).then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n\r\n                    setExitoApiTextsCustom(\r\n                        {\r\n                            titulo: 'Asignación del proceso disciplinario',\r\n                            cuerpo: 'El proceso disciplinario es asignado a ' + datos.data.attributes.funcionario_asignado.nombre +' '+datos.data.attributes.funcionario_asignado.apellido,\r\n                            boton: 'Aceptar',\r\n                            destino: 'MisPendientes'\r\n                        }\r\n                    );\r\n\r\n                    setEvaluacionRepartoCerrada(true);\r\n                    window.showModal(7);\r\n\r\n                }\r\n                else {\r\n                    setErrorApi(datos.error.toString())\r\n\r\n                    window.showModal(1)\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const componentAntecedente = () => {\r\n\r\n\r\n        return (\r\n            <>\r\n\r\n                <div className=\"col-lg-12 push\">\r\n                    <div className=\"alert alert-info\" role=\"alert\">\r\n                        <h3 className=\"text-center\">(RADICADO) :: {radicado}</h3>\r\n                        <span><strong>Antecedente: </strong></span><br />\r\n                        <span title={(from.antecedente) ? from.antecedente : \"\"}>{from.antecedente ? from.antecedente.substring(0, 300) : \"\"}...</span><br />\r\n\r\n                        <br />\r\n                        <div className=\"row\">\r\n                            <div className=\"col-lg-6\"><span><strong>Registrado Por: </strong>{from.registradoPor ? from.registradoPor : \"Sin Información\"}</span></div>\r\n                            <hr />\r\n                            <div className=\"col-lg-6\"><span><strong>Dependencia registro: </strong> {from.dependendencia ? from.dependendencia : \"Sin Información\"}</span></div>\r\n                            <hr />\r\n                        </div>\r\n\r\n                        <div className=\"row\">\r\n\r\n                            <div className=\"col-lg-4\"><span><strong>Vigencia: </strong>{vigencia}</span></div>\r\n                            <div className=\"col-lg-4\"><span><strong>Fecha de registro: </strong> {moment(from.fechaRegistro, 'DD/MM/YYYY, h:mm:ss a').format(\"DD/MM/YYYY, h:mm:ss a\")}</span></div>\r\n                            <div className=\"col-lg-4\"><span><strong>Fecha de ingreso: </strong> {moment(from.fechaIngreso, 'YYYY/MM/DD').format(\"DD/MM/YYYY\")}</span></div>\r\n\r\n\r\n                        </div>\r\n\r\n                    </div>\r\n                </div>\r\n            </>\r\n        )\r\n    }\r\n\r\n    return (\r\n        <>\r\n            {<Spinner />}\r\n            {isRedirect ? redirectToRoutes() : null}\r\n            {<InfoErrorApi error={getErrorApi} />}\