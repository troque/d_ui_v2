   fetchData();\r\n    }, []);\r\n\r\n    const validarEvaluacion = (attributes) => {\r\n\r\n        // Etapa: Evaluación Fase: validar clasificación\r\n\r\n        if (attributes[\"validar_clasificacion\"] === true) {\r\n            console.log(\"VALIDAR CLASIFICACION\");\r\n            setColorValidarClasificacion(getBotonColorSuccess);\r\n            setHabilitaEvaluacion(true);\r\n        } else {\r\n            setHabilitaEvaluacion(false);\r\n        }\r\n\r\n        // Etapa: Evaluación Fase: Evaluacion\r\n        if (attributes[\"validar_clasificacion\"] === true) {\r\n            if (attributes[\"cantidad_evaluacion\"].length == 0) {\r\n\r\n                setFasesPermitidas(oldArray => [...oldArray, global.Constants.FASES.EVALUACION]);\r\n            }\r\n\r\n            else if (attributes[\"cantidad_evaluacion\"].length == 1) {\r\n                setColorEvaluacion(getBotonColorWarning);\r\n            }\r\n            else if (attributes[\"cantidad_evaluacion\"].length > 1) {\r\n                //setHabilitaRemisionQueja(false);\r\n\r\n                setColorEvaluacion(getBotonColorSuccess);\r\n                setHabilitaRemisionQueja(true);\r\n\r\n            }\r\n        }\r\n\r\n\r\n        //SI TIENE UN REGISTRO en clasificacion de derecho de peticion que solo muestra unos botones, damos prioridad primero a este listado\r\n        //par activar o inactivar las fases de evaluacion\r\n        if (attributes[\"clasificacion_radicado_datos\"][0][\"attributes\"][\"fases_permitidas\"]) {\r\n\r\n            attributes[\"clasificacion_radicado_datos\"][0][\"attributes\"][\"fases_permitidas\"].split(',').map((rol, i) => {\r\n                setFasesPermitidas(oldArray => [...oldArray, rol]);\r\n            });\r\n        }\r\n        //verificamos a cuales fases tiene acceso el resultado\r\n        else if (attributes[\"cantidad_evaluacion\"]) {\r\n            if (attributes[\"cantidad_evaluacion\"][0][\"attributes\"][\"fases_permitidas\"]) {\r\n\r\n                attributes[\"cantidad_evaluacion\"][0][\"attributes\"][\"fases_permitidas\"].split(',').map((rol, i) => {\r\n                    setFasesPermitidas(oldArray => [...oldArray, rol]);\r\n                });\r\n\r\n            }\r\n        }\r\n\r\n\r\n\r\n\r\n\r\n        // Etapa: Evaluación Fase: remision queja\r\n        if (attributes[\"muestra_remision_queja\"] === true && attributes[\"muestra_evaluacion\"] === true) {\r\n            setColorRemisionQueja(getBotonColorSuccess);\r\n            setHabilitaGestorRespuesta(true);\r\n        } else {\r\n            //setHabilitaGestorRespuesta(false);\r\n        }\r\n        // Etapa: Evaluación Fase: gestor respuesta\r\n        if (attributes[\"muestra_gestor_respuesta\"] === true && attributes[\"muestra_remision_queja\"] === true) {\r\n            setColorGestorRespuesta(getBotonColorSuccess);\r\n            if (attributes[\"muestra_comunicacion_interesado\"] === true) {\r\n                setHabilitadaComunicacionInteresado(true);\r\n            }\r\n        } else {\r\n            //setHabilitadaComunicacionInteresado(false);\r\n        }\r\n\r\n        //si lo muestra revisamos si debe cambiar el color dependiendo si ya hay registros\r\n        if (attributes[\"comunicacion_interesado\"] === true && attributes[\"muestra_gestor_respuesta\"] === true) {\r\n            setColorComunicacionInteresado(getBotonColorSuccess);\r\n            setHabilitaDocumentoCierre(true);\r\n        }\r\n        else {\r\n            //setHabilitaDocumentoCierre(false);\r\n        }\r\n\r\n\r\n        if (attributes[\"muestra_documento_cierre\"] === true && attributes[\"comunicacion_interesado\"]) {\r\n            setColorDocumentoCierre(getBotonColorSuccess);\r\n            setEtapaEvaluacionCompleta(true);\r\n        }\r\n\r\n\r\n\r\n\r\n\r\n    }\r\n\r\n\r\n    const consultaCierreEtapa = (id_etapa) => {\r\n\r\n        /*const data = {\r\n            \"data\": {\r\n                \"type\": \"log_disciplinario\",\r\n        