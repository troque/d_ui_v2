ta: {} });\r\n    const [respuestaTiposQueja, setRespuestaTiposQueja] = useState(false);\r\n\r\n    const [listaTerminosRespuesta, setListaTerminosRespuesta] = useState({ data: {} });\r\n    const [respuestaTerminosRespuesta, setRespuestaTerminosRespuesta] = useState(false);\r\n    \r\n    const [getTiposExpedientes, setTiposExpedientes] = useState({ data: {} });\r\n    const [getReparto, setReparto] = useState('');\r\n    const [getIdDependenciaSecretariaComun, setIdDependenciaSecretariaComun] = useState('');\r\n    const [listaGruposTrabajo, setListaGruposTrabajo] = useState({ data: {} });\r\n    const [getGrupoTrabajoSeleccionado, setGrupoTrabajoSeleccionado] = useState('');\r\n    const [getIdGrupoTrabajo, setIdGrupoTrabajo] = useState('');\r\n    const [respuestaGrupoTrabajo, setRespuestaGrupoTrabajo] = useState(false);\r\n    const [getListaGruposSeleccionadas, setListaGruposSeleccionadas] = useState([]);\r\n    const [getErrorGrupos, setErrorGrupos] = useState(\"\");\r\n\r\n    const [getModalState, setModalState] = useState({ title: \"\", message: \"\", show: false });\r\n    const [getValidarSecretariaComun, setValidarSecretariaComun] = useState(false);\r\n\r\n    // Metodo encargado de cargar la configuracion de la clase\r\n    useEffect(() => {\r\n\r\n        // Se usa el cargando\r\n        window.showSpinner(true);\r\n\r\n        // Se llama la funcion del metodo\r\n        async function fetchData() {\r\n\r\n            // Se cargan las dependencias origen\r\n            getAllDependenciaOrigen();\r\n        }\r\n\r\n        // Se quita el cargando\r\n        window.showSpinner(false);\r\n\r\n        // Se llama la funcion asyncrona\r\n        fetchData();\r\n    }, []);\r\n\r\n    // Metodo encargdo de cargar las dependencias registradas\r\n    const getAllDependenciaOrigen = () => {\r\n\r\n        // Se consume la api para traer las depedencias a estado registrar\r\n        GenericApi.getByIdGeneric('mas-dependencia-filtrado', global.Constants.ACCESO_DEPENDENCIA.CREAR_USUARIO).then(\r\n\r\n            // Se incializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se setean los datos\r\n                    setListaDependenciaOrigen(datos);\r\n\r\n                    // Se redeclara la variable en true\r\n                    setDependenciaOrigen(true);\r\n\r\n                    // Se llama el metodo para cargar los roles\r\n                    getllRoles();\r\n\r\n                }\r\n                else {\r\n\r\n\r\n                }\r\n            }\r\n\r\n\r\n        )\r\n    }\r\n\r\n    // Metodo encagrado de cargar los roles\r\n    const getllRoles = () => {\r\n\r\n        // Se consume la api para traer los roles\r\n        GenericApi.getAllGeneric('role').then(\r\n\r\n            // Se incializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se setean los datos\r\n                    setListaRoles(datos);\r\n\r\n                    // Se redeclara la variable en true\r\n                    setRespuestaRoles(true);\r\n\r\n                    // Se llama el metodo para cargar los tipos de expediente\r\n                    cargarTiposExpedientes();\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encagrado de cargar los tipos de expediente\r\n    const cargarTiposExpedientes = () => {\r\n\r\n        // Se consume la api para traer los roles\r\n        GenericApi.getAllGeneric('mas-tipo-expediente').then(\r\n\r\n            // Se incializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se setean los datos\r\n                    setTiposExpedientes(datos);\r\n\r\n                    // Se redeclara la variable en true\r\n                    setRespuestaTipoExpediente(true);\r\n\r\n                    // Se llama el metodo para cargar los tipos de derecho de peticion\r\n                    cargarTiposDerechoPeticion();\r\n                } else {\r\n\r\n                    // Se lanza el mensaje de error\r\n                    setModalState({ title: \"Cargar Tipos Expedientes :: Usuarios\", message: datos.error.toString(), show: true, redirect: '/Usuario/Add', alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encagrado de cargar los tipos de derecho peticion\r\n    const cargarTiposDerechoPeticion = () => {\r\n\r\n        // Se consume la api para traer los tipos de derecho peticion\r\n        GenericApi.getAllGeneric('mas-tipo-derecho-peticion').then(\r\n\r\n            // Se incializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se setean los datos\r\n                    setListaDerechoPeticion(datos);\r\n\r\n                    // Se redeclara la variable en true\r\n                    setRespuestaDerechoPeticion(true);\r\n\r\n                    // Se llama el metodo para cargar los tipos de queja\r\n                    cargarTiposQueja()\r\n                } else {\r\n\r\n                    // Se lanza el mensaje de error\r\n                    setModalState({ title: \"Cargar Tipos Derecho Petición :: Usuarios\", message: datos.error.toString(), show: true, redirect: '/Usuario/Add', alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encagrado de cargar los tipos de queja\r\n    const cargarTiposQueja = () => {\r\n\r\n        // Se consume la api para traer los tipos de queja\r\n        GenericApi.getAllGeneric('mas-tipo-queja').then(\r\n\r\n            // Se incializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se setean los datos\r\n                    setListaTiposQueja(datos);\r\n\r\n                    // Se redeclara la variable en true\r\n                    setRespuestaTiposQueja(true);\r\n\r\n                    // Se llama el metodo para cargar los tipos de procesos disciplinarios\r\n                    cargarTiposProcesoDisciplinario();\r\n                } else {\r\n\r\n                    // Se lanza el mensaje de error\r\n                    setModalState({ title: \"Tipos Procesos Disciplinarios :: Usuarios\", message: datos.error.toString(), show: true, redirect: '/Usuario/Add', alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encagrado de cargar los tipos de proceso disciplinario -- PENDIENTE VALIDAR mas-tipo-queja\r\n    const cargarTiposProcesoDisciplinario = () => {\r\n\r\n        // Se consume la API\r\n        GenericApi.getAllGeneric('mas-tipo-proceso').then(\r\n\r\n            // Se incializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se setean los datos\r\n                    setListaProcesoDisciplinario(datos);\r\n\r\n                    // Se redeclara la variable en true\r\n                    setRespuestaProcesoDisciplinario(true);\r\n\r\n                    // Se llama el metodo para cargar los tipos de respuesta\r\n                    cargarTerminoRespuesta();\r\n                } else {\r\n\r\n                    // Se lanza el mensaje de error\r\n                    setModalState({ title: \"Tipos Procesos Disciplinarios :: Usuarios\", message: datos.error.toString(), show: true, redirect: '/Usuario/Add', alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encargado de cargar los terminos de respuesta\r\n    const cargarTerminoRespuesta = () => {\r\n\r\n        // Se consume la API\r\n        GenericApi.getAllGeneric('mas-termino-respuesta').then(\r\n\r\n            // Se incializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se setean los datos\r\n                    setListaTerminosRespuesta(datos);\r\n\r\n                    // Se redeclara la variable en true\r\n                    setRespuestaTerminosRespuesta(true);\r\n\r\n                    // Se llama el metodo para cargar el id de secretaria comun\r\n                    cargarIdDependenciaSecretariaComun();\r\n                } else {\r\n\r\n                    // Se lanza el mensaje de error\r\n                    setModalState({ title: \" Administración :: Usuarios\", message: datos.error.toString(), show: true, redirect: '/Usuario/Add', alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encargado de cargar el id de secretaria comun\r\n    const cargarIdDependenciaSecretariaComun = () => {\r\n\r\n        // Se inicializa la variable\r\n        const data = {\r\n            \"data\": {\r\n                \"type\": 'mas_parametro',\r\n                \"attributes\": {\r\n                    \"nombre\": \"id_dependencia_secretaria_comun\"\r\n                }\r\n            }\r\n        }\r\n\r\n        // Se consume la API\r\n        GenericApi.getByDataGeneric(\"parametro/parametro-nombre\", data).then(\r\n\r\n            // Se incializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se setea el valor del id\r\n                    setIdDependenciaSecretariaComun(datos.data[0].attributes.valor);\r\n\r\n                    // Se llama el metodo para cargar los grupos de trabajo\r\n                    cargarGruposDeTrabajo();\r\n                } else {\r\n\r\n                    // Se lanza el mensaje de error\r\n                    setModalState({ title: \" Administración :: Usuarios\", message: datos.error.toString(), show: true, redirect: '/Usuario/Add', alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encargado de cargar los grupos de trabajo\r\n    const cargarGruposDeTrabajo = () => {\r\n\r\n        // Se consume la API\r\n        GenericApi.getGeneric(\"mas_grupo_trabajo\").then(\r\n\r\n            // Se incializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se setea los datos\r\n                    setListaGruposTrabajo(datos);\r\n\r\n                    // Se setea la respuesta en true\r\n                    setRespuestaGrupoTrabajo(true);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encargado de cargar los grupos de trabajo\r\n    const selectGrupoTrabajo = () => {\r\n\r\n        // Se retorna\r\n        return (\r\n\r\n            // Se recorre el array\r\n            listaGruposTrabajo.data.map((grupoTrabajo, i) => {\r\n\r\n                // Se retorna el option por posicion\r\n                return (\r\n                    <option key={grupoTrabajo.id} value={grupoTrabajo.id}>{grupoTrabajo.attributes.nombre}</option>\r\n                )\r\n            })\r\n        )\r\n    }\r\n\r\n    // Metodo encargado de setear el valor el cambiar\r\n    const handleInputChange = (event) => {\r\n\r\n        // Se capturan el input y valor correspondiente\r\n        const target = event.target;\r\n        const value = target.type === 'checkbox' ? target.checked : target.value;\r\n        const name = target.name;\r\n\r\n        // Se valida cuando es nombre\r\n        if (name == \"nombre\") {\r\n\r\n            // Se setea el valor\r\n            setNombre(value);\r\n        }\r\n\r\n        // Se valida cuando es apellido\r\n        if (name == \"apellido\") {\r\n\r\n            // Se setea el valor\r\n            setApeliido(value);\r\n        }\r\n\r\n        // Se valida cuando es correo\r\n        if (name == \"correo\") {\r\n\r\n            // Se setea el valor\r\n            setEmail(value);\r\n        }\r\n\r\n        // Se valida cuando es dependencia\r\n        if (name == \"dependencia\") {\r\n\r\n            // Se setea el valor\r\n            setIdDependencia(value);\r\n\r\n            // Se llama el metodo encargado de validar si la dependencia tiene accceso a Secretaria Comun\r\n            validarSecretariaComun(value);\r\n        }\r\n\r\n        // Se valida cuando es estado\r\n        if (name == \"estado\") {\r\n\r\n            // Se setea el valor\r\n            setEstado(value);\r\n        }\r\n\r\n        // Se valida cuando es user\r\n        if (name == \"user\") {\r\n\r\n            // Se setea el valor\r\n            setUser(value);\r\n        }\r\n\r\n        // Se valida cuando es reparto\r\n        if (name == \"reparto\") {\r\n\r\n            // Se setea el valor\r\n            setReparto(value);\r\n        }\r\n\r\n        // Se valida cuando es grupoTrabajo\r\n        if (name == \"grupoTrabajo\") {\r\n\r\n            // Se setea el valor\r\n            setIdGrupoTrabajo(value);\r\n        }\r\n    }\r\n\r\n    // Metodo encargado de cambiar el valor en los grupos de trabajo\r\n    const selectChangeGrupo = (v) => {\r\n\r\n        // Se valida que es diferente de null\r\n        if (v != null) {\r\n\r\n            // Se setea el valor seleccionado\r\n            setListaGruposSeleccionadas(v);\r\n        } else {\r\n\r\n            // Se setea el mensaje de error de los grupos\r\n            setErrorGrupos('Campo requerido');\r\n        }\r\n    }\r\n\r\n    // Metodo encargado de validar si la dependencia tiene activo el permiso de secretaria comun\r\n    const validarSecretariaComun = (idDependencia) => {\r\n\r\n        // Se usa el cargando\r\n        window.showSpinner(true);\r\n\r\n        // Se consume la API\r\n        GenericApi.getGeneric(\"mas-dependencia-configuracion/\" + idDependencia).then(\r\n\r\n            // Se inicializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se redeclara la variable en true\r\n                    setValidarSecretariaComun(true);\r\n                } else {\r\n\r\n                    // Se redeclara la variable en true\r\n                    setValidarSecretariaComun(false);\r\n                }\r\n\r\n                // Se quita el cargando\r\n                window.showSpinner(false);\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encargado de cargar las dependencias de origen\r\n    const selectDependenciaOrigen = () => {\r\n\r\n        // Se retorna\r\n        return (\r\n\r\n            // Se recorre el array\r\n            listaDependenciaOrigen.data.map((dependencia, i) => {\r\n\r\n                // Se retorna el select por posicion\r\n                return (\r\n                    <option key={dependencia.id} value={dependencia.id}>{dependencia.attributes.nombre}</option>\r\n                )\r\n            })\r\n        )\r\n    }\r\n\r\n    // Metodo encargado de mostrar el modal del directorio\r\n    const mostrarModalDirectorio = () => {\r\n\r\n        // Se llama el modal\r\n        window.showModalCoincidenciasUsuario();\r\n    }\r\n\r\n    // Metodo encargado de retorna la buscqueda del modal\r\n    const handleCallback = (childData) => {\r\n\r\n        // Se inicializa el trycatch\r\n        try {\r\n\r\n            // Se valida que la data sea diferente de null\r\n            if (childData != null) {\r\n\r\n                // Se setean los valores\r\n                setNombre(childData.nombre);\r\n                setApeliido(childData.apellido);\r\n                setEmail(childData.email);\r\n                setUser(childData.name);\r\n            }\r\n        } catch (error) {\r\n        }\r\n    }\r\n\r\n    // Metodo encargado de enviar los datos al backend\r\n    const enviarDatos = () => {\r\n\r\n        // Se usa el cargando\r\n        window.showSpinner(true);\r\n\r\n        // Se inicializa la data\r\n        let data;\r\n\r\n        // Se redeclara la variable con la informacion a enviar\r\n        data = {\r\n\r\n            \"data\": {\r\n                \"type\": \"usuario\",\r\n                \"attributes\": {\r\n                    \"nombre\": getNombre ? getNombre : \"\",\r\n                    \"apellido\": getApellido ? getApellido : \"\",\r\n                    \"name\": getUser ? getUser : \"\",\r\n                    \"email\": getEmail ? getEmail : \"\",\r\n                    \"id_dependencia\": getIdDependencia ? getIdDependencia : \"\",\r\n                    \"id_mas_grupo_trabajo_secretaria_comun\": getListaGruposSeleccionadas ? getListaGruposSeleccionadas : \"\",\r\n                    \"roles\": rolesAsociados,\r\n                    \"expedientes\": tipoExpAsociados,\r\n                    \"estado\": getEstado ? getEstado : \"\",\r\n                    \"reparto_habilitado\": getReparto ? getReparto : \"\",\r\n                }\r\n            }\r\n        }\r\n\r\n        // Se consume la API\r\n        GenericApi.addGeneric('usuario', data).then(\r\n\r\n            // Se inicializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se setea el valor en true\r\n                    setIsRedirect(true);\r\n\r\n                    // Se muestra el mensaje\r\n                    setModalState({ title: \"Administración :: Usuarios\", message: 'registro exitoso', show: true, redirect: '/Usuario', alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                } else {\r\n\r\n                    // Se muestra el mensaje de error\r\n                    setModalState({ title: \"Administración :: Usuarios\", message: datos.error.toString(), show: true, redirect: '/Usuario/Add', alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                }\r\n\r\n                // Se quita el cargando\r\n                window.showSpinner(false);\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encargado de agregar el check\r\n    const agregarCheck = (e, rolId) => {\r\n\r\n        // Se valida que sea true el check\r\n        if (e == true) {\r\n\r\n            // Se añade al array el valor\r\n            setRolesAsociados(oldArray => [...oldArray, rolId]);\r\n        } else {\r\n\r\n            // Se busca el indice con el id\r\n            var index = rolesAsociados.indexOf(rolId);\r\n\r\n            // Se valida que se encuentre en el array\r\n            if (index !== -1) {\r\n\r\n                // Se quita del array de roles\r\n                rolesAsociados.splice(index, 1);\r\n            }\r\n        }\r\n    }\r\n\r\n    // Metodo encargado de agregar el check de tipos de expediente\r\n    const agregarCheckTipoExp = (e, extId, subExpid) => {\r\n\r\n        // Se valida que sea true el check\r\n        if (e == true) {\r\n\r\n            // Se añade al array el valor\r\n            setTipoExpAsociados(oldArray => [...oldArray, (extId + \"|\" + subExpid)]);\r\n        } else {\r\n\r\n            // Se busca el indice con el id\r\n            var index = tipoExpAsociados.indexOf((extId + \"|\" + subExpid));\r\n\r\n            // Se valida que se encuentre en el array\r\n            if (index !== -1) {\r\n\r\n                // Se quita del array de roles\r\n                tipoExpAsociados.splice(index, 1);\r\n            }\r\n        }\r\n    }\r\n\r\n    // Metodo encargado de listar los roles\r\n    const listarRoles = () => {\r\n\r\n        // Se retorna\r\n        return (\r\n\r\n            // Se recorre el array\r\n            getListaRoles.data.map((rol, i) => {\r\n\r\n                // Se retorna cada columna por posicion\r\n                return (\r\n                    <tr key={(rol.id + 1)}>\r\n                        <td>\r\n                            <div className=\"custom-control custom-switch custom-control-lg mb-2\">\r\n                                <input type=\"checkbox\" onChange={e => agregarCheck(e.target.checked, rol.id)} className=\"custom-control-input\" id={rol.id} name={rol.id} />\r\n                                <label className=\"custom-control-label\" htmlFor={rol.id}></label>\r\n                            </div>\r\n                        </td>\r\n                        <td>\r\n                            {rol.attributes.nombre}\r\n                        </td>\r\n                    </tr>\r\n                )\r\n            })\r\n        )\r\n    }\r\n\r\n    // Metodo encargado de listar los tippos de expediente asociados\r\n    const listarTiposExpedientessociados = () => {\r\n\r\n        // Se retorna\r\n        return (\r\n\r\n            // Se carga el html\r\n            <div className='row'>\r\n                <div className=\"col-md-12\">\r\n                    {\r\n                        // Se recorre el array\r\n                        getTiposExpedientes.data.map((tipoExp, i) => {\r\n\r\n                            // Se retorna el html ppr posicion\r\n                            return (\r\n                                <div key={tipoExp.id} id=\"accordion\" role=\"tablist\" aria-multiselectable=\"true\">\r\n                                    <div class=\"block block-rounded mb-1\">\r\n                                        <div class=\"block-header block-header-default\" role=\"tab\" id=\"accordion_h1\">\r\n                                            <a class=\"font-w600\" data-toggle=\"collapse\" data-parent=\"#accordion\" href=\"#accordion_q1\" aria-expanded=\"true\" aria-controls=\"accordion_q1\"><i class=\"fas fa-folder\" /> {tipoExp.attributes.nombre} </a>\r\n                                        </div>\r\n                                        <div id=\"accordion_q1\" class=\"show\" role=\"tabpanel\" aria-labelledby=\"accordion_h1\" data-parent=\"#accordion\">\r\n                                            <div className=\"block-content\">\r\n                                                {/*DERECHO DE PETICION*/}\r\n                                                {tipoExp.id === '1' && respuestaTipoDerechoPeticion ? componenteTipoExpedienteDerecho() : ''}\r\n                                                {/*PODER REFERENTE A SOLIICTUD*/}\r\n                                                {tipoExp.id === '2' && respuestaTiposQueja ? componenteTipoExpedienteReferente() : ''}\r\n                                                {/*QUEJA*/}\r\n                                                {tipoExp.id === '3' && respuestaTiposQueja ? componenteTipoExpedienteQueja() : ''}\r\n                                                {/*TUTELA*/}\r\n                                                {tipoExp.id === '4' && respuestaTerminosRespuesta ? componenteTipoExpedienteTutela() : ''}\r\n                                                {/*PROCESO DISCIPLINARIO*/}\r\n                                                {tipoExp.id === '5' && respuestaProcesoDisciplinario ? componenteTipoExpedienteProcesoDisciplinario() : ''}\r\n                                            </div>\r\n                                        </div>\r\n                                    </div>\r\n                                </div >\r\n                            )\r\n                        })\r\n                    }\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n\r\n    // Tipo expediente - Derecho Peticion\r\n    const compone