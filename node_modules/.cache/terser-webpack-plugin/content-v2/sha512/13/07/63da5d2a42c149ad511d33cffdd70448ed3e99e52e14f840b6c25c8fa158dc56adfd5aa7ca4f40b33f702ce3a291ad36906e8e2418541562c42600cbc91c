date={(valores) => {\r\n\r\n                    let errors = {}\r\n\r\n                    if(!getNombreSemaforo){\r\n                        errors.nombreSemaforo = \"Debe ingresar un nombre\"\r\n                    }\r\n\r\n                    if(!getEventoInicio){\r\n                        errors.eventoInicio = \"Debe seleccionar un evento de inicio\"\r\n                    }\r\n\r\n                    if(!getEstadoSeleccionada){\r\n                        errors.estado = \"Debe seleccionar un estado\"\r\n                    }\r\n                    if(getEventoInicio == 3 || getEventoInicio == 6){\r\n                        if(!getNombreCampoFecha){\r\n                            errors.nombreCampoFecha = \"Debe ingresar el nombre del campo\"\r\n                        }\r\n                    }\r\n\r\n                    if(getEventoInicio == 2 || getEventoInicio == 3){\r\n                        if(!getActuacionIniciaSemaforo){\r\n                            errors.actuacionIniciaSemaforo = \"Debe seleccionar la actuación\"\r\n                        }\r\n                    }else if(getEventoInicio == 4){\r\n                        if(!getGrupoIniciaSemaforo){\r\n                            errors.grupoIniciaSemaforo = \"Debe seleccionar el grupo\"\r\n                        }\r\n                    }else if(getEventoInicio == 5){\r\n                    }\r\n\r\n                    return errors;\r\n                }}\r\n                onSubmit={(valores, { resetForm }) => {\r\n\r\n                    if (from != null) {\r\n                        actualizarDatos(valores);\r\n                    } else {\r\n                        enviarDatos(valores);\r\n                    }\r\n\r\n                }}\r\n            >\r\n\r\n                {({ errors }) => (\r\n                    <Form>\r\n                        <div className=\"block block-rounded block-bordered\">\r\n                            <div className=\"block block-themed\">\r\n               