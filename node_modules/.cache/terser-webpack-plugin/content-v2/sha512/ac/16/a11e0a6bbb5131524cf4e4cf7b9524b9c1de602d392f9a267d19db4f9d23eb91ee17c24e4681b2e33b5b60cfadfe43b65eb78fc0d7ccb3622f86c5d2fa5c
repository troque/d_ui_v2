   const [getIdTipoProceso, setIdTipoProceso] = useState(false);\r\n    const [getOrigenRadicado, setOrigenRadicado] = useState(false);\r\n\r\n    const [getexistenActuacionesEnPD, setexistenActuacionesEnPD] = useState(false);\r\n    const [getexistenActuacionesEnPreliminar, setexistenActuacionesEnPreliminar] = useState(false);\r\n    const [getexistenActuacionesEnInvestigacionDiscplinaria, setexistenActuacionesEnInvestigacionDiscplinaria] = useState(false);\r\n    const [getexistenActuacionesEnCausaJuzgamiento, setexistenActuacionesEnCausaJuzgamiento] = useState(false);\r\n    const [getexistenActuacionesEnProcesoVerbal, setexistenActuacionesEnProcesoVerbal] = useState(false);\r\n    const [getexistenActuacionesEnSegundaInstancia, setexistenActuacionesEnSegundaInstancia] = useState(false);\r\n\r\n\r\n    const [getActivarTipoConducta, setActivarTipoConducta] = useState(false);\r\n    const [getActivarReTipoExpediente, setActivarReTipoExpediente] = useState(false);\r\n\r\n    const location = useLocation();\r\n    const { from, disable } = location.state;\r\n    let radicado = from.radicado;\r\n    let procesoDisciplinarioId = from.procesoDisciplinarioId;\r\n    let vigencia = from.vigencia;\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n\r\n            window.showSpinner(true);\r\n\r\n            // VALIDA SI ES JEFE\r\n            jefeDependencia();\r\n            listaFasesHabilitadas();\r\n            etapaActual();\r\n            existenActuacionesEn();\r\n            validarEstadoTipoConducta();\r\n            validarEstadoReTipoExpediente()\r\n\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n    /**\r\n     * Se consulta si el usuario que se encuentra en sesin es jefe de la dependencia\r\n     */\r\n    const jefeDependencia = () => {\r\n        const data = {\r\n            \"data\": {\r\n                \"type\": \"usuario\",\r\n                \"attributes\": {\r\n                    \"\": \"\"\r\n                }\r\n            }\r\n        }\r\n\r\n        GenericApi.getByDataGeneric(\"usuario/get-jefe-dependencia\", data).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setRtaJefe(true)\r\n                }\r\n                else {\r\n                    setRtaJefe(false);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encargado de generar la caratula desde las ramas del proceso\r\n    const generarCaratula = () => {\r\n\r\n        // Se inicializa el cargadno\r\n        window.showSpinner(true);\r\n\r\n        // Se inicializa la api\r\n        GenericApi.getGeneric(\"caratulas/caratula-ramas-proceso/\" + procesoDisciplinarioId).then(\r\n\r\n            // Se traen los datos\r\n            datos => {\r\n\r\n                // Se desactiva el cargando\r\n                window.showSpinner(false);\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se manda al metodo encargado de convertir el base64 en un archivo descargable\r\n                    downloadBase64File(datos.content_type, datos.base_64, datos.file_name);\r\n                } else {\r\n                    setModalState({ title: \"Generacin de cartula\", message: datos.error.toString(), show: true, redirect: '/MisPendientes', alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    function downloadBase64File(contentType, base64Data, fileName) {\r\n        const linkSource = `data:${contentType};base64,${base64Data}`;\r\n        const downloadLink = document.createElement(\"a\");\r\n        downloadLink.href = linkSource;\r\n        downloadLink.download = fileName;\r\n        downloadLink.click();\r\n    }\r\n\r\n    const etapaActual = () => {\r\n        GenericApi.getGeneric(\"proceso-diciplinario/\" + procesoDisciplinarioId).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    from.idEtapa = datos.data.attributes.id_etapa;\r\n                    setEtapa(datos.data.attributes.id_etapa);\r\n                    cargarActuaciones(datos.data.attributes.id_etapa);\r\n                    funcionarioAsignado(datos.data.attributes.id_etapa);\r\n                    if (datos.data.attributes.usuario_comisionado != null) {\r\n\r\n                        GenericApi.getGeneric(\"mas-dependencia-origen/\" + datos.data.attributes.usuario_comisionado.id_dependencia).then(\r\n                            datos => {\r\n                                if (!datos.error) {\r\n                                    setDependenciaComisionado(datos.data.attributes.nombre);\r\n                                }\r\n                            }\r\n                        )\r\n                        setUsuarioComisionado(datos.data.attributes.usuario_comisionado.nombre + \" \" + datos.data.attributes.usuario_comisionado.apellido);\r\n                    }\r\n                    setTipoProceso(datos.data.attributes.nombre_tipo_proceso);\r\n                    setIdTipoProceso(datos.data.attributes.id_tipo_proceso);\r\n                    if (datos.data.attributes.id_tipo_proceso == global.Constants.TIPO_INGRESO.SIRIUS) {\r\n                        setOrigenRadicado(datos.data.attributes.nombre_origen_radicado);\r\n                    }\r\n\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const listaFasesHabilitadas = () => {\r\n\r\n        GenericApi.getGeneric(\"proceso-diciplinario/get-fases-registradas/\" + procesoDisciplinarioId).then(\r\n\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    setListaFasesHabilitadas(datos);\r\n                    setFase(true);\r\n                    setRespuestaAntecedente(true);\r\n                }\r\n                else {\r\n\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const funcionarioAsignado = (etapa) => {\r\n\r\n\r\n        GenericApi.getByIdGeneric(\"proceso-diciplinario\", procesoDisciplinarioId).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setIdEtapaAnterior(datos.data.attributes.id_etapa - 1);\r\n                    if(datos.data.attributes.dependencia_duena != null){\r\n                        GenericApi.getByIdGeneric(\"usuario\", datos.data.attributes.dependencia_duena.id_usuario_jefe).then(\r\n                            datos => {\r\n                                if (!datos.error) {\r\n                                    setFuncionarioAsignado(datos.data.attributes.email);\r\n                                }\r\n                            }\r\n                        )\r\n                    }\r\n                    \r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n\r\n    const validarEstadoTipoConducta = () => {\r\n\r\n        GenericApi.getGeneric(\"activar-reclasificacion-tipo-conducta/\" + procesoDisciplinarioId).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setActivarTipoConducta(datos.data.attributes.activar_funcionalidad);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n\r\n    const validarEstadoReTipoExpediente = () => {\r\n\r\n        GenericApi.getGeneric(\"activar-reclasificacion-tipo-expediente/\" + procesoDisciplinarioId).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setActivarReTipoExpediente(datos.data.attributes.activar_funcionalidad);\r\n                }\r\n            }\r\n        )\r\n\r\n        \r\n    }\r\n\r\n\r\n\r\n    //Funciones\r\n    const handleClicSoporteRadicado = (id_etapa, id_fase, es_soporte) => {\r\n        from.id_etapa = id_etapa;\r\n        from.id_fase = id_fase;\r\n        changeValueEsFuncion(es_soporte);\r\n    };\r\n\r\n    function changeValueEsFuncion(es_funcion) {\r\n        from.es_soporte = es_funcion;\r\n    }\r\n\r\n    const cerrarEtapa = (datos) => {\r\n\r\n        window.showSpinner(true);\r\n\r\n        let data\r\n\r\n        data = {\r\n            \"data\": {\r\n                \"type\": \"cerrar_etapa\",\r\n                \"attributes\": {\r\n                    \"id_proceso_disciplinario\": procesoDisciplinarioId,\r\n                    \"id_etapa\": datos.inputEtapa,\r\n                    \"descripcion\": \"Cierre de etapa\",\r\n                    \"created_user\": getUser().nombre,\r\n                }\r\n            }\r\n        }\r\n\r\n\r\n        GenericApi.addGeneric('cierre-etapa', data).then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    setModalState({ title: \"Cierre de etapa\", message: \"El proceso disciplinario se asigna a: \" + datos.data.attributes.funcionario_asignado.nombre + \" \" + datos.data.attributes.funcionario_asignado.apellido, show: true, redirect: '/MisPendientes', from: { from }, alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                }\r\n                else {\r\n                    setModalState({ title: \"Cierre de etapa\", message: datos.error.toString(), show: true, redirect: '/RamasProceso', from: { from }, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n\r\n                }\r\n                window.showSpinner(false);\r\n            }\r\n        )\r\n    }\r\n\r\n    const componentAntecedente = () => {\r\n        return (\r\n            <>\r\n\r\n                <div className=\"col-lg-12 push\">\r\n                    <div className=\"alert alert-info\" role=\"alert\">\r\n                        <h4 className=\"txt-blue-primary\">RADICADO: {radicado}</h4>\r\n                        \r\n                        <span className=\"txt-blue-primary\"><strong>Antecedente: </strong></span><br />\r\n                        \r\n                        <div className=''>\r\n                        <span className='txt-black-primary' title={(from.antecedente) ? from.antecedente : \"\"}>{from.antecedente ? from.antecedente.substring(0, 300) : \"\"}...</span>\r\n                        </div> \r\n\r\n                        <br />\r\n                        <div className=\"row\">\r\n                            <div className=\"col-lg-4\"><span className=\"txt-blue-primary\"><strong >Registrado Por: </strong> <span className='txt-black-primary'>{from.registradoPor ? from.registradoPor : \"Sin Informacin\"}</span></span></div>\r\n                            <hr />\r\n                            <div className=\"col-lg-4\"><span className=\"txt-blue-primary\"><strong>Dependencia registro: </strong> <span className='txt-black-primary'>{from.dependendencia ? from.dependendencia : \"Sin Informacin\"}</span></span></div>\r\n                            <hr />\r\n                            <div className=\"col-lg-4\">\r\n                                {getUsuarioComisionado != \"\" ? (\r\n                                    <span><strong>Usuario comisionado: </strong> {getUsuarioComisionado + \" (\" + getDependenciaComisionado + \")\"} </span>\r\n                                ) : null}\r\n                            </div>\r\n                            <hr />\r\n                        </div>\r\n\r\n                        <div className=\"row\">\r\n                            <div className=\"col-lg-4\"><span><strong className=\"txt-blue-primary\">Vigencia: </strong>{vigencia}</span></div>\r\n                            <div className=\"col-lg-4\"><span><strong className=\"txt-blue-primary\">Fecha de registro: </strong> {moment(from.fechaRegistro, 'DD/MM/YYYY, h:mm:ss a').format(\"DD/MM/YYYY, h:mm:ss a\")}</span></div>\r\n                            <div className=\"col-lg-4\"><span><strong className=\"txt-blue-primary\">Fecha de ingreso: </strong> {moment(from.fechaIngreso, 'YYYY/MM/DD').format(\"DD/MM/YYYY\")}</span></div>\r\n                        </div>\r\n\r\n                        <div className=\"row\">\r\n                            <div className=\"col-lg-4\"><span><strong className=\"txt-blue-primary\">Tipo de Proceso: </strong></span> <span className='txt-black-primary'>{getTipoProceso}</span></div>\r\n                            {getIdTipoProceso == 1 ?\r\n                                <div className=\"col-lg-4\"><span><strong className=\"txt-blue-primary\">Origen del radicado: </strong> </span><span className='txt-black-primary'>{getOrigenRadicado}</span></div> : null\r\n                            }\r\n                        </div>\r\n\r\n                    </div>\r\n                </div>\r\n            </>\r\n        )\r\n    }\r\n\r\n    const cargarFase = (id_etapa, id_fase, es_soporte, redirect) => {\r\n        for (var i = 0; i < getListaFasesHabilitadas.data.length; i++) {\r\n\r\n            if (getListaFasesHabilitadas.data[i].id == id_fase\r\n                && (getListaFasesHabilitadas.data[i].attributes.visible\r\n                    || (id_etapa >= 3) //Validacin para fases de Evaluacin en PD en adelante\r\n                )) {\r\n\r\n                if (getListaFasesHabilitadas.data[i].attributes.estado) {\r\n                    return (\r\n                        <>\r\n                            <div className='w2d-form-group'>\r\n                                <Link to={redirect} state={{ from: from, selected_id_etapa: id_etapa, disable: disable }}>\r\n                                    <button type=\"button\" className=\"btn btn-sm btn-secundary w2d_btn-large mr-1 mb-3 text-left\" onClick={() => changeValueEsFuncion(false)}>\r\n                                        <i className={colorFase(getListaFasesHabilitadas.data[i].attributes.semaforizacion)}></i> {getListaFasesHabilitadas.data[i].attributes.nombre}</button>\r\n                                </Link>\r\n\r\n                                {getListaFasesHabilitadas.data[i].attributes.etapa == global.Constants.ETAPAS.CAPTURA_REPARTO && disable != true ? (\r\n                                    <Link to={`/SoporteradicadoForm/`} state={{ from: from, disable: disable }}>\r\n                                        <button type=\"button\" className=\"btn btn-sm btn-secundary mr-1 mb-3\" onClick={() => handleClicSoporteRadicado(id_etapa, id_fase, es_soporte)}><i class=\"far fa-folder-open\"></i></button>\r\n                                    </Link>\r\n                                ) : null\r\n                                }\r\n                            </div>\r\n                        </>\r\n                    )\r\n                }\r\n\r\n                else {\r\n                    return (\r\n                        <>\r\n                            <div className='w2d-form-group'>\r\n                                <Link to={redirect} state={{ from: from, selected_id_etapa: id_etapa, disable: disable }}>\r\n                                    <button type=\"button\" className=\"btn btn-sm btn-secundary w2d_btn-large mr-1 mb-3 text-left\" onClick={() => changeValueEsFuncion(false)}>\r\n                                        <i className={colorFase(getListaFasesHabilitadas.data[i].attributes.semaforizacion)}></i> {getListaFasesHabilitadas.data[i].attributes.nombre}</button>\r\n                                </Link>\r\n\r\n                                {getListaFasesHabilitadas.data[i].attributes.etapa == global.Constants.ETAPAS.CAPTURA_REPARTO && disable != true ? (\r\n                                    <Link to={`/SoporteradicadoForm/`} state={{ from: from, disable: disable }}>\r\n                                        <button type=\"button\" className=\"btn btn-sm btn-secundary mr-1 mb-3\" onClick={() => handleClicSoporteRadicado(id_etapa, id_fase, es_soporte)}>\r\n                                            <i className=\"far fa-folder-open\"></i></button>\r\n                                    </Link>\r\n                                ) : null\r\n                                }\r\n\r\n                            </div>\r\n                        </>\r\n                    )\r\n                }\r\n            }\r\n\r\n        }\r\n\r\n    }\r\n\r\n    const colorFase = (estado_fase) => {\r\n\r\n        if (estado_fase === 1)\r\n            return (global.Constants.SEMAFORIZACION_FASES.RED)\r\n        else if (estado_fase === 2)\r\n            return (global.Constants.SEMAFORIZACION_FASES.ORANGE)\r\n        else\r\n            return (global.Constants.SEMAFORIZACION_FASES.GREEN)\r\n    }\r\n\r\n    const cargarSoportesRadicado = (id_etapa, id_fase, es_soporte, redirect) => {\r\n\r\n        for (var i = 0; i < getListaFasesHabilitadas.data.length; i++) {\r\n\r\n            if (getListaFasesHabilitadas.data[i].id == id_fase && getListaFasesHabilitadas.data[i].attributes.estado) {\r\n                return (\r\n                    <div className='w2d-form-group'>\r\n                        <Link to={redirect} state={{ from: from, disable: disable }}>\r\n                            <button type=\"button\" className=\"btn btn-sm btn-secundary w2d_btn-large mr-1 mb-3 text-left\" onClick={() => handleClicSoporteRadicado(id_etapa, id_fase, es_soporte)}>\r\n                                <i className={colorFase(getListaFasesHabilitadas.data[i].attributes.semaforizacion)}></i> {getListaFasesHabilitadas.data[i].attributes.nombre}</button>\r\n                        </Link>\r\n                    </div>\r\n                )\r\n            }\r\n            else if (getListaFasesHabilitadas.data[i].id == id_fase && !getListaFasesHabilitadas.data[i].attributes.estado) {\r\n                return (\r\n                    <div className='w2d-form-group'>\r\n                        <Link to={redirect} state={{ from: from, disable: disable }}>\r\n                            <button type=\"button\" className=\"btn btn-sm btn-secundary w2d_btn-large mr-1 mb-3 text-left\" onClick={() => handleClicSoporteRadicado(id_etapa, id_fase, es_soporte)}>\r\n                                <i className={colorFase(getListaFasesHabilitadas.data[i].attributes.semaforizacion)}></i> {getListaFasesHabilitadas.data[i].attributes.nombre}</button>\r\n                        </Link>\r\n                    </div>\r\n                )\r\n            }\r\n        }\r\n    }\r\n\r\n    const cargarCierreEtapa = (id_etapa, id_fase_lista_para_cierre, id_fase_cerrada) => {\r\n\r\n        for (var i = 0; i < getListaFasesHabilitadas.data.length; i++) {\r\n\r\n            // valida cierre de captura y reparto\r\n            if (getListaFasesHabilitadas.data[i].id == id_fase_lista_para_cierre) {\r\n\r\n                console.log(\"ID_ETAPA: \" + id_etapa + \" ID_FASE_LISTA_PARA_CIERRE: \" + id_fase_lista_para_cierre + \" ID_FASE_CERRADA: \" + id_fase_cerrada)\r\n                return (\r\n                    <div className=\"form-group\">\r\n\r\n                        <Formik\r\n                            initialValues={{\r\n                                inputCierreEtapa: '',\r\n                                inputEtapa: id_etapa,\r\n                            }}\r\n                            enableReinitialize\r\n                            validate={(valores) => {\r\n\r\n                                let errores = {}\r\n                                if (!valores.inputCierreEtapa) {\r\n                                    errores.inputCierreEtapa = 'Debe seleccionar la opcin cierre de etapa'\r\n                                }\r\n                                return errores\r\n                            }}\r\n\r\n                            onSubmit={(valores, { resetForm }) => {\r\n                                cerrarEtapa(valores);\r\n                            }}\r\n                        >\r\n\r\n                            <Form>\r\n                                <div className=\"col-md-12\">\r\n                                    <Field type=\"checkbox\" className=\"form-check-input\" id=\"inputCierreEtapa\" name=\"inputCierreEtapa\" />\r\n                                    <label className=\"form-check-label\" htmlFor=\"example-checkbox-inline1\">Con la informacin actual se puede dar cierre a esta etapa?</label>\r\n                                </div>\r\n\r\n                                <div className=\"col-md-12\">\r\n                                    <>\r\n                                        {\r\n                                            (hasAccess('CR_CierreEtapa', 'Crear')) ? (\r\n                                                <div className=\"block-content block-content-full text-center\">\r\n                                                    <button type=\"submit\" className=\"btn btn-rounded btn-outline-primary\"> Cerrar Etapa</button>\r\n                                                </div>\r\n                                            ) : null\r\n                                        }\r\n                                    </>\r\n                                </div>\r\n                            </Form>\r\n                        </Formik>\r\n                    </div>\r\n                )\r\n            }\r\n\r\n            // valida cierre de captura y reparto\r\n            if (getListaFasesHabilitadas.data[i].id == id_fase_cerrada && getListaFasesHabilitadas.data[i].attributes.estado) {\r\n\r\n                return (\r\n                    <div className=\"col-md-12 text-center\">\r\n                        <button type='button' title='Consultar cierre de etapa' className='btn btn-sm'\r\n                            data-toggle=\"modal\" data-target={'#modal-consultar-detalle-' + id_etapa}> <i className=\"fas fa-clipboard-list fa-2x txt-blue-secundary\"></i></button>\r\n                    </div>\r\n                )\r\n            }\r\n        }\r\n    }\r\n\r\n    const cargarActuaciones = (etapa) => {\r\n        GenericApi.getGeneric('actuaciones/get-actuaciones-discipl-etapa/' + procesoDisciplinarioId + '/' + etapa + '/1').then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    if (datos.data.length > 0) {\r\n                        datos.data.forEach(element => {\r\n                            if (element.attributes.created_user == getUser().nombre) {\r\n                                setExistenActuacionesMias(true);\r\n                            }\r\n                        });\r\n                        setExistenActuaciones(true);\r\n                    } else {\r\n                        setExistenActuaciones(false);\r\n                    }\r\n                } else {\r\n                    window.showModal(1)\r\n                }\r\n                window.showSpinner(false);\r\n            }\r\n            \r\n        )\r\n    }\r\n\r\n    const existenActuacionesEn = () => {\r\n\r\n        GenericApi.getGeneric('actuaciones/get-actuaciones-discipl-etapa/' + procesoDisciplinarioId + '/3/1').then(\r\n            datos => {\r\n                if (!datos.error) {\r\n\r\n                    if (datos.data.length > 0) {\r\n                        setexistenActuacionesEnPD(true);\r\n                    } else {\r\n                        setexistenActuacionesEnPD(false);\r\n                    }\r\n                }\r\n            }\r\n        )\r\n\r\n        GenericApi.getGeneric('actuaciones/get-actuaciones-discipl-etapa/' + procesoDisciplinarioId + '/4/1').then(\r\n            datos => {\r\n                if (!datos.error) {\r\n\r\n                    if (datos.data.length > 0) {\r\n                        setexistenActuacionesEnPreliminar(true);\r\n                    } else {\r\n                        setexistenActuacionesEnPreliminar(false);\r\n                    }\r\n                }\r\n            }\r\n        )\r\n\r\n        GenericApi.getGeneric('actuaciones/get-actuaciones-discipl-etapa/' + procesoDisciplinarioId + '/5/1').then(\r\n            datos => {\r\n                if (!datos.error) {\r\n\r\n                    if (datos.data.length > 0) {\r\n                        setexistenActuacionesEnInvestigacionDiscplinaria(true);\r\n                 