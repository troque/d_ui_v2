teresado(getBotonColorSuccess);\r\n                            //setEtapaEvaluacionRepartoCompleta(true);\r\n                            from.cambiaColorDatosInteresado = getBotonColorSuccess;\r\n                        } else {\r\n                            evaluacionRepartoCompleta = false;\r\n                        }\r\n\r\n                        // Etapa: Captura y reparto Fase: Clasificación del radicado\r\n                        if (datos[\"data\"][\"attributes\"][\"clasificacion_radicado\"] === true) {\r\n                            setColorClasificacionRadicado(getBotonColorSuccess);\r\n                            //setEtapaEvaluacionRepartoCompleta(true);\r\n                            from.cambiaColorClasificacionRadicado = getBotonColorSuccess;\r\n                        } else {\r\n                            evaluacionRepartoCompleta = false;\r\n                        }\r\n\r\n                        // Etapa: Captura y reparto Fase: Entidad del investigado\r\n\r\n                        if (datos[\"data\"][\"attributes\"][\"entidad_investigado\"] === true) {\r\n\r\n                            setColorEntidadInvestigado(getBotonColorSuccess);\r\n                            //setEtapaEvaluacionRepartoCompleta(true);\r\n                            from.cambiaColorEntidadInvestigado = getBotonColorSuccess;\r\n                        } else {\r\n                            evaluacionRepartoCompleta = false;\r\n                        }\r\n\r\n                        // Etapa: Captura y reparto Fase: Soporte radicado\r\n                        if (datos[\"data\"][\"attributes\"][\"soporte_radicado\"] === true) {\r\n                            setColorSoporteRadicado(getBotonColorSuccess);\r\n                            //setEtapaEvaluacionRepartoCompleta(true);\r\n                            from.cambiaColorSoporteRadicado = getBotonColorSuccess;\r\n                        } else {\r\n                            evaluacionRepartoCompleta = false;\r\n                        }\r\n\r\n                        setEtapaEvaluacionRepartoCompleta(evaluacionRepartoCompleta);\r\n\r\n                        if (datos[\"data\"][\"attributes\"][\"etapa_cerrada_captura_reparto\"] === true) {\r\n                            setEvaluacionRepartoCerrada(true);\r\n                            from.evaluacionRepartoCerrada = true;\r\n                        }\r\n                        //from.evaluacionRepartoCerrada = true;\r\n                        //setEvaluacionRepartoCerrada(true);\r\n\r\n                        //}\r\n\r\n\r\n                        if (datos[\"data\"][\"attributes\"][\"etapa\"] == 2) {\r\n\r\n                            validarEvaluacion(datos[\"data\"][\"attributes\"]);\r\n\r\n                        }\r\n\r\n                        consultaCierreEtapa(1);\r\n\r\n                        setRespuestaAntecedente(true);\r\n                        from.radicado = radicado;\r\n                        from.idEtapa = datos[\"data\"][\"attributes\"][\"etapa\"];\r\n                        window.showSpinner(false);\r\n\r\n\r\n\r\n                    }\r\n\r\n\r\n                }\r\n\r\n            )\r\n\r\n\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n    const validarEvaluacion = (attributes) => {\r\n\r\n        // Etapa: Evaluación Fase: validar clasificación\r\n\r\n        if (attributes[\"validar_clasificacion\"] === true) {\r\n            setColorValidarClasificacion(getBotonColorSuccess);\r\n            setHabilitaEvaluacion(true);\r\n        } else {\r\n            //setHabilitaEvaluacion(false);\r\n        }\r\n\r\n        // Etapa: Evaluación Fase: Evaluacion\r\n\r\n        if (attributes[\"muestra_evaluacion\"] === true && attributes[\"validar_clasificacion\"] === true) {\r\n            setColorEvaluacion(getBotonColorSuccess);\r\n            setHabilitaRemisionQueja(true);\r\n        } else {\r\n            //setHabilitaRemisionQueja(false);\r\n        }\r\n\r\n        // Etapa: Evaluación Fa