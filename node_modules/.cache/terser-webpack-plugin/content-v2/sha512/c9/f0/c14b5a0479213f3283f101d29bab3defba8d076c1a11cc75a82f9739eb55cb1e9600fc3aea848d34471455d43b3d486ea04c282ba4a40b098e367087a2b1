eneric('log-proceso-disciplinario/get-log-cierre-etapa/', data).then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n\r\n                    console.log(JSON.stringify(datos));\r\n                    setInfoCierreEtapa_CR(datos);\r\n                    setRtaCierreEtapa_CR(true);\r\n                }\r\n\r\n                else {\r\n\r\n                    setErrorApi(datos.error.toString())\r\n                    window.showModal()\r\n                }\r\n            }\r\n        );\r\n\r\n        window.showSpinner(true);\r\n\r\n    }\r\n\r\n\r\n    const listaCierreEtapa = () => {\r\n\r\n        if (getInfoCierreEtapa_CR.data != null && typeof (getInfoCierreEtapa_CR.data) != 'undefined') {\r\n\r\n            return (\r\n\r\n                getInfoCierreEtapa_CR.data.map((etapa, i) => {\r\n                    console.log(\"INFO CIERRE DE ETAPA2\");\r\n\r\n                    return (\r\n\r\n                        <tr key={etapa.id}>\r\n                            <td>{etapa.attributes.etapa.nombre}</td>\r\n                            <td>{etapa.attributes.created_at}</td>\r\n                            <td>{etapa.attributes.funcionario_registra.nombre} {etapa.attributes.funcionario_registra.apellido}</td>\r\n                            <td>{etapa.attributes.dependencia_origen.nombre} </td>\r\n                            <td>Etapa revisada y aprobada por funcionario(a)</td>\r\n                        </tr>\r\n                    )\r\n                })\r\n            )\r\n        }\r\n\r\n\r\n    }\r\n\r\n\r\n\r\n\r\n    const consultaAntecedentes = () => {\r\n\r\n        const dataAntecedente = {\r\n            \"data\": {\r\n                \"type\": \"antecedente\",\r\n                \"attributes\": {\r\n                    \"descripcion\": \"descripcion\",\r\n                    \"fecha_registro\": \"fecha_registro\",\r\n                    \"id_dependencia\": \"id_dependencia\",\r\n                    \"estado\": \"estado\",\r\n                    \"id_proceso_disciplinario\": \"id_proceso_disciplinario\",\r\n                    \"id_etapa\": \"id_etapa\",\r\n                    \"dependencia_origen\": \"dependencia_origen\",\r\n                    \"vigencia\": \"vigencia\",\r\n                    \"fecha_creado\": \"fecha_creado\",\r\n                    \"etapa\": \"etapa\"\r\n                }\r\n            }\r\n        }\r\n\r\n        AntecedentesApi.getPrimerYUltimoAntecedente(dataAntecedente, procesoDisciplinarioId).then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n\r\n                  \r\n                    //window.showModal(2)\r\n\r\n                }\r\n                else {\r\n                    setErrorApi(datos.error.toString())\r\n                    window.showModal()\r\n                }\r\n            }\r\n        )\r\n\r\n    }\r\n\r\n    //Funciones\r\n    const handleClicSoporteRadicado = (id_etapa, id_fase, es_soporte) => {\r\n        from.id_etapa = id_etapa;\r\n        from.id_fase = id_fase;\r\n        changeValueEsFuncion(es_soporte);\r\n    };\r\n\r\n    function changeValueEsFuncion(es_funcion) {\r\n        from.es_soporte = es_funcion;\r\n    }\r\n\r\n    const cerrarEtapa = (datos) => {\r\n\r\n        let data\r\n\r\n        data = {\r\n            \"data\": {\r\n                \"type\": \"cerrar_etapa\",\r\n                \"attributes\": {\r\n                    \"id_proceso_disciplinario\": procesoDisciplinarioId,\r\n                    \"id_etapa\": datos.inputEtapa,\r\n                    \"id_delegada\": 1,\r\n                    \"id_dependencia\": 1,\r\n                    \"descripcion\": \"Cierre de etapa\",\r\n                    \"id_funcionario\": 1,\r\n                    \"created_user\": getUser().nombre,\r\n                }\r\n            }\r\n        }\r\n\r\n        CierreEtapaApi.addCierreEtapa(data).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n\r\n                    setExitoApiTextsCustom(\r\n                        {\r\n                            titulo: 'Asignación del proceso disciplinario',\r\n                            cuerpo: 'El proceso disciplinario es asignado a ' + datos.data.attributes.funcionario_asignado.nombre + ' ' + datos.data.attributes.funcionario_asignado.apellido,\r\n                            boton: 'Aceptar'\r\n                        }\r\n                    );\r\n\r\n                    setEvaluacionRepartoCerrada(true);\r\n                    window.showModal(7);\r\n\r\n                }\r\n                else {\r\n                    console.log(datos.error.toString());\r\n                    setErrorApi(datos.error.toString())\r\n\r\n                    window.showModal(1)\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n\r\n    const componentAntecedente = () => {\r\n\r\n\r\n        return (\r\n            <>\r\n\r\n                <div className=\"col-lg-12 push\">\r\n                    <div className=\"alert alert-info\" role=\"alert\">\r\n                        <h3 className=\"text-center\">(RADICADO) :: {radicado}</h3>\r\n                        <span><strong>Antecedente: </strong></span><br />\r\n                        <span title={(from.antecedente) ? from.antecedente : \"\"}>{from.antecedente ? from.antecedente.substring(0, 300) : \"\"}...</span><br />\r\n\r\n                        <br />\r\n                        <div className=\"row\">\r\n                            <div className=\"col-lg-6\"><span><strong>Registrado Por: </strong>{from.registradoPor ? from.registradoPor : \"Sin Información\"}</span></div>\r\n                            <hr />\r\n                            <div className=\"col-lg-6\"><span><strong>Dependencia registro: </strong> {from.dependendencia ? from.dependendencia : \"Sin Información\"}</span></div>\r\n                            <hr />\r\n                        </div>\r\n\r\n                        <div className=\"row\">\r\n\r\n                            <div className=\"col-lg-4\"><span><strong>Vigencia: </strong>{vigencia}</span></div>\r\n                            <div className=\"col-lg-4\"><span><strong>Fecha de registro: </strong> {moment(from.fechaRegistro, 'DD/MM/YYYY, h:mm:ss a').format(\"DD/MM/YYYY, h:mm:ss a\")}</span></div>\r\n                            <div className=\"col-lg-4\"><span><strong>Fecha de ingreso: </strong> {moment(from.fechaIngreso, 'YYYY/MM/DD').format(\"DD/MM/YYYY\")}</span></div>\r\n\r\n\r\n                        </div>\r\n\r\n                    </div>\r\n                </div>\r\n            </>\r\n        )\r\n    }\r\n\r\n    return (\r\n        <>\r\n            {<Spinner />}\r\n            {isRedirect ? redirectToRoutes() : null}\r\n            {<InfoErrorApi error={getErrorApi} />}\r\n            {<InfoExitoApiTextsCustom success={getExitoApiTextsCustom} />}\r\n            {<InfoExitoCierreEtapa success={getExitoApiTextsCustom} />}\r\n\r\n            <div className=\"row\">\r\n\r\n                <div className=\"col-md-12\">\r\n                    <div className=\"w2d_block\">\r\n                        <nav aria-label=\"breadcrumb\">\r\n                            <ol className=\"breadcrumb breadcrumb-alt push\">\r\n\r\n                                <li className=\"breadcrumb-item\">\r\n                                    <Link underline=\"hover\" className=\"text-dark\" to={`/MisPendientes`}><small>Mis Pendientes</small></Link></li>\r\n                                <li className=\"breadcrumb-item\"><small>Ramas del proceso</small></li>\r\n                            </ol>\r\n                        </nav>\r\n\r\n                    </div>\r\n                </div>\r\n                {getRespuestaAntecedente ? componentAntecedente() : null}\r\n\r\n\r\n                {/*CAPTURA Y REPARTO*/}\r\n                <div className=\"col-lg-4 col-md-6 col-sm-6\">\r\n                    <div className=\"block block-themed\">\r\n\r\n\r\n                        <div classNa