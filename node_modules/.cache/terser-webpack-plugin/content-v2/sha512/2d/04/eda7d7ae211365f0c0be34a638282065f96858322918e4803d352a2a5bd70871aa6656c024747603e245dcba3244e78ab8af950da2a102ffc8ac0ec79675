data[0].attributes.nombre_completo_funcionario_actual, show: true, \r\n                        redirect: '/MisPendientes' , from: { from }, alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                }\r\n                else {\r\n                    setModalState({ title: getNombreProceso + \" :: Validar clasificacin\", message: datos.error.toString(), show: true, \r\n                    redirect: '/ValidarClasificacionRadicadoForm', from: { from }, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                }\r\n                window.showSpinner(false);\r\n            }\r\n        )\r\n    }\r\n\r\n\r\n\r\n    return (\r\n\r\n        <>\r\n            {<Spinner />}\r\n            <ModalGen data={modalState} />\r\n\r\n            <div className=\"row\">\r\n\r\n\r\n                <div className=\"w2d_block\">\r\n                    <nav aria-label=\"breadcrumb\">\r\n                        <ol className=\"breadcrumb breadcrumb-alt push\">\r\n                            <li className=\"breadcrumb-item\"> <Link underline=\"hover\" className=\"text-dark\" to={`/MisPendientes`}><small>Mis Pendientes</small></Link></li>\r\n                            <li className=\"breadcrumb-item\"> <Link underline=\"hover\" className=\"text-dark\" to={`/RamasProceso/`} state={{ from: from, disable: disable }}><small>Ramas del proceso</small></Link></li>\r\n                            <li className=\"breadcrumb-item\"> <Link underline=\"hover\" className=\"text-dark\" aria-current=\"page\" to={`/ClasificacionRadicadoLista/`} state={{ from: from, disable: disable }}><small>Validar Clasificacin</small></Link></li>\r\n                        </ol>\r\n                    </nav>\r\n                </div>\r\n\r\n\r\n                <div className=\"col-md-12\">\r\n                    <div className=\"block block-themed\">\r\n                        <div className={(disable ? (\"block-header bg-dark\") : \"block-header\")}>\r\n                            <h3 className=\"block-titl