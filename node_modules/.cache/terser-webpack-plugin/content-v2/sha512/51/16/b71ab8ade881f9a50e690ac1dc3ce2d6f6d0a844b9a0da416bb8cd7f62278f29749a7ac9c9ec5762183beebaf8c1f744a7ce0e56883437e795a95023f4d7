ange={e => handleInputChangeArchivos(e, i)} required={x.filebase64 == \"\"} />\r\n                                                            <label>Peso del archivo: {formatBytes(x.archivo.size)}</label>\r\n                                                        </div>\r\n                                                        {\r\n                                                            inputListArchivos.length !== 1\r\n                                                                ?\r\n                                                                (\r\n                                                                    <div className=\"col-md-2\">\r\n                                                                        <button type=\"button\" className=\"btn btn-danger\" onClick={() => handleRemoveClick(i)}><i className=\"fas fa-trash-alt col\"></i></button>\r\n                                                                    </div>\r\n                                                                )\r\n                                                                :\r\n                                                                null\r\n                                                        }\r\n\r\n                                                    </div>\r\n                                                </div>\r\n                                                <div className='col-md-3'>\r\n                                                    <div className='row'>\r\n                                                        {\r\n                                                            inputListArchivos.length - 1 === i\r\n                                                                ?\r\n                                                                (\r\n                                                                    <div className=\"col-md-12\">\r\n                                                                        <button type=\"but