ciaOrigen = () => {\r\n        return (\r\n            listaDependenciaOrigen.data.map((dependencia, i) => {\r\n                return (\r\n                    <option key={dependencia.id} value={dependencia.id}>{dependencia.attributes.nombre}</option>\r\n                )\r\n            })\r\n        )\r\n    }\r\n\r\n    const componentSirius = (paso) => {\r\n        if (paso === 1) {\r\n            return (\r\n                <>{<Spinner />}\r\n                    <Formik\r\n                        initialValues={{\r\n                            codigoSirius: '',\r\n                            origenRadicado: ''\r\n                        }}\r\n                        validate={(valores) => {\r\n                            let errores = {}\r\n                            if (!valores.codigoSirius) {\r\n                                errores.codigoSirius = 'Debe ingresar un código de SIRIUS'\r\n                            }\r\n                            if (!valores.origenRadicado) {\r\n                                errores.origenRadicado = 'Debe seleccionar un origen'\r\n                            }\r\n                            return errores\r\n                        }}\r\n                        onSubmit={(valores, { resetForm }) => {\r\n                            consultarRadicado(valores)\r\n                        }}>\r\n                        {({ errors }) => (\r\n                            <Form>\r\n                                <div className=\"row\">\r\n                                    <div className=\"col-md-6\">\r\n                                        <div className=\"form-group\">\r\n                                            <label htmlFor=\"codigoSirius\">SIRIUS <span className=\"text-danger\">*</span></label>\r\n                                            <Field as=\"input\" className=\"form-control\" type=\"text\" id=\"codigoSirius\" name=\"codigoSirius\" placeholder=\"SIRUS\" />\r\n                                            <E