            listaVigenciaDesglose.data.map((vigencia, i) => {\r\n                return (\r\n                    <option key={vigencia.id} value={vigencia.vigencia}>{vigencia.attributes.vigencia}</option>\r\n                )\r\n            })\r\n        )\r\n    }\r\n\r\n    const selectVigenciaAutoDesglose = () => {\r\n        return (\r\n            listaVigenciaAutoDesglose.data.map((vigencia, i) => {\r\n                return (\r\n                    <option key={vigencia.id} value={vigencia.vigencia}>{vigencia.attributes.vigencia}</option>\r\n                )\r\n            })\r\n        )\r\n    }\r\n\r\n\r\n\r\n\r\n    const consultarRadicado = (datos) => {\r\n        window.showSpinner(true);\r\n        let tipoProceso = listaTipoProceso.data.filter(\r\n            proceso => (proceso.id).includes(selectedTipoIngreso)\r\n        );\r\n        if (tipoProceso.length > 0) {\r\n            setTipoProceso(tipoProceso[0].attributes.nombre);\r\n        }\r\n\r\n        if (selectedTipoIngreso === global.Constants.TIPO_INGRESSO.SIRIUS) {//SIRIUS\r\n            setDatosFormSiriusConsulta(datos);\r\n\r\n\r\n\r\n\r\n            const data = {\r\n                \"data\": {\r\n                    \"type\": \"proceso_sirius\",\r\n                    \"attributes\": {\r\n                        \"radicado\": datos.codigoSirius,\r\n                        \"id_origen_radicado\": \"1\",\r\n                        \"id_tipo_proceso\": selectedTipoIngreso,\r\n                    }\r\n                }\r\n            }\r\n\r\n            //console.log(JSON.stringify(data));\r\n            ProcesoDiciplinarioApi.consultarRadicadoSirius(data).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        window.showModal(2)\r\n                        setCountTextArea(datos.data.attributes.antecedente.length)\r\n                        setRadicacionSirius(datos)\r\n                        setValidacionExitosa(true)\r\n                        window.showSpinner(false);\r\n                    }\r\n                    else {\r\n                        setErrorApi(datos.error.toString())\r\n                        window.showModal(1)\r\n                        window.showSpinner(false);\r\n                    }\r\n\r\n                }\r\n            )\r\n        }\r\n        else if (selectedTipoIngreso === global.Constants.TIPO_INGRESSO.DESGLOSE) {//DESGLOCE\r\n            setDatosFormDesgloseConsulta(datos);\r\n\r\n            const data = {\r\n                \"data\": {\r\n                    \"type\": \"proceso_desgloce\",\r\n                    \"attributes\": {\r\n                        \"radicado\": datos.codigoDesglose,\r\n                        \"id_tipo_proceso\": selectedTipoIngreso,\r\n                        \"vigencia\": datos.vigenciaDesglose ? datos.vigenciaDesglose : getFechaVigenciaDesglose,\r\n                        \"observacion_mesa_trabajo\": datos.mesaTrabajo\r\n                    }\r\n                }\r\n            }\r\n\r\n            //console.log(JSON.stringify(data));\r\n\r\n            ProcesoDiciplinarioApi.consultarRadicadoDesgloce(data).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        console.log(datos);\r\n                        window.showModal(4)\r\n                        setCountTextArea(datos.data.attributes.antecedente.length)\r\n                        setRadicacionDesglose(datos)\r\n                        setValidacionExitosa(true)\r\n                        window.showSpinner(false);\r\n\r\n                    }\r\n                    else {\r\n                        setErrorApi(datos.error.toString())\r\n                        window.showModal(1)\r\n                        window.showSpinner(false);\r\n                    }\r\n\r\n                }\r\n            )\r\n        }\r\n        else {//SINPROC\r\n            setDatosFormSinprocConsulta(datos);\r\n\r\n            const data = {\r