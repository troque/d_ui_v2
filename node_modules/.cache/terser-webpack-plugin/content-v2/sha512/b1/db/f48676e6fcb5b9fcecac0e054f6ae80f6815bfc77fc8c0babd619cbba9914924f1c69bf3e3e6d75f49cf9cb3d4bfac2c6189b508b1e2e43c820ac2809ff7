vo\r\n            GenericApi.getGeneric(\"archivo-actuaciones/get-documento/\" + id_documento + \"/\" + extension).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        downloadBase64File(datos.content_type, datos.base_64, nombre_documento);\r\n                    } else {\r\n                        setErrorApi(datos.error.toString())\r\n                        window.showModal(1)\r\n                    }\r\n                    window.showSpinner(false);\r\n                }\r\n            )\r\n        } catch (error) {\r\n            console.error(error);\r\n        }\r\n    };\r\n\r\n    const handleClicArchivoManual = () => {\r\n        try {\r\n            window.showSpinner(true);\r\n\r\n            const data = {\r\n                \"data\": {\r\n                    \"type\": \"mas_actuaciones\",\r\n                    \"attributes\": {\r\n                        \"nombre_actuacion\": \"\",\r\n                        \"nombre_plantilla\": \"\",\r\n                        \"id_etapa\": \"\",\r\n                        \"estado\": \"\",\r\n                        \"id_etapa_despues_aprobacion\": \"\",\r\n                        \"despues_aprobacion_listar_actuacion\": \"\",\r\n                        \"nombre_plantilla_manual\": \"\",\r\n                        \"fileBase64\": \"\",\r\n                        \"fileBase64_manual\": \"\"\r\n                    }\r\n                }\r\n            }\r\n\r\n            GenericApi.getByDataGeneric('mas_actuaciones/manual/' + getidMasActuacion, data).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        downloadBase64FileManual(datos.content_type, datos.base_64, datos.file_name);\r\n                    }\r\n                    else {\r\n\r\n                    }\r\n                    window.showSpinner(false);\r\n                }\r\n            )\r\n\r\n        } catch (error) {\r\n            console.error(error);\r\n        }\r\n    };\r\n\r\n    function downloadBase64FileManual(contentType, base64Data, fileName) {\r\n        const linkSource = `data:${contentType};base64,${base64Data}`;\r\n        const downloadLink = document.createElement(\"a\");\r\n        downloadLink.href = linkSource;\r\n        downloadLink.download = fileName;\r\n        downloadLink.click();\r\n    }\r\n\r\n    function downloadBase64File(contentType, base64Data, fileName) {\r\n        const linkSource = `data:${contentType};base64,${base64Data}`;\r\n        const downloadLink = document.createElement(\"a\");\r\n        downloadLink.href = linkSource;\r\n        downloadLink.download = fileName;\r\n        downloadLink.click();\r\n    }\r\n\r\n    const infoTrazabilidad = () => {\r\n        setUltimoEstado(getTrazabilidadListaSearch.data[0].attributes.nombre_estado_actuacion);\r\n        setUsuarioSolicitante(getTrazabilidadListaSearch.data[0].attributes.created_user);\r\n        return (getTrazabilidadListaSearch.data.map((t) => {\r\n\r\n            if (t.attributes.nombre_estado_actuacion == \"Aprobada\") {\r\n                setActuacionEstaAprobadaSioNo(true);\r\n            }\r\n\r\n            let nombreDependencia = t.attributes.id_dependencia == \"\" ? \"\" : \"(\" + t.attributes.id_dependencia.nombre + \")\";\r\n\r\n            return (\r\n                <tr key={t.attributes.id}>\r\n                    <td>\r\n                        {t.attributes.created_at}\r\n                    </td>\r\n                    <td>\r\n                        {t.attributes.usuario.nombre + \" \" + t.attributes.usuario.apellido + nombreDependencia}\r\n                    </td>\r\n                    <td>\r\n                        {t.attributes.updated_user}\r\n                    </td>\r\n                    <td>\r\n                        {t.attributes.nombre_estado_actuacion}\r\n                    </td>\r\n                    <td>\r\n                        {t.attributes.observacion}\r\n                    </td>\r\n                </tr>\r\n            )\r\n        }))\r\n    }\r\n\r\n    const infoArchivos = () => {\r\n\r\n        let index = 0;\r\n\r\n        return (getArchivosListaSearch.data.map((t) => {\r\n            return (\r\n                <tr key={t.attributes.id}>\r\n                    <td>\r\n                        {t.attributes.nombre_tipo_archivo}\r\n                    </td>\r\n                    <td>\r\n                        {t.attributes.nombre_archivo}\r\n                    </td>\r\n                    <td>\r\n                        <div className='col-3'>\r\n                            <button type='button' title='Descargar documento' className='btn btn-sm btn-primary' onClick={() => handleClicArchivo(t.attributes.id, t.attributes.extension, t.attributes.nombre_archivo)}><i className=\"fas fa-download\"></i></button>\r\n                        </div>\r\n                    </td>\r\n                    <td style={{ width: '35%' }}>\r\n                        <div className='col-12'>\r\n\r\n                            {(getUltimoEstado == \"Aprobada\" ||\r\n                                getUltimoEstado == \"Aprobada y pdf definitivo\" ||\r\n                                getUltimoEstado == \"Remitida\" ||\r\n                                getUltimoEstado != \"Documento firmado\") ?\r\n\r\n                                <div className=\"col-md-12\">\r\n                                    <label >{tipoActuacion == \"0\" ? \"La actuación\" : (tipoActuacion == \"1\" ? 'El impedimento' : 'El comisorio')} no se puede actualizar ya que se encuentra aprobado</label>\r\n                                </div>\r\n\r\n                                :\r\n\r\n                                disable != true ? (\r\n                                    inputListArchivos.map((x, i) => {\r\n                                        let indice = index++;\r\n\r\n                                        return (\r\n                                            <div className=\"col-md-12\" key={i}>\r\n                                                <label className=\"custom-file-label\" htmlFor=\"example-file-input-custom\" name=\"input-file\" data-toggle=\"custom-file-input\">{x.archivo.name}</label>\r\n                                                <input className=\"custom-file-input\" data-toggle=\"custom-file-input\" type=\"file\" name=\"archivo\" onChange={e => handleInputChangeArchivos(e, i, 1, t.attributes.id, t.attributes.uuid_actuacion, t.attributes.nombre_tipo_archivo, indice)} required />\r\n                                                <label >Peso del archivo: {formatBytes(x.archivo.size)}</label>\r\n                                            </div>\r\n                                        );\r\n                                    })\r\n                                ) : null\r\n                            }\r\n\r\n                        </div>\r\n                    </td>\r\n                </tr >\r\n            )\r\n        }))\r\n    }\r\n\r\n    const handleInputChangeArchivos = (e, index, actualizar = 0, id = 0, uuid_actuacion = 0, tipo_archivo = \"\", valorTabla = \"\") => {\r\n\r\n        const { name, files } = e.target;\r\n        setTipoDocumentoSubir(valorTabla);\r\n\r\n        if (files.length > 0) {\r\n            const extension = getFileExtension(files[0].name);\r\n\r\n            var error = 0;\r\n\r\n            if (tipo_archivo == \"Documento inicial\" && actualizar == 1 && extension != \"docx\") {\r\n                setErrorApi(\"El archivo seleccionado no tiene un formato permitido debe ser .docx\");\r\n                window.showModal(1);\r\n                error = 1;\r\n                return false;\r\n            } else if (tipo_archivo == \"Documento definitivo\" && actualizar == 1 && extension != \"pdf\") {\r\n                setErrorApi(\"El archivo seleccionado no tiene un formato permitido, debe ser .pdf\");\r\n                window.showModal(1);\r\n                error = 1;\r\n                return false;\r\n            } else if (tipo_archivo == \"Documento definitivo\" && actualizar == 0 && extension != \"pdf\") {\r\n                setErrorApi(\"El archivo seleccionado no tiene un formato permitido, debe ser .pdf\");\r\n                window.showModal(1);\r\n                error = 1;\r\n                return false;\r\n            }\r\n\r\n            if (error == 0) {\r\n\r\n                if (actualizar == 0) {\r\n                    var list = [...inputListArchivos];\r\n                    list[index][name] = files[0];\r\n                } else {\r\n                    var list = [...inputActualizarArchivos];\r\n                    list[index][name] = files[0];\r\n                    setValidarAccion(1);\r\n                }\r\n\r\n                if (files[0]) {\r\n                    list[index][name] = files[0];\r\n                } else {\r\n                    list[index][name] = '';\r\n                    list[index].filebase64 = '';\r\n                    list[index].size = 0;\r\n                    list[index].ext = \"\";\r\n                    list[index].uuid = \"\";\r\n                    if (actualizar == 0) {\r\n                        setInputListArchivos(list);\r\n                    } else {\r\n                        setInputActualizarArchivos(list);\r\n                    }\r\n                    return false;\r\n                }\r\n\r\n                // Conversion a Base64\r\n                Array.from(e.target.files).forEach(archivo => {\r\n                    var reader = new FileReader();\r\n                    reader.readAsDataURL(archivo);\r\n                    reader.onload = function () {\r\n                        var arrayAuxiliar = [];\r\n                        var base64 = reader.result;\r\n                        arrayAuxiliar = base64.split(',');\r\n                        list[index].filebase64 = arrayAuxiliar[1];\r\n                        list[index].ext = extension;\r\n                        list[index].uuid = id;\r\n                        if (actualizar == 0) {\r\n                            setInputListArchivos(list);\r\n                        } else {\r\n                            list[index].uuid_actuacion = uuid_actuacion;\r\n                            setInputActualizarArchivos(list);\r\n                            setValidarAccion(1);\r\n                        }\r\n                        setRtaInfoTrue(true);\r\n                        window.showModal(9)\r\n                    }\r\n                })\r\n                obtenerPesoTotalArchivos(list);\r\n            } else {\r\n                if (actualizar == 0) {\r\n                    var list = [...inputListArchivos];\r\n                } else {\r\n                    var list = [...inputActualizarArchivos];\r\n                }\r\n                list[index][name] = files[0];\r\n                list[index].filebase64 = '';\r\n                list[index].size = 0;\r\n                list[index].uuid = \"\";\r\n                if (actualizar == 0) {\r\n                    setInputListArchivos(list);\r\n                } else {\r\n                    list[index].uuid_actuacion = uuid_actuacion;\r\n                    setInputActualizarArchivos(list);\r\n                }\r\n            }\r\n        } else {\r\n            if (actualizar == 0) {\r\n                var list = [...inputListArchivos];\r\n            } else {\r\n                var list = [...inputActualizarArchivos];\r\n            }\r\n            list[index][name] = '';\r\n            list[index].filebase64 = '';\r\n            list[index].size = 0;\r\n            list[index].uuid = \"\";\r\n            if (actualizar == 0) {\r\n                setInputListArchivos(list);\r\n            } else {\r\n                setInputActualizarArchivos(list);\r\n            }\r\n            obtenerPesoTotalArchivos(list);\r\n            return false;\r\n        }\r\n    }\r\n\r\n    function getFileExtension(filename) {\r\n        return filename.slice((filename.lastIndexOf(\".\") - 1 >>> 0) + 2);\r\n    }\r\n\r\n    function obtenerPesoTotalArchivos(list) {\r\n        let peso = 0;\r\n        list.forEach(\r\n            (archivo, index) => {\r\n                if (archivo.archivo.size) {\r\n                    peso += archivo.archivo.size;\r\n                }\r\n            }\r\n        )\r\n\r\n        if (peso > 15000000) {\r\n            setErrorApi('El peso/tamaño de los todos los adjuntos superan los 15 Mb pertmitidos para el registro, verifique e tamaño y elimine algunos adjuntos.')\r\n            window.showModal(1)\r\n        }\r\n\r\n        setPesoTotalArchivos(peso);\r\n    }\r\n\r\n    function formatBytes(bytes, decimals = 3) {\r\n        if (bytes === undefined) return '0 Bytes';\r\n        if (bytes === 0) return '0 Bytes';\r\n        const k = 1024;\r\n        const dm = decimals < 0 ? 0 : decimals;\r\n        const sizes = ['Bytes', 'KB', 'MB', 'GB', 'TB', 'PB', 'EB', 'ZB', 'YB'];\r\n        const i = Math.floor(Math.log(bytes) / Math.log(k));\r\n        return parseFloat((bytes / Math.pow(k, i)).toFixed(dm)) + ' ' + sizes[i];\r\n    }\r\n\r\n    function subirArchivo(e, accion) {\r\n\r\n        if (e.type == \"click\") {\r\n\r\n            window.showSpinner(true);\r\n\r\n            if (accion == 0) {\r\n                const data = {\r\n                    \"data\": {\r\n                        \"type\": 'archivo-actuaciones/up-documento',\r\n                        \"attributes\": {\r\n                            \"uuid_actuacion\": getArchivosListaSearch.data[0].attributes.uuid_actuacion,\r\n                            \"fileBase64\": inputListArchivos[0].filebase64,\r\n                            \"nombre_archivo\": inputListArchivos[0].archivo.name,\r\n                            \"extension\": inputListArchivos[0].ext,\r\n                            \"peso\": inputListArchivos[0].archivo.size,\r\n                            \"id_tipo_archivo\": 2\r\n                        }\r\n                    }\r\n                }\r\n\r\n                GenericApi.addGeneric(\"archivo-actuaciones/up-documento\", data).then(\r\n                    datos => {\r\n\r\n                        if (!datos.error) {\r\n                            setModalState({ title: \"SINPROC No \" + radicado + \" :: Crear Actuaciones\", message: 'Registro Exitoso', show: true, redirect: \"/ActuacionesLista/\", from: { from: from, selected_id_etapa: selected_id_etapa }, alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                        } else {\r\n                            setModalState({ title: \"SINPROCNo \" + radicado + \" :: Crear Actuaciones\", message: datos.error.toString(), show: true, redirect: \"/ActuacionesLista/\", from: { from: from, selected_id_etapa: selected_id_etapa }, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                        }\r\n                        window.showSpinner(false);\r\n                    }\r\n                )\r\n            } else if (accion == 1) {\r\n\r\n                const data = {\r\n                    \"data\": {\r\n                        \"type\": 'archivo-actuaciones/update-documento',\r\n                        \"attributes\": {\r\n                            \"uuid_actuacion\": inputActualizarArchivos[0].uuid,\r\n                            \"fileBase64\": inputActualizarArchivos[0].filebase64,\r\n                            \"nombre_archivo\": inputActualizarArchivos[0].archivo.name,\r\n                            \"extension\": inputActualizarArchivos[0].ext,\r\n                            \"peso\": inputActualizarArchivos[0].archivo.size,\r\n                            \"id_tipo_archivo\": 2,\r\n                            \"tipoDocumentoActualizar\": getTipoDocumentoSubir\r\n                        }\r\n                    }\r\n                }\r\n\r\n                GenericApi.getByDataGeneric(\"archivo-actuaciones/update-documento/\" + inputActualizarArchivos[0].uuid + \"/\" + inputActualizarArchivos[0].uuid_actuacion, data).then(\r\n                    datos => {\r\n\r\n                        if (!datos.error) {\r\n                            setModalState({ title: \"SINPROC No \" + radicado + \" :: Crear Actuaciones\", message: 'Registro actualizado correctamente', show: true, redirect: \"/ActuacionesLista/\", from: { from: from, selected_id_etapa: selected_id_etapa }, alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                        } else {\r\n                            setModalState({ title: \"SINPROCNo \" + radicado + \" :: Crear Actuaciones\", message: datos.error.toString(), show: true, redirect: \"/ActuacionesLista/\", from: { from: from, selected_id_etapa: selected_id_etapa }, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                        }\r\n                        window.showSpinner(false);\r\n                    }\r\n                )\r\n            }\r\n\r\n\r\n        }\r\n    }\r\n\r\n    const IdDependenciaAceptaSolicitudesDeAnulacion = () => {\r\n        const data = {\r\n            \"data\": {\r\n                \"type\": \"mas_parametro\",\r\n                \"attributes\": {\r\n                    \"nombre\": \"id_dependencia_que_atiende_solicitud_de_anulacion\"\r\n                }\r\n            }\r\n        }\r\n\r\n        //buscamos el parametro\r\n        GenericApi.getByDataGeneric(\"parametro/parametro-nombre\", data).then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    if (datos.data[0].attributes.valor != null) {\r\n                        IdUsuarioJefeDependencia(datos.data[0].attributes.valor);\r\n                    } else {\r\n                        // console.log(\"No existe una dependencia asignada para atender solicitudes de inactivación\");\r\n                    }\r\n\r\n                } else {\r\n                    // Aqui tengo que colocar algun mensaje de que no existe una dependencia asignada\r\n                    // para atender las solicitudes de anulacion\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const IdUsuarioJefeDependencia = (id) => {\r\n        GenericApi.getByIdGeneric(\"mas-dependencia-origen\", id).then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    if (datos.data.attributes.id_usuario_jefe != null) {\r\n                        consultarUsuario(datos.data.attributes.id_usuario_jefe);\r\n                    } else {\r\n                        // console.log(\"No existe un usuario jefe en la dependencia que atiende solicitudes de inactivación\");\r\n                    }\r\n\r\n                } else {\r\n\r\n                }\r\n\r\n            }\r\n        )\r\n    }\r\n\r\n    const consultarUsuario = (id) => {\r\n        GenericApi.getByIdGeneric(\"usuario\", id).then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    setNameUsuario(datos.data.attributes.name);\r\n                } else {\r\n\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const dataFirmasMecanica = () => {\r\n        GenericApi.getGeneric(\"actuaciones/get-firmas-por-actuacion/\" + id).then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    if (datos.data.length > 0) {\r\n                        datos.data.forEach(element => {\r\n                            // console.log(element);\r\n                            if (element.attributes.estado == global.Constants.ESTADO_FIRMA_MECANICA.PENDIENTE_FIRMA) {\r\n                                setActuacionPendienteDeFirmas(true);\r\n                            }\r\n                        });\r\n\r\n                        setListaFirmasMecanicas(datos);\r\n                        setFirmasMecanicas(true);\r\n                    }\r\n                }\r\n            }\r\n        )\r\n\r\n        // Se carga el metodo cuando hay por lo menos un usuario pendiente de firma\r\n        validarFirmadoDocumentoTodosUsuarios();\r\n    }\r\n\r\n    const handleClicEliminarFirmaUser = (idFirma) => {\r\n        let data = {\r\n            \"data\": {\r\n                \"type\": \"Agregar Usuario\",\r\n                \"attributes\": {\r\n                    \"estado\": global.Constants.ESTADO_FIRMA_MECANICA.ELIMINADO,\r\n                }\r\n            }\r\n        }\r\n\r\n        GenericApi.updateGeneric(\"actuaciones/set-eliminar-firmas-mecanicas\", idFirma, data).then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    window.location.reload();\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const ListaFirmasMecanicas = () => {\r\n        return (getListaFirmasMecanicas.data.map((t) => {\r\n            if (t.attributes.estado == global.Constants.ESTADO_FIRMA_MECANICA.ELIMINADO) {\r\n                return null;\r\n            } else {\r\n                return (\r\n                    <tr>\r\n                        <td>\r\n                            {t.attributes.id_user.nombre + \" \" + t.attributes.id_user.apellido}\r\n                        </td>\r\n                        <td>\r\n                            {t.attributes.tipo_firma2.nombre}\r\n                        </td>\r\n                        <td>\r\n                            {t.attributes.estado == global.Constants.ESTADO_FIRMA_MECANICA.PENDIENTE_FIRMA ? \"Pendiente firma\" : null}\r\n                            {t.attributes.estado == global.Constants.ESTADO_FIRMA_MECANICA.FIRMADO ? \"Firmado\" : null}\r\n                        </td>\r\n                        <td>\r\n                            {getRtaJefe && t.attributes.estado == global.Constants.ESTADO_FIRMA_MECANICA.PENDIENTE_FIRMA ? (\r\n                                <button type=\"button\" className=\"btn btn-rounded btn-primary\" value={t.attributes.id} onClick={() => handleClicEliminarFirmaUser(t.id)}><i class=\"fas fa-trash-alt\"></i> Eliminar</button>\r\n                            ) : null}\r\n\r\n                            {getUser().nombre == t.attributes.id_user.name && t.attributes.estado != global.Constants.ESTADO_FIRMA_MECANICA.FIRMADO ? (\r\n                                t.attributes.id_user.firma_mecanica != null ?\r\n                                    (\r\n                                        <Link to=\"/FirmarDocumentoActuacion\" state={{ from: from, FirmaActuacion: t, nombreActuacion: nombre, estadoActualActuacion: estadoActualActuacion, tipoActuacion: tipoActuacion }}>\r\n                                            <button type=\"button\" className=\"btn btn-rounded btn-primary\" >Firmar</button>\r\n                                        </Link>\r\n                                    )\r\n                                    :\r\n                                    (\r\n                                        <button type=\"button\" className=\"btn btn-rounded btn-primary\" onClick={() => setModalState({ title: \"SINPROCNo \" + radicado + \" :: Firmar Actuación\", message: \"Falta agregar firma mecánica a su perfil\", show: true, redirect: \"/AgregarFirma/\", alert: global.Constants.TIPO_ALERTA.ERROR })}>Firmar</button>\r\n                                    )\r\n\r\n\r\n                            ) : null}\r\n\r\n                        </td>\r\n                    </tr>\r\n                )\r\n            }\r\n\r\n        }));\r\n    }\r\n\r\n    return (\r\n        <>\r\n            {<Spinner />}\r\n            {<ModalGen data={getModalState} />}\r\n            {<InfoErrorApi error={errorApi} />}\r\n            {getRtaInfoTrue ? <InfoConfirmarAccion mensaje={'¿Esta seguro que desea subir este archivo?'} function={e => subirArchivo(e, getValidarAccion == 0 ? getValidarAccion : 1)} /> : null}\r\n            {<InfoExitoApi />}\r\n            {getInfoModal ? <ModalActuaciones accionActuacion={getMessageModal} /> : null}\r\n            <Formik\r\n                initialValues={{\r\n         