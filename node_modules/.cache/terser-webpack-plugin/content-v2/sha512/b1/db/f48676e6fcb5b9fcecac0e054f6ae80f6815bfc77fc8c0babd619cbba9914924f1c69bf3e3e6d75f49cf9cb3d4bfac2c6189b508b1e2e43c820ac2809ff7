aJefe] = useState(false);\r\n    const [getInfoModal, setInfoModal] = useState(false);\r\n    const [getMessageModal, setMessageModal] = useState('');\r\n    const [getNameUsuario, setNameUsuario] = useState(\"\");\r\n    const [getUltimoEstado, setUltimoEstado] = useState(\"\");\r\n    const [getUsuarioSolicitante, setUsuarioSolicitante] = useState(\"\");\r\n    const [getTipoDocumentoSubir, setTipoDocumentoSubir] = useState(\"\");\r\n    const [getFirmasMecanicas, setFirmasMecanicas] = useState(false);\r\n    const [getListaFirmasMecanicas, setListaFirmasMecanicas] = useState({ data: [], links: [], meta: [] });\r\n    const [getActuacionPendienteDeFirmas, setActuacionPendienteDeFirmas] = useState(false);\r\n    const [getActuacionEstaAprobadaSioNo, setActuacionEstaAprobadaSioNo] = useState(false);\r\n    const [getValidarUsuariosPendienteFirma, setValidarUsuariosPendienteFirma] = useState(false);\r\n    const [getMuestraBotonFechaParaSemaforo, setMuestraBotonFechaParaSemaforo] = useState(false);\r\n\r\n    useEffect(() => {\r\n        async function fetchData(params) {\r\n            window.showSpinner(true);\r\n            IdDependenciaAceptaSolicitudesDeAnulacion();\r\n            setNombreUsuario(getUser().nombre);\r\n            \r\n\r\n            GenericApi.getGeneric('mas_actuaciones/getActuacionesByName/' + nombre).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        setidMasActuacion(datos.data[0].id)\r\n                        GenericApi.getGeneric('semaforo').then(\r\n                            datosSemaforo => {\r\n                                if (!datosSemaforo.error) {\r\n                                    datosSemaforo.data.forEach(element => {\r\n                                        console.log(element);\r\n                                        if(element.attributes.id_mas_actuacion_inicia != null){\r\n                                            if(element.attributes.id_mas_actuacion_inicia.id == datos.data[0].id){\r\n                                                setMuestraBotonFechaParaSemaforo(true);\r\n                                            }\r\n                                        }\r\n                                    });\r\n                                    window.showSpinner(false);\r\n                                }else{\r\n            \r\n                                }\r\n                            }\r\n                        )\r\n                    } else {\r\n\r\n                    }\r\n                }\r\n            )\r\n\r\n            \r\n\r\n            // Buscamos el detalle de los documentos\r\n            GenericApi.getGeneric(\"archivo-actuaciones/get-archivo-actuaciones-by-uuid/\" + id).then(\r\n                datos => {\r\n\r\n                    if (!datos.error) {\r\n                        if (datos.data.length > 0) {\r\n                            setArchivosListaSearch(datos);\r\n                            setRtaInfoArchivoActuaciones(true);\r\n                        }\r\n                    } else {\r\n                        // cargarHistorialCambios(id);\r\n                        // setModalState({ title: \"Datos del interesado\", message: datos.error.toString(), show: true, redirect: '/DatosInteresadoLista', from: { from } });\r\n                    }\r\n                }\r\n            )\r\n\r\n            // Buscamos el detalle de la trazabilidad\r\n            GenericApi.getGeneric(\"trazabilidad-actuaciones/get-all-trazabilidad-actuaciones-uuid/\" + id).then(\r\n                datos => {\r\n\r\n                    if (!datos.error) {\r\n                        if (datos.data.length > 0) {\r\n                            \r\n\r\n                            setTrazabilidadListaSearch(datos);\r\n                            setRtaInfoTrazabilidadActuaciones(true);\r\n                        }\r\n                    } else {\r\n                        // cargarHistorialCambios(id);\r\n     