/Usuario\",\r\n        muestra_atras: false\r\n    });\r\n\r\n    const handleCallback = (childData) => {\r\n        try {\r\n       \r\n            setEstadoLista(childData)\r\n        } catch (error) {\r\n\r\n        }\r\n\r\n    }\r\n\r\n    const columns = [\r\n        {\r\n            name: 'Nombre',\r\n            cell: usuario => <div>{(usuario.attributes.nombre ? usuario.attributes.nombre : \"\") + ' ' + (usuario.attributes.apellido ? usuario.attributes.apellido : \"\")}</div>,\r\n            selector: usuario => usuario.attributes.nombre,\r\n            sortable: true,\r\n            width: \"15%\"\r\n        },\r\n        {\r\n            name: 'Usuario',\r\n            selector: usuario => usuario.attributes.name,\r\n            sortable: true,\r\n            width: \"15%\"\r\n        },\r\n        {\r\n            name: 'Correo',\r\n            selector: usuario => usuario.attributes.email,\r\n            sortable: true,\r\n            width: \"20%\"\r\n        },\r\n        {\r\n            name: 'Dependencia',\r\n            selector: usuario => (usuario.attributes.dependencia ? usuario.attributes.dependencia.nombre : ''),\r\n            sortable: true,\r\n            width: \"20%\"\r\n\r\n        },\r\n        {\r\n            name: 'Estado',\r\n            selector: usuario => (usuario.attributes.estado == \"1\" ? 'Activo' : 'Inactivo'),\r\n            sortable: true,\r\n            width: \"10%\"\r\n        },\r\n        {\r\n            name: 'Acciones',\r\n\r\n            cell: row => <div><Link to={`${row.id}`}>\r\n                <button type=\"button\" className=\"btn btn btn-primary\" title='Editar'>\r\n                    <i className=\"fa fa-fw fa-edit\"></i>\r\n                </button>\r\n            </Link></div>\r\n\r\n\r\n        }\r\n    ];\r\n\r\n    const redirectToRoutes = () => {\r\n        return <Navigate to={`/UsuarioLista/`} />;\r\n    }\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n            window.showSpinner(true);\r\n            cargarUsuarios(1, paganationPerPages);\r\n\r\n\r\n\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n    const cargarUsuarios = (page, perPage) => {\r\n        //cargamos todos los usuarios\r\n        //GenericApi.getAllGeneric('usuario/usuario-paginate' + '/' + page + '/' + perPage).then(\r\n        GenericApi.getAllGeneric('usuario-sin-estado').then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    if (datos[\"data\"] != \"\") {\r\n                        setUsuarioListaSearch(datos)\r\n                        window.showSpinner(false);\r\n                    }\r\n                    else {\r\n                        setErrorApi(datos.error.toString())\r\n                        window.showModal(1)\r\n                        window.showSpinner(false);\r\n\r\n                    }\r\n                }\r\n                else {\r\n                    setErrorApi(datos.error.toString())\r\n                    window.showModal(1)\r\n                    window.showSpinner(false);\r\n                }\r\n\r\n            }\r\n        )\r\n    }\r\n\r\n    const handlePageChange = page => {\r\n\r\n        //window.showSpinner(true);\r\n        setPageActual(page);\r\n        //cargarUsuarios(page, perPage);\r\n    }\r\n\r\n    const handlePerRowsChange = async (newPerPage, page) => {\r\n        //window.showSpinner(true);\r\n        setPerPage(newPerPage);\r\n        setPageActual(page);\r\n        //cargarUsuarios(page, newPerPage);\r\n\r\n    }\r\n\r\n    return (\r\n        <>\r\n\r\n\r\n\r\n\r\n            {isRedirect ? redirectToRoutes() : null}\r\n            {<Spinner />}\r\n            {<InfoErrorApi error={errorApi} />}\r\n            {<InfoExitoApi />}\r\n            <Formik>\r\n                <Form>\r\n\r\n                    <div className=\"block block-themed\">\r\n                        <div className=\"col-md-12\">\r\n\r\n                            <div className=\"bloc