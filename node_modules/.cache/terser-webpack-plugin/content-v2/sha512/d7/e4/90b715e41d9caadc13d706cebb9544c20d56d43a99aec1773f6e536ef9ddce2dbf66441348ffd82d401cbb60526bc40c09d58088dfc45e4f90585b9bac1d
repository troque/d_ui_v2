n        } catch (error) {\r\n            console.error(error);\r\n        }\r\n    };\r\n\r\n    function downloadBase64File(contentType, base64Data, fileName) {\r\n\r\n        // Se inicializan las constantes del documento en base 64\r\n        const linkSource = `data:${contentType};base64,${base64Data}`;\r\n        const downloadLink = document.createElement(\"a\");\r\n\r\n        // Se crear el open file en una new tab\r\n        downloadLink.href = linkSource;\r\n        downloadLink.download = fileName;\r\n        downloadLink.click();\r\n\r\n        // Se quita el cargando\r\n        window.showSpinner(false);\r\n    }\r\n\r\n    const redirectToRoutes = () => {\r\n        return <Navigate to={`/ActuacionesLista/`} state={{ from: from, selected_id_etapa: selected_id_etapa }} />;\r\n    }\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n            window.showSpinner(true);\r\n            setEstadoLista(\"Activos\");\r\n            nombreProceso();\r\n            cargarUsuarioComisionado();\r\n            cargarFuncionarioAsignado();\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n\r\n\r\n    const nombreProceso = () => {\r\n        GenericApi.getByIdGeneric(\"nombre-proceso\", from.procesoDisciplinarioId).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setNombreProceso(datos.data.attributes.nombre);\r\n                    validarPermisosCrearActuaciones();\r\n                    cargarActuaciones(1, paganationPerPages, global.Constants.ESTADOS.ACTIVO);\r\n                    obtenerDatosDeLaEtapaSeleccionada();\r\n                }\r\n            }\r\n        )\r\n    }\r\n    const cargarFuncionarioAsignado = () => {\r\n        GenericApi.getByIdGeneric(\"proceso-diciplinario\", procesoDisciplinarioId).then(\r\n\r\n            // Se inicializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se valida que haya una dependencia duea del proceso\r\n                    if (datos.data.attributes.dependencia_duena != null) {\r\n\r\n                        // Se consume la API\r\n                        //GenericApi.getByIdGeneric(\"usuario\", datos.data.attributes.dependencia_duena.id_usuario_jefe).then(\r\n                        GenericApi.getByIdGeneric(\"usuario\", datos.data.attributes.dependencia_actual.id_usuario_jefe).then(\r\n\r\n                            // Se inicializa la variable de respuesta\r\n                            datos => {\r\n\r\n                                // Se valida que no haya error\r\n                                if (!datos.error) {\r\n\r\n                                    // Se setea la informacion\r\n                                    setFuncionarioAsignado(datos.data.attributes.email);\r\n                                }\r\n                            }\r\n                        )\r\n                    } else {\r\n                        window.showSpinner(false);\r\n                    }\r\n                } else {\r\n                    window.showSpinner(false);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const cargarUsuarioComisionado = (page, perPage, estado) => {\r\n        GenericApi.getGeneric('actuaciones/get-usuario-comisionado/' + procesoDisciplinarioId).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    console.log(\"USuario COmisionado\");\r\n                    console.log(datos);\r\n                    setUsuarioComisionado(datos);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encargado de cargar las actuaciones por estado activo o inactivo\r\n    const cargarActuaciones = (page, perPage, estado) => {\r\n\r\n        // Se activa el cargando\r\n        window.showSpinner(true);\r\n\r\n        // Se consume la API\r\n        GenericApi.getGeneric('actuaciones/get-actuaciones-discipl-etapa-documento-final/' + procesoDisciplinarioId + '/' + selected_id_etapa + '/' + estado).then(\r\n\r\n            // Se inicializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n                    \r\n                    // Se setean los datos\r\n                    setActuacionesListaSearch(datos)\r\n\r\n                    // Se valida que haya data\r\n                    if (datos[\"data\"] != \"\") {\r\n\r\n                        // Se obtiene los datos de la ultima actuacion y se setea\r\n                        obtenerLaUltimaActuacion(datos.data[0].attributes.actuacion.attributes.id_estado_actuacion, datos.data[0].attributes.actuacion.attributes.id_actuacion);\r\n                    } else {\r\n                        // Se realiza la validacion para el boton de agregar con usuario comisionado\r\n                        validacionDeBotonAgregar(0, 0, 0);\r\n                    }\r\n                } else {\r\n\r\n                    // Se setea el error\r\n                    setErrorApi(datos.error.toString());\r\n\r\n                    // Se muestra el modal\r\n                    window.showModal(1);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const obtenerLaUltimaActuacion = (estado, id) => {\r\n        GenericApi.getByIdGeneric('mas_actuaciones', id).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    if (datos[\"data\"] != \"\") {\r\n                        validacionDeBotonAgregar(datos.data.attributes.tipo_actuacion, estado, datos.data.attributes.excluyente);\r\n                    }\r\n                } else {\r\n                    window.showModal(1);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const obtenerDatosDeLaEtapaSeleccionada = () => {\r\n        GenericApi.getGeneric('mas-etapa/' + selected_id_etapa).then(\r\n         