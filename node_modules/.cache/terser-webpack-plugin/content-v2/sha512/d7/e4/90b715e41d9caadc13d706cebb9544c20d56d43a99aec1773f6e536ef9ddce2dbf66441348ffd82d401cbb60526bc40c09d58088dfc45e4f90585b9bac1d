           } else {\r\n                        setMostrarBotonAgregar(false);\r\n                    }\r\n                } else if (tipo_actuacion == 1) {\r\n                    //if (datos == \"Si tiene\" || (hasAccess('SeleccionarDependenciaDuenaDelProceso', 'Gestionar'))) {\r\n                    if (datos == \"Si tiene\") {\r\n                        setMostrarBotonAgregar(true);\r\n                    } else {\r\n                        setMostrarBotonAgregar(false);\r\n                    }\r\n\r\n                    // Se valida cuando el tipo es comisorio\r\n                } else if (tipo_actuacion == 2) {\r\n                    // Se valida que la etapa actual sea igual a la etapa seleccionada y que no sea excluyente y que sea diferente de pendiente de aprobacion\r\n                    if (from.idEtapa == selected_id_etapa && excluyente != 1 && \r\n                        (\r\n                            estado != 1  && \r\n                            estado != 3  && \r\n                            estado != 6  && \r\n                            estado != 9  && \r\n                            estado != 11 &&\r\n                            estado != 12\r\n                        )\r\n                    ) {\r\n                        // Se habilita el boton de agregado\r\n                        setMostrarBotonAgregar(true);\r\n\r\n                        // Se valida que el estado rechazado\r\n                        if (estado == 2) {\r\n                            // Se setea la ruta en actuacion \r\n                            setRutaParametrizada(\"Comisorio\");\r\n                        } else {\r\n                            // Se setea la ruta en comisorio\r\n                            setRutaParametrizada(\"Actuacin\");\r\n                        }\r\n                    } else {\r\n                        // Se deshabilita el boton de agregado\r\n                        setMostrarBotonAgregar(false);\r\n                    }\r\n\r\n            