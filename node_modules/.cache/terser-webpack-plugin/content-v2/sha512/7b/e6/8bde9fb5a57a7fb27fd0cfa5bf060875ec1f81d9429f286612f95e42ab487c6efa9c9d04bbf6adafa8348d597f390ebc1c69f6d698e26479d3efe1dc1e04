plinarioId = from.procesoDisciplinarioId;\r\n    let vigencia = from.vigencia;\r\n\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n\r\n            window.showSpinner(true);\r\n\r\n            // VALIDA SI ES JEFE\r\n            jefeDependencia();\r\n            listaFasesHabilitadas();\r\n            etapaActual();\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n\r\n    /**\r\n     * Se consulta si el usuario que se encuentra en sesión es jefe de la dependencia\r\n     */\r\n    const jefeDependencia = () => {\r\n\r\n        const data = {\r\n            \"data\": {\r\n                \"type\": \"usuario\",\r\n                \"attributes\": {\r\n                    \"\": \"\"\r\n                }\r\n            }\r\n        }\r\n\r\n        GenericApi.getByDataGeneric(\"usuario/get-jefe-dependencia\", data).then(\r\n\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setRtaJefe(true)\r\n                }\r\n                else {\r\n                    setRtaJefe(false);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const etapaActual = () => {\r\n\r\n        GenericApi.getGeneric(\"proceso-diciplinario/\" + procesoDisciplinarioId).then(\r\n\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n\r\n                    from.idEtapa = datos.data.attributes.etapa.id;\r\n                    setEtapa(datos.data.attributes.etapa.id);\r\n                }\r\n                else {\r\n\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n\r\n    const listaFasesHabilitadas = () => {\r\n\r\n        GenericApi.getGeneric(\"proceso-diciplinario/get-fases-registradas/\" + procesoDisciplinarioId).then(\r\n\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    setListaFasesHabilitadas(datos);\r\n                    setFase(true);\r\n                    setRespuestaAntecedente(true);\r\n                    window.showSpinner(false);\r\n                }\r\n                else {\r\n\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    //Funciones\r\n    const handleClicSoporteRadicado = (id_etapa, id_fase, es_soporte) => {\r\n        from.id_etapa = id_etapa;\r\n        from.id_fase = id_fase;\r\n        changeValueEsFuncion(es_soporte);\r\n    };\r\n\r\n    function changeValueEsFuncion(es_funcion) {\r\n        from.es_soporte = es_funcion;\r\n    }\r\n\r\n    const cerrarEtapa = (datos) => {\r\n\r\n        let data\r\n\r\n        data = {\r\n            \"data\": {\r\n                \"type\": \"cerrar_etapa\",\r\n                \"attributes\": {\r\n                    \"id_proceso_disciplinario\": procesoDisciplinarioId,\r\n                    \"id_etapa\": datos.inputEtapa,\r\n                    \"descripcion\": \"Cierre de etapa\",\r\n                    \"created_user\": getUser().nombre,\r\n                }\r\n            }\r\n        }\r\n\r\n\r\n        GenericApi.addGeneric('cierre-etapa', data).then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    setModalState({ title: \"Cierre de etapa\", message: \"El proceso disciplinario se asigna a: \" + datos.data.attributes.funcionario_asignado.nombre + \" \" + datos.data.attributes.funcionario_asignado.apellido, show: true, redirect: '/MisPendientes', from: { from }, alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                }\r\n                else {\r\n                    setModalState({ title: \"Cierre de etapa\", message: datos.error.toString(), show: true, redirect: '/RamasProceso', from: { from }, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const componentAntecedente = () => {\r\n\r\n        return (\r\n            <>\r\n\r\n                <div className=\"col-lg-12 push\">\r\n                    <div className=\"alert alert-info\" role=\"alert\">\r\n                        <h3 className=\"text-center\">(RADICADO) :: {radicado}</h3>\r\n                        <span><strong>Antecedente: </strong></span><br />\r\n                        <span title={(from.antecedente) ? from.antecedente : \"\"}>{from.antecedente ? from.antecedente.substring(0, 300) : \"\"}...</span><br />\r\n\r\n                        <br />\r\n                        <div className=\"row\">\r\n                            <div className=\"col-lg-6\"><span><strong>Registrado Por: </strong>{from.registradoPor ? from.registradoPor : \"Sin Información\"}</span></div>\r\n                            <hr />\r\n                            <div className=\"col-lg-6\"><span><strong>Dependencia registro: </strong> {from.dependendencia ? from.dependendencia : \"Sin Información\"}</span></div>\r\n                            <hr />\r\n                        </div>\r\n\r\n                        <div className=\"row\">\r\n\r\n                            <div className=\"col-lg-4\"><span><strong>Vigencia: </strong>{vigencia}</span></div>\r\n                            <div className=\"col-lg-4\"><span><strong>Fecha de registro: </strong> {moment(from.fechaRegistro, 'DD/MM/YYYY, h:mm:ss a').format(\"DD/MM/YYYY, h:mm:ss a\")}</span></div>\r\n                            <div className=\"col-lg-4\"><span><strong>Fecha de ingreso: </strong> {moment(from.fechaIngreso, 'YYYY/MM/DD').format(\"DD/MM/YYYY\")}</span></div>\r\n\r\n\r\n                        </div>\r\n\r\n                    </div>\r\n                </div>\r\n            </>\r\n        )\r\n    }\r\n\r\n\r\n    const cargarFase = (id_etapa, id_fase, es_soporte, redirect) => {\r\n\r\n        for (var i = 0; i < getListaFasesHabilitadas.data.length; i++) {\r\n\r\n            if (getListaFasesHabilitadas.data[i].id == id_fase && getListaFasesHabilitadas.data[i].attributes.visible) {\r\n\r\n                if (getListaFasesHabilitadas.data[i].attributes.estado) {\r\n                    return (\r\n                        <>\r\n                            <div className='w2d-form-group'>\r\n                                <Link to={redirect} state={{ from: from }}>\r\n                                    <button type=\"button\" className={colorFase(getListaFasesHabilitadas.data[i].attributes.semaforizacion)} onClick={() => changeValueEsFuncion(false)}><i className=\"fas fa-clipboard-list\"></i> {getListaFasesHabilitadas.data[i].attributes.nombre}</button>\r\n                                </Link>\r\n\r\n                                {getListaFasesHabilitadas.data[i].attributes.etapa == global.Constants.ETAPAS.CAPTURA_REPARTO ? (\r\n                                    <Link to={`/SoporteradicadoForm/`} state={{ from: from }}>\r\n                                        <button type=\"button\" className=\"btn btn-sm btn-primary mr-1 mb-3\" onClick={() => handleClicSoporteRadicado(id_etapa, id_fase, es_soporte)}><i className=\"fas fa-folder-open\"></i></button>\r\n                                    </Link>\r\n                                ) : null\r\n                                }\r\n                            </div>\r\n                        </>\r\n                    )\r\n                }\r\n\r\n                else {\r\n                    return (\r\n                        <>\r\n                            <div className='w2d-form-group'>\r\n                                <Link to={redirect} state={{ from: from }}>\r\n                                    <button type=\"button\" className={global.Constants.BOTONES_RAMAS.PRIMARY} onClick={() => changeValueEsFuncion(false)}><i className=\"fas fa-clipboard-list\"></i> {getListaFasesHabilitadas.data[i].attributes.nombre}</button>\r\n                                </Link>\r\n\r\n                                {getListaFasesHabilitadas.data[i].attributes.etapa == global.Constants.ETAPAS.CAPTURA_REPARTO ? (\r\n                                    <Link to={`/SoporteradicadoForm/`} state={{ from: from }}>\r\n                                        <button type=\"button\" className=\"btn btn-sm btn-primary mr-1 mb-3\" onClick={() => handleClicSoporteRadicado(id_etapa, id_fase, es_soporte)}><i className=\"fas fa-folder-open\"></i></button>\r\n                                    </Link>\r\n                                ) : null\r\n                                }\r\n\r\n                            </div>\r\n                        </>\r\n                    )\r\n                }\r\n            }\r\n\r\n        }\r\n\r\n    }\r\n\r\n    const colorFase = (estado_fase) => {\r\n\r\n        if (estado_fase === 1)\r\n            return (global.Constants.BOTONES_RAMAS.PRIMARY)\r\n        else if (estado_fase === 2)\r\n            return (global.Constants.BOTONES_RAMAS.WARNING)\r\n        else\r\n            return (global.Constants.BOTONES_RAMAS.SUCCESS)\r\n    }\r\n\r\n\r\n\r\n\r\n    const cargarSoportesRadicado = (id_etapa, id_fase, es_soporte, redirect) => {\r\n\r\n        for (var i = 0; i < getListaFasesHabilitadas.data.length; i++) {\r\n\r\n            if (getListaFasesHabilitadas.data[i].id == id_fase && getListaFasesHabilitadas.data[i].attributes.estado) {\r\n                return (\r\n                    <div className='w2d-form-group'>\r\n                        <Link to={redirect} state={{ from: from }}>\r\n                            <button type=\"button\" className={global.Constants.BOTONES_RAMAS.SUCCESS} onClick={() => handleClicSoporteRadicado(id_etapa, id_fase, es_soporte)}><i className=\"fas fa-archive\"></i> {getListaFasesHabilitadas.data[i].attributes.nombre}</button>\r\n                        </Link>\r\n                    </div>\r\n                )\r\n            }\r\n            else if (getListaFasesHabilitadas.data[i].id == id_fase && !getListaFasesHabilitadas.data[i].attributes.estado) {\r\n                return (\r\n                    <div className='w2d-form-group'>\r\n                        <Link to={redirect} state={{ from: from }}>\r\n                            <button type=\"button\" className={global.Constants.BOTONES_RAMAS.PRIMARY} onClick={() => handleClicSoporteRadicado(id_etapa, id_fase, es_soporte)}><i className=\"fas fa-archive\"></i> {getListaFasesHabilitadas.data[i].attributes.nombre}</button>\r\n                        </Link>\r\n                    </div>\r\n                )\r\n            }\r\n        }\r\n    }\r\n\r\n\r\n    const cargarCierreEtapa = (id_etapa, id_fase_lista_para_cierre, id_fase_cerrada) => {\r\n\r\n\r\n        for (var i = 0; i < getListaFasesHabilitadas.data.length; i++) {\r\n\r\n            // valida cierre de captura y reparto\r\n            if (getListaFasesHabilitadas.data[i].id == id_fase_lista_para_cierre) {\r\n\r\n                console.log(\"ID_ETAPA: \" + id_etapa + \" ID_FASE_LISTA_PARA_CIERRE: \" + id_fase_lista_para_cierre + \" ID_FASE_CERRADA: \" + id_fase_cerrada)\r\n                return (\r\n                    <div className=\"form-group\">\r\n\r\n                        <Formik\r\n                            initialValues={{\r\n                                inputCierreEtapa: '',\r\n                                inputEtapa: id_etapa,\r\n                            }}\r\n                            enableReinitialize\r\n                            validate={(valores) => {\r\n\r\n                                let errores = {}\r\n                                if (!valores.inputCierreEtapa) {\r\n                                    errores.inputCierreEtapa = 'Debe seleccionar la opción cierre de etapa'\r\n                                }\r\n                                return errores\r\n                            }}\r\n\r\n                            onSubmit={(valores, { resetForm }) => {\r\n                                cerrarEtapa(valores);\r\n                            }}\r\n                        >\r\n\r\n                            <Form>\r\n                                <div className=\"col-md-12\">\r\n                                    <Field type=\"checkbox\" className=\"form-check-input\" id=\"inputCierreEtapa\" name=\"inputCierreEtapa\" />\r\n                                    <label className=\"form-check-label\" htmlFor=\"example-checkbox-inline1\">¿Con la información actual se puede dar cierre a esta etapa?</label>\r\n                                </div>\r\n\r\n                                <div className=\"col-md-12\">\r\n                                    <>\r\n                                        {\r\n                                            (hasAccess('CR_CierreEtapa', 'Crear')) ? (\r\n                                                <div className=\"block-content block-content-full text-center\">\r\n                                                    <button type=\"submit\" className=\"btn btn-danger\"> Cerrar Etapa</button>\r\n                                                </div>\r\n                                            ) : null\r\n                                        }\r\n                                    </>\r\n                                </div>\r\n                            </Form>\r\n                        </Formik>\r\n                    </div>\r\n                )\r\n            }\r\n\r\n            // valida cierre de captura y reparto\r\n            if (getListaFasesHabilitadas.data[i].id == id_fase_cerrada && getListaFasesHabilitadas.data[i].attributes.estado) {\r\n\r\n                return (\r\n                    <div className=\"col-md-12 text-center\">\r\n                        <button type='button' title='Consultar cierre de etapa' className='btn btn-sm'\r\n                            data-toggle=\"modal\" data-target={'#modal-consultar-detalle-'+id_etapa}> <i className=\"fas fa-clipboard-list fa-2x text-info \"></i></button>\r\n                    </div>\r\n                )\r\n            }\r\n        }\r\n    }\r\n\r\n\r\n\r\n    return (\r\n        <>\r\n            {<Spinner />}\r\n            {<ModalCierreEtapaInfo object={1} />}\r\n            {<ModalGen data={getModalState} />}\r\n\r\n            <div className=\"row\">\r\n\r\n                <div className=\"col-md-12\">\r\n                    <div className=\"w2d_block\">\r\n                        <nav aria-label=\"breadcrumb\">\r\n                            <ol className=\"breadcrumb breadcrumb-alt push\">\r\n\r\n                                <li className=\"breadcrumb-item\">\r\n                                    <Link underline=\"hover\" className=\"text-dark\" to={`/MisPendientes`}><small>Mis Pendientes</small></Link></li>\r\n                                <li className=\"breadcrumb-item\"><small>Ramas del proceso</small></li>\r\n                            </ol>\r\n                        </nav>\r\n\r\n                    </div>\r\n                </div>\r\n                {getRespuestaAntecedente ? componentAntecedente() : null}\r\n\r\n                {getRtaJefe ? (\r\n\r\n                    <div className=\"col-md-12 w2d-enter\">\r\n\r\n                        <div id=\"horizontal-navigation-hover-normal\" class=\"d-none d-lg-block mt-2 mt-lg-0\">\r\n                            <ul class=\"nav-main nav-main-horizontal nav-main-hover\">\r\n\r\n                                <li class=\"nav-main-item\">\r\n                                    <a class=\"nav-main-link nav-main-link-submenu\" data-toggle=\"submenu\" aria-haspopup=\"true\" aria-expanded=\"false\" href=\"#\">\r\n                                        <span class=\"nav-main-link-name\"><i class=\"fas fa-tools\"></i> Herramientas</span>\r\n                                    </a>\r\n                                    <ul class=\"nav-main-submenu\">\r\n                                        <li class=\"nav-main-item\">\r\n                                            <Link className=\"nav-main-link\" to=\"/ValidarClasificacionRadicadoForm\" state={{ from: from }}>\r\n                                                <span class=\"nav-main-link-name\">Reclasificación del expediente</span>\r\n                                            </Link>\r\n                                        </li>\r\n                                        <li class=\"nav-main-item\">\r\n                                            <Link className=\"nav-main-link\" to=\"/TipoConductaForm\" state={{ from: from }}>\r\n                                                <span class=\"nav-main-link-name\">Tipo de conducta</span>\r\n                                            </Link>\r\n                                        </li>\r\n                                        <li class=\"nav-main-item\">\r\n                                            <Link className=\"nav-main-link\" to=\"/Transacciones\" state={{ from: from }}>\r\n                                                <span class=\"nav-main-link-name\">Transacciones</span>\r\n                                            </Link>\r\n                                        </li>\r\n                                    </ul>\r\n                                </li>\r\n                            </ul>\r\n                        </div>\r\n                    </div>\r\n                ) : null\r\n                }\r\n\r\n                {/*CAPTURA Y REPARTO*/}\r\n                <div className=\"col-lg-4 col-md-6 col-sm-6\">\r\n                    <div className=\"block block-themed\">\r\n\r\n\r\n                        <div className=\"block-header block-header-default\">\r\n                            <h3 className=\"block-title\">Captura y Reparto</h3>\r\n                            <Link to={`/SoporteRadicadoLista/`} state={{ from: from }}>\r\n                                <button type=\"button\" className=\"btn btn-sm\" style={{ color: 'white' }} onClick={() => handleClicSoporteRadicado(1, 1, true)}><i className=\"fas fa-folder-open fa-2x\"></i></button>\r\n                            </Link>\r\n                        </div>\r\n                        <>\r\n                            {\r\n                               