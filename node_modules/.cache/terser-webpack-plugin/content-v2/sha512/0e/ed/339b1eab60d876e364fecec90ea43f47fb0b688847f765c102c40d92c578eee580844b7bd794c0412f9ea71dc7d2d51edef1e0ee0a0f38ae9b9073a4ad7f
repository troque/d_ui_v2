nt={() => (<span className=\"text-danger\">{errors.descripcion}</span>)} />\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                            <div className=\"block-content block-content-full text-right bg-light\">\r\n                                <button type=\"submit\" className=\"btn btn-primary\"><i className=\"fas fa-save\"></i> Registrar</button>\r\n\r\n                                <Link to={`/DatosInteresadoLista/`} state={{ from: from }}>\r\n                                    <button type=\"button\" className=\"btn btn-danger\"><i class=\"fas fa-window-close\"></i> Cancelar</button>\r\n                                </Link>\r\n                            </div>\r\n                        </div>\r\n\r\n                    </Form>\r\n                )}\r\n            </Formik>\r\n        </>\r\n    );\r\n}\r\n\r\n\r\nexport default DatosInteresadoCambiarEstadoForm;","\r\nexport default global.Constants = {\r\n    ESTADOS: {\r\n        ACTIVO: '1',\r\n        INACTIVO: '0'\r\n    },\r\n\r\n    //MIS PENDIENTES\r\n    RANGOS: {\r\n        TODOS: '-1'\r\n    },\r\n\r\n    BOTONES_RAMAS: {\r\n        PRIMARY: 'btn btn-sm btn-primary w2d_btn-large mr-1 mb-3 text-left',\r\n        SUCCESS: 'btn btn-sm btn-success w2d_btn-large mr-1 mb-3 text-left',\r\n        WARNING: 'btn btn-sm btn-warning w2d_btn-large mr-1 mb-3 text-left'\r\n    },\r\n\r\n    BOTONES_CARPETAS: {\r\n        PRIMARY: 'btn btn-sm btn-primary mr-1 mb-3',\r\n        SUCCESS: 'btn btn-sm btn-success mr-1 mb-3',\r\n    },\r\n\r\n    COLOR_FASE: {\r\n        COMPLETA: '#8BCD50'\r\n    },\r\n\r\n    //PARA LOS TEXT_AREA\r\n    TEXT_AREA: {\r\n        CARACTERES_ESPECIALES: `\\`^_\\[\\]{}'\\\\|<>\\/~`,\r\n        //CARACTERES_ESPECIALES: `\\`!@#$%^&*()_+\\-=\\[\\]{};':\"\\\\|<>\\/?~`,\r\n        CANTIDAD_MINIMA_DESCRIPCION: 100 //Cantidad mínima que se muestra en las descripciones\r\n    },\r\n\r\n    //CARAC