a: [] });\r\n    const [getSeach, setSeach] = useState('');\r\n    const paganationPerPages = global.Constants.DATA_TABLE.PAGINATION_PER_PAGE;\r\n    const [perPage, setPerPage] = useState(global.Constants.DATA_TABLE.PAGINATION_PER_PAGE);\r\n    const [pageActual, setPageActual] = useState(1);\r\n    const [getEstadoLista, setEstadoLista] = useState('1');\r\n\r\n\r\n\r\n    const [getRoutes, setRoutes] = useState({\r\n\r\n        crear_registro: \"/Usuario/Add\",\r\n        consultar_registros: \"/Usuario\",\r\n        muestra_atras: false,\r\n        id_etapa: global.Constants.ETAPAS.NINGUNA,\r\n    });\r\n\r\n\r\n    const handleCallback = (childData) => {\r\n        try {\r\n       \r\n            setEstadoLista(childData)\r\n        } catch (error) {\r\n\r\n        }\r\n\r\n    }\r\n\r\n    const columns = [\r\n        {\r\n            name: 'Nombre',\r\n            cell: usuario => <div>{(usuario.attributes.nombre ? usuario.attributes.nombre : \"\") + ' ' + (usuario.attributes.apellido ? usuario.attributes.apellido : \"\")}</div>,\r\n            selector: usuario => usuario.attributes.nombre,\r\n            sortable: true,\r\n            width: \"15%\"\r\n        },\r\n        {\r\n            name: 'Usuario',\r\n            selector: usuario => usuario.attributes.name,\r\n            sortable: true,\r\n            width: \"15%\"\r\n        },\r\n        {\r\n            name: 'Correo',\r\n            selector: usuario => usuario.attributes.email,\r\n            sortable: true,\r\n            width: \"20%\"\r\n        },\r\n        {\r\n            name: 'Dependencia',\r\n            selector: usuario => (usuario.attributes.dependencia ? usuario.attributes.dependencia.nombre : ''),\r\n            sortable: true,\r\n            width: \"20%\"\r\n\r\n        },\r\n        {\r\n            name: 'Estado',\r\n            selector: usuario => (usuario.attributes.estado == \"1\" ? 'Activo' : 'Inactivo'),\r\n            sortable: true,\r\n            width: \"10%\"\r\n        },\r\n        {\r\n            name: 'Acciones',\r\n\r\n            cell: row => <div><Link to={`${row.id}`}>\r\n                <button type=\"button\" className=\"btn btn btn-primary\" title='Editar'>\r\n                    <i className=\"fa fa-fw fa-edit\"></i>\r\n                </button>\r\n            </Link></div>\r\n\r\n\r\n        }\r\n    ];\r\n\r\n    const redirectToRoutes = () => {\r\n        return <Navigate to={`/UsuarioLista/`} />;\r\n    }\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n            window.showSpinner(true);\r\n            cargarUsuarios(1, paganationPerPages);\r\n\r\n\r\n\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n    const cargarUsuarios = (page, perPage) => {\r\n        //cargamos todos los usuarios\r\n        //GenericApi.getAllGeneric('usuario/usuario-paginate' + '/' + page + '/' + perPage).then(\r\n        GenericApi.getAllGeneric('usuario-sin-estado').then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    if (datos[\"data\"] != \"\") {\r\n                        setUsuarioListaSearch(datos)\r\n                        window.showSpinner(false);\r\n                    }\r\n                    else {\r\n                        setErrorApi(datos.error.toString())\r\n                        window.showModal(1)\r\n                        window.showSpinner(false);\r\n\r\n                    }\r\n                }\r\n                else {\r\n                    setErrorApi(datos.error.toString())\r\n                    window.showModal(1)\r\n                    window.showSpinner(false);\r\n                }\r\n\r\n            }\r\n        )\r\n    }\r\n\r\n    const handlePageChange = page => {\r\n\r\n        //window.showSpinner(true);\r\n        setPageActual(page);\r\n        //cargarUsuarios(page, perPage);\r\n    }\r\n\r\n    const handlePerRowsChange = async (newPerPage, page) => {\r\n        //window.showSpinner(true);\r\n        setPerPage(newPerPage);\r\n        setPageActual(page);\r\n        //cargarUsuarios(page, newPerPage);\r\n\r\n    }\r\n\r\n    return (\r\n        <>\r\n\r\n\r\n\r\n\r\n            {isRedirect ? redirectToRoutes() : null}\r\n            {<Spinner />}\r\n            {<InfoErrorApi error={errorApi} />}\r\n            {<InfoExitoApi />}\r\n            <Formik>\r\n                <Form>\r\n\r\n                    <div className=\"block block-themed\">\r\n                        <div className=\"col-md-12\">\r\n\r\n                            <div className=\"block-content\">\r\n                                <nav aria-label=\"breadcrumb\">\r\n                                    <ol className=\"breadcrumb breadcrumb-alt push\">\r\n                                        <li class=\"breadcrumb-item\"> <small>Lista de Usuarios</small></li>\r\n                                    </ol>\r\n                                </nav>\r\n                            </div>\r\n\r\n                        </div>\r\n                        <div className=\"block-header\">\r\n                            <h3 className=\"block-title\">Usuario registrados {getEstadoLista == global.Constants.ESTADOS.INACTIVO ? \"Inactivos\" : \"Activos\"}</h3>\r\n\r\n                        </div>\r\n\r\n                        <div className=\"block-content\">\r\n\r\n                            <div className='row'>\r\n\r\n                                <div className='col-md-3'>\r\n                                    <div className=\"form-group \">\r\n                                        <Field type=\"text\" id=\"search\" name=\"search\" value={getSeach} onChange={e => setSeach(e.target.value)} onKeyPress={(e) => { e.key === 'Enter' && e.preventDefault(); }} className=\"form-control border border-success\" placeholder=\"Buscar\" />\r\n\r\n                                    </div>\r\n                                </div>\r\n\r\n                                <ListaBotones getRoutes={getRoutes} parentCallback={handleCallback} id=\"botonesNavegacion\" name=\"botonesNavegacion\" />\r\n                            </div>\r\n\r\n                            <div className=\"row\">\r\n                                <div className=\"col-sm-12\">\r\n\r\n                                    <DataTable className=\"table table-bordered table-striped table-vcenter js-dataTable-full\"\r\n                                        columns={columns}\r\n\r\n                                        data={getUsuarioListaSearch.data.filter((suggestion) => {\r\n                                            if (getSeach === \"\") {\r\n                                                if(suggestion.attributes.estado == getEstadoLista){\r\n                                                    return suggestion;\r\n                                                }\r\n                                            } else if (\r\n\r\n                                                ((suggestion.attributes.nombre\r\n                                                    + suggestion.attributes.apellido\r\n                                                    + suggestion.attributes.name\r\n                                                    + suggestion.attributes.email\r\n                                                    + (suggestion.attributes.dependencia ? suggestion.attributes.dependencia.nombre : '')\r\n                                                    + (suggestion.attributes.estado == '0' ? 'Inactivo' : 'Activo')).toLowerCase().includes(getSeach.toLowerCase())\r\n                                                    && (suggestion.attributes.estado == getEstadoLista))\r\n\r\n                                            ) {\r\n                                                return suggestion;\r\n                                            }\r\n                                        })}\r\n                                        perPage={perPage}\r\n                                        page={pageActu