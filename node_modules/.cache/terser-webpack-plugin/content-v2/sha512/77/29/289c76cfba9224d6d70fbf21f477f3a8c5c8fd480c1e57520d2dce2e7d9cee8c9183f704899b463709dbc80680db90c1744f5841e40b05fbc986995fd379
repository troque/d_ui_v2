(\"\");\r\n    const [getValorNombreQuejoso , setValorNombreQuejoso] = useState(\"\");\r\n    const [getValorIdentificacionQuejoso , setValorIdentificacionQuejoso] = useState(\"\");\r\n    const [getValorTipoQuejoso , setValorTipoQuejoso] = useState(\"\");\r\n    const [getValorEtapaDelExpediente , setValorEtapaDelExpediente] = useState(\"\");\r\n    const [getValorDelegada , setValorDelegada] = useState(\"\");\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n            window.showSpinner(true);\r\n            setNombreUsuario(getUser().nombre);\r\n            cargarPendientes(1, paganationPerPages);\r\n\r\n\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n    const enviarDatos = () =>{\r\n\r\n        let data = {\r\n            \"data\":{\r\n                \"type\":\"buscador\",\r\n                \"attributes\": {\r\n                    \"n_expediente\": getValorExpediente,\r\n                    \"Vigencia\": getValorVigencia,\r\n                    \"estado_del_expediente\": getValorEstadoDelExpediente,\r\n                    \"ubicacion_del_expediente\": getValorUbicacionDelExpediente,\r\n                    \"nombre_disciplinado\": getValorNombreDisciplinado,\r\n                    \"identificacion_disciplinado\": getValorIdentificacionDisciplinado,\r\n                    \"asunto_del_expediente\": getValorAsuntoDelExpediente,\r\n                    \"sector\": getValorSector,\r\n                    \"nombre_entidad\": getValorNombreEntidad,\r\n                    \"nombre_quejoso\": getValorNombreQuejoso,\r\n                    \"identificacion_quejoso\": getValorIdentificacionQuejoso,\r\n                    \"tipo_quejoso\": getValorTipoQuejoso,\r\n                    \"etapa_del_expediente\": getValorEtapaDelExpediente,\r\n                    \"delegada\": getValorDelegada\r\n                }\r\n            }\r\n        }\r\n\r\n        GenericApi.getByDataGeneric('buscador-general', data).then(\r\n            datos => {\