ADA","SOLICITUD_ACTIVACION_RECHAZADA","ESTADO_ACTUACION","TIPO_ARCHIVO_ACTUACIONES","DOCUMENTO_INICIAL","DOCUMENTO_DEFINITIVO","VISIBILIDAD","VISIBLE_TODOS","VISIBLE_DEPENDENCIA","VISIBLE_PARA_MI_Y_JEFE","OCULTO_TODOS","SEMAFORO_EVENTOS","EVENTO_ANTECEDENTE","EVENTO_APROBACION_ACTUACION","EVENTO_APROBACION_ACTUACION_FECHA","EVENTO_SECRETARIA_COMUN","EVENTO_DEPENDENCIA","EVENTO_INTERESADO_DEPENDENCIA","MENSAJE_TIPO_INFORMATIVO_PARA_APROBAR","ETAPA","FIRMA","ANULACION","MENSAJE_INFORMATIVO_PARA_APROBAR","relativeUrl","getBearerToken","handleError","serviceName","updateGeneric","put","Spinner","style1","mensajeCargando","ModalGen","navigate","useNavigate","setShow","handleClose","getAlerta","id_alerta","Array","isArray","dato"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;AAcA,QAASA,aAAY,EAAG,2RAEtB,cAA2DC,sDAAQ,CAAC,KAAK,CAAC,wKAAnEC,uBAAuB,eAAEC,uBAAuB,eACvD,eAAiCF,sDAAQ,CAAC,KAAK,CAAC,yKAAzCG,UAAU,eAAEC,UAAU,eAC7B,eAA6DJ,sDAAQ,CAAC,CAAEK,IAAI,CAAE,EAAG,CAAC,CAAC,yKAA5EC,wBAAwB,eAAEC,wBAAwB,eACzD,eAAuCP,sDAAQ,CAAC,CAAEQ,KAAK,CAAE,EAAE,CAAEC,OAAO,CAAE,EAAE,CAAEC,IAAI,CAAE,KAAM,CAAC,CAAC,yKAAjFC,aAAa,eAAEC,aAAa,eACnC,eAA2BZ,sDAAQ,CAAC,KAAK,CAAC,0KAAnCa,OAAO,gBAAEC,OAAO,gBACvB,gBAA6Bd,sDAAQ,CAAC,CAAC,CAAC,2KAAjCe,QAAQ,gBAAEC,QAAQ,gBACzB,gBAAuDhB,sDAAQ,CAAC,EAAE,CAAC,2KAA5DiB,qBAAqB,gBAAEC,qBAAqB,gBACnD,gBAAmElB,sDAAQ,CAAC,CAAEK,IAAI,CAAE,CAAC,CAAE,CAAC,CAAC,2KAAlFc,2BAA2B,gBAAEC,2BAA2B,gBAC/D,gBAAmFpB,sDAAQ,CAAC,KAAK,CAAC,2KAA3FqB,mCAAmC,gBAAEC,mCAAmC,gBAC/E,gBAA2GtB,sDAAQ,CAAC,KAAK,CAAC,2KAAnHuB,+CAA+C,gBAAEC,+CAA+C,gBACvG,gBAAyCxB,sDAAQ,CAAC,KAAK,CAAC,2KAAjDyB,cAAc,gBAAEC,cAAc,gBACrC,gBAA6C1B,sDAAQ,CAAC,KAAK,CAAC,2KAArD2B,gBAAgB,gBAAEC,gBAAgB,gBACzC,gBAA+C5B,sDAAQ,CAAC,KAAK,CAAC,2KAAvD6B,iBAAiB,gBAAEC,iBAAiB,gBAC3C,gBAAyD9B,sDAAQ,CAAC,KAAK,CAAC,2KAAjE+B,sBAAsB,gBAAEC,sBAAsB,gBACrD,gBAAiEhC,sDAAQ,CAAC,KAAK,CAAC,2KAAzEiC,0BAA0B,gBAAEC,0BAA0B,gBAC7D,gBAAiElC,sDAAQ,CAAC,KAAK,CAAC,2KAAzEmC,0BAA0B,gBAAEC,0BAA0B,gBAC7D,gBAAyCpC,sDAAQ,CAAC,EAAE,CAAC,2KAA9CqC,cAAc,gBAAEC,cAAc,gBACrC,gB