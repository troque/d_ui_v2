s[\"data\"] != \"\") {\r\n                        setNombre(datos[\"data\"][\"attributes\"][\"nombre\"]);\r\n                        //console.log(JSON.stringify(datos[\"data\"][\"attributes\"][\"ids_funcionalidades\"]));\r\n                        if (datos[\"data\"][\"attributes\"][\"ids_funcionalidades\"]) {\r\n\r\n                            datos[\"data\"][\"attributes\"][\"ids_funcionalidades\"].split(',').map((funcId, i) => {\r\n                                setFuncionalidadesAsociadas(oldArray => [...oldArray, funcId]);\r\n                            });\r\n\r\n                        }\r\n\r\n\r\n                    }\r\n                    else {\r\n                        setErrorApi(datos.error.toString())\r\n                        window.showModal(1)\r\n\r\n\r\n                    }\r\n                }\r\n                else {\r\n                    setErrorApi(datos.error.toString())\r\n                    window.showModal(1)\r\n\r\n                }\r\n                window.showSpinner(false);\r\n            }\r\n        )\r\n    }\r\n\r\n\r\n\r\n\r\n    const getAllFuncionalidades = () => {\r\n        GenericApi.getAllGeneric('modulo/get-modulos').then(\r\n            datos => !datos.error ? (setListaFuncionalidades(datos), setRespuestaFuncionalidades(true),\r\n                getUserById()) : (window.showModal(1), getUserById())\r\n        )\r\n    }\r\n\r\n    const enviarDatos = () => {\r\n        window.showSpinner(true);\r\n        let data;\r\n\r\n\r\n        data = {\r\n\r\n            \"data\": {\r\n                \"type\": \"usuario\",\r\n                \"attributes\": {\r\n\r\n                    \"nombre\": getNombre ? getNombre : \"\",\r\n                    \"funcionalidades\": getFuncionalidadesAsociadas,\r\n\r\n                }\r\n            }\r\n        }\r\n\r\n\r\n        // console.log(JSON.stringify(data));\r\n\r\n        GenericApi.updateGeneric('role', id, data).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    window.showModal(2)\r\n                    setIsRedirect(true);\r\n                }\r\n                else {\r\n                    //console.log(\"hay error\");\r\n                    // console.log(datos.error);\r\n                    setErrorApi(datos.error.toString())\r\n                    window.showModal(1)\r\n                }\r\n                window.showSpinner(false);\r\n            }\r\n        )\r\n    }\r\n\r\n\r\n\r\n    const agregarCheck = (e, funcionalidadId) => {\r\n\r\n        if (e == true) {\r\n\r\n            setFuncionalidadesAsociadas(oldArray => [...oldArray, funcionalidadId]);\r\n\r\n        }\r\n        else {\r\n\r\n            // console.log(JSON.stringify(getFuncionalidadesAsociadas));\r\n            var index = getFuncionalidadesAsociadas.indexOf(funcionalidadId);\r\n            //console.log(index);\r\n            if (index !== -1) {\r\n                getFuncionalidadesAsociadas.splice(index, 1);\r\n            }\r\n        }\r\n\r\n\r\n\r\n    }\r\n\r\n\r\n\r\n    const listarFuncionalidades = () => {\r\n\r\n        let mitadLista = (getListaFuncionalidades.data.length) / 2;\r\n        return (\r\n            <div className='row'>\r\n                <div className=\"col-md-6\">\r\n                    {getListaFuncionalidades.data.map((modulo, i) => {\r\n                        return (\r\n                            (i < mitadLista) ? (\r\n                                <div key={modulo.id} id=\"accordion\" role=\"tablist\" aria-multiselectable=\"true\">\r\n                                    <div class=\"block block-rounded mb-1\">\r\n                                        <div class=\"block-header block-header-default\" role=\"tab\" id=\"accordion_h1\">\r\n                                            <a class=\"font-w600\" data-toggle=\"collapse\" data-parent=\"#accordion\" href=\"#accordion_q1\" aria-expanded=\"true\" aria-controls=\"accordion_q1\"><i class=\"fas fa-folder\" /> {modulo.attributes.nombre_mostrar} </a>\r\n                                        </div>\r\n                                        <div id=\"accordion_q1\" class=\"collapse show\" role=\"tabpanel\" aria-labelledby=\"accordion_h1\" data-parent=\"#accordion\">\r\n                                            <div className=\"block-content\">\r\n\r\n                                                {\r\n                                                    modulo.attributes.funcionalidades.map((funcionalidad, i) => {\r\n                                                        return (\r\n\r\n                                                            <div className='row' key={i}>\r\n                                                                <div className=\"col-md-2\" >\r\n                                                                    <div className=\"custom-control custom-switch custom-control-lg mb-2\">\r\n                                                                        <>\r\n                                                                            {\r\n                                                                                (getFuncionalidadesAsociadas.length > 0) ? (\r\n                                                                                    (getFuncionalidadesAsociadas.indexOf(funcionalidad.id.toString()) > -1) ? (\r\n                                                                                        <div>\r\n                                                                                            <input defaultChecked=\"true\" type=\"checkbox\" onClick={e => agregarCheck(e.target.checked, funcionalidad.id.toString())} className=\"custom-control-input\" id={funcionalidad.id} name={funcionalidad.id} />\r\n                                                                                            <label className=\"custom-control-label\" htmlFor={funcionalidad.id}></label>\r\n                                                                                        </div>\r\n                                                                                    ) :\r\n                                                                                        <div>\r\n                                                                                            <input type=\"checkbox\" onClick={e => agregarCheck(e.target.checked, funcionalidad.id.toString())} className=\"custom-control-input\" id={funcionalidad.id} name={funcionalidad.id} />\r\n                                                                                            <label className=\"custom-control-label\" htmlFor={funcionalidad.id}></label>\r\n                                                                                        </div>\r\n                                                                                ) :\r\n                                                                                    <div>\r\n                                                                                        <input type=\"checkbox\" onClick={e => agregarCheck(e.target.checked, funcionalidad.id.toString())} className=\"custom-control-input\" id={funcionalidad.id} name={funcionalidad.id} />\r\n                                                                                        <label className=\"custom-control-label\" htmlFor={funcionalidad.id}></label>\r\n                                                                                    </div>\r\n                                                                            }\r\n                                                                        </>\r\n                                                                    </div>\r\n\r\n                                                                </div>\r\n                                                                <div className=\"col-md-10\" >\r\n                                                                    {funcionalidad.nombre_mostrar}\r\n                                                                </div>\r\n                                                            </div>\r\n                                                        )\r\n                                                    })\r\n                                                }\r\n\r\n                                            </div>\r\n                                        </div>\r\n                                    </div>\r\n                                </div >\r\n\r\n                            ) : null\r\n                        )\r\n                    })\r\n                    }\r\n                </div>\r\n\r\n                <div className=\"col-md-6\">\r\n                    {getListaFuncionalidades.data.map((modulo, i) => {\r\n                        return (\r\n                            (i >= mitadLista) ? (\r\n                                <div key={modulo.id} id=\"accordion\" role=\"tablist\" aria-multiselectable=\"true\">\r\n                                    <div class=\"block block-rounded mb-1\">\r\n                                        <div class=\"block-header block-header-default\" role=\"tab\" id=\"accordion_h1\">\r\n                                            <a class=\"font-w600\" data-toggle=\"collapse\" data-parent=\"#accordion\" href=\"#accordion_q1\" aria-expanded=\"true\" aria-controls=\"accordion_q1\"><i class=\"fas fa-folder\" /> {modulo.attributes.nombre_mostrar} </a>\r\n                                        </div>\r\n                                        <div id=\"accordion_q1\" class=\"collapse show\" role=\"tabpanel\" aria-labelledby=\"accordion_h1\" data-parent=\"#accordion\">\r\n                                            <div className=\"block-content\">\r\n\r\n                                                {\r\n                                                    modulo.attributes.funcionalidades.map((funcionalidad, i) => {\r\n                                                        return (\r\n\r\n                                                            <div className='row' key={i}>\r\n                                                                <div className=\"col-md-2\" >\r\n                                                                    <div className=\"custom-control custom-switch custom-control-lg mb-2\">\r\n                                                                        <>\r\n                                                                            {\r\n                                                                                (getFuncionalidadesAsociadas.length > 0) ? (\r\n                                                                                    (getFuncionalidadesAsociadas.indexOf(funcionalidad.id.toString()) > -1) ? (\r\n\r\n                                                                                        <div>\r\n                                                                                            <input defaultChecked=\"true\" type=\"checkbox\" onClick={e => agregarCheck(e.target.checked, funcionalidad.id.toString())} className=\"custom-control-input\" id={funcionalidad.id} name={funcionalidad.id} />\r\n                                                                                            <label className=\"custom-control-label\" htmlFor={funcionalidad.id}></label>\r\n                                                                                        </div>\r\n                                                                                    ) :\r\n                                                                                        <div>\r\n                                                                                            <input type=\"checkbox\" onClick={e => agregarCheck(e.target.checked, funcionalidad.id.toString())} className=\"custom-control-input\" id={funcionalidad.id} name={funcionalidad.id} />\r\n                                                                                            <label className=\"custom-control-label\" htmlFor={funcionalidad.id}></label>\r\n                                                                                        </div>\r\n                                                                                ) :\r\n                                                                                    <div>\r\n                                                                                        <input type=\"checkbox\" onClick={e => agregarCheck(e.target.checked, funcionalidad.id.toString())} className=\"custom-control-input\" id={funcionalidad.id} name={funcionalidad.id} />\r\n                                                                                        <label className=\"custom-control-label\" htmlFor={funcionalidad.id}></label>\r\n                                                                                    </div>\r\n                                                                            }\r\n                                                                        </>\r\n                                                                    </div>\r\n\r\n                                                                </div>\r\n                                                                <div className=\"col-md-10\" >\r\n                                                                    {funcionalidad.nombre_mostrar}\r\n                                                                </div>\r\n                                                            </div>\r\n                                                        )\r\n                                                    })\r\n                                                }\r\n\r\n                                            </div>\r\n                                        </div>\r\n                                    </div>\r\n                                </div >\r\n\r\n                            ) : null\r\n                        )\r\n                    })\r\n                    }\r\n                </div>\r\n            </div>\r\n\r\n        )\r\n    }\r\n\r\n    const componentFormularioUsuario = () => {\r\n\r\n        return (\r\n            <>\r\n\r\n                <Spinner />\r\n                <Formik\r\n                    initialValues={{\r\n\r\n                        nombre: '',\r\n                        funcionalidades: ''\r\n\r\n                    }}\r\n                    enableReinitialize\r\n                    validate={(valores) => {\r\n\r\n                        let errores = {}\r\n\r\n                        if (!getNombre) {\r\n                            errores.nombre = \"Debe ingresar un nombre\";\r\n                        }\r\n\r\n                        if (getFuncionalidadesAsociadas.length == 0) {\r\n\r\n                            errores.funcionalidades = \"Debe seleccionar al menos una funcionalidad\";\r\n                        }\r\n\r\n                        return errores\r\n                    }}\r\n                    onSubmit={(valores, { resetForm }) => {\r\n\r\n                        enviarDatos();\r\n                    }}\r\n                >\r\n                    {({ errors }) => (\r\n                        <Form>\r\n\r\n                            <div className='row'>\r\n\r\n                                <div className=\"col-md-12\">\r\n                                    <div className=\"block block-themed\">\r\n                                        <div className=\"block-header\">\r\n                                            <h3 className=\"block-title\">Detalle del Rol</h3>\r\n                                        </div>\r\n\r\n                                        <div className=\"block-content\">\r\n                                            <div className=\"row\">\r\n\r\n                                                <div className=\"col-md-12\">\r\n                                                    <div className=\"form-group\">\r\n                                                        <label htmlFor='nombre'>Nombre </label>\r\n                                                        <Field value={getNombre} type=\"text\" id=\"nombre\" name=\"nombre\" className=\"form-control\" placeholder=\"Nombre\"\r\n                                                            onChange={handleInputChange} />\r\n                                                        <ErrorMessage name=\"nombre\" component={() => (<span className=\"text-danger\">{errors.nombre}</span>)} />\r\n                                                    </div>\r\n                                                </div>\r\n                                            </div>\r\n\r\n                                            <div className=\"row\" >\r\n\r\n                                                <div className=\"col-md-12\" >\r\n                                                    <div className=\"form-group\">\r\n                                                        <label>Asignar funcionalidades</label>\r\n                                                    </div>\r\n                                                    <ErrorMessage name=\"funcionalidades\" component={() => (<span className=\"text-danger\">{errors.funcionalidades}</span>)} />\r\n                                                    <div style={{ 'height': 'auto' }}>\r\n                                                        {getRespuestaFuncionalidades ? listarFuncionalidades() : null}\r\n                                                    </div>\r\n\r\n                                                </div>\r\n                                            </div>\r\n\r\n                                        </div>\r\n                                        <div className=\"block-content block-content-full text-right bg-light\">\r\n                                            <button type=\"submit\" className=\"btn btn-rounded btn-primary\"> Actualizar Rol </button>\r\n                                        </div>\r\n\r\n                                    </div>\r\n\r\n\r\n                                </div>\r\n\r\n\r\n                            </div>\r\n                        </Form>\r\n                    )}\r\n                </Formik>\r\n            </>\r\n        )\r\n    }\r\n\r\n    return (\r\n\r\n        <>\r\n            {isRedirect ? redirectToRoutes() : null}\r\n            {<InfoErrorApi error={errorApi} />}\r\n            {<InfoExitoApi />}\r\n\r\n            <div className=\"w2d_block\">\r\n                <nav aria-label=\"breadcrumb\">\r\n                    <ol className=\"breadcrumb breadcrumb-alt push\">\r\n                        <li class=\"breadcrumb-item\"> <Link underline=\"hover\" className=\"text-dark\" to={`/Rol`}><small>Lista de Roles</small></Link></li>\r\n                        <li class=\"breadcrumb-item\"> <small> Actualizar Rol</small></li>\r\n                    </ol>\r\n                </nav>\r\n            </div>\r\n\r\n\r\n\r\n\r\n            <div className=\"col-md-12\">\r\n\r\n\r\n                {componentFormularioUsuario()}\r\n\r\n            </div>\r\n\r\n\r\n\r\n        </>\r\n    )\r\n\r\n}\r\nexport default RolDetalle;"]}}