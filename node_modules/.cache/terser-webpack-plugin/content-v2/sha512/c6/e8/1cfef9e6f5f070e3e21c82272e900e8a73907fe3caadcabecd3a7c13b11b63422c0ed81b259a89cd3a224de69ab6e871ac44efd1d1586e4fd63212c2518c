nes-by-uuid/\" + uuid_actuacion).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        if (datos.data.length > 0) {\r\n                            handleDownloadArchivo2(datos.data[0]);\r\n                        }\r\n                    }\r\n                }\r\n            )\r\n        } catch (error) {\r\n            console.error(error);\r\n        }\r\n    }\r\n\r\n    const handleDownloadArchivo2 = (getArchivosListaSearch) => {\r\n        try {\r\n            GenericApi.getGeneric(\"archivo-actuaciones/get-documento/\" + getArchivosListaSearch.attributes.id + \"/\" + getArchivosListaSearch.attributes.extension).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        downloadBase64File(datos.content_type, datos.base_64, getArchivosListaSearch.attributes.nombre_archivo);\r\n                    }\r\n                    window.showSpinner(false);\r\n                }\r\n            )\r\n        } catch (error) {\r\n            console.error(error);\r\n        }\r\n    }\r\n\r\n    function downloadBase64File(contentType, base64Data, fileName) {\r\n        const linkSource = `data:${contentType};base64,${base64Data}`;\r\n        const downloadLink = document.createElement(\"a\");\r\n        downloadLink.href = linkSource;\r\n        downloadLink.download = fileName;\r\n        downloadLink.click();\r\n    }\r\n\r\n    const columns = [\r\n        {\r\n            name: 'Proceso disciplinario',\r\n            cell: documento =>\r\n                <div>\r\n                    <strong>Radicado: </strong>{documento.attributes.proceso_disciplinario.radicado}<br/>\r\n                    <strong>Vigencia: </strong>{documento.attributes.proceso_disciplinario.vigencia}\r\n                </div>\r\n            ,\r\n            selector: documento => documento.attributes.proceso_disciplinario.radicado,\r\n            sortable: true,\r\n        },\r\n        {\r\n            name: 'Solicitado Por',\r\n\r\n            cell: documento => \r\n                <div>\r\n                    <strong>Fecha: </strong>{documento.attributes.created_at}<br/>\r\n                    <strong>Usuario: </strong>{documento.attributes.id_user.nombre + \" \" + documento.attributes.id_user.apellido + \" (\"+ documento.attributes.id_user.name +\")\"}\r\n                </div>\r\n            ,\r\n            selector: documento => documento.attributes.id_user.nombre,\r\n            sortable: true,\r\n        },\r\n        {\r\n            name: 'Documento',\r\n            cell: documento =>\r\n                <button type='button' title={documento.attributes.id_actuacion? documento.attributes.id_actuacion.documento_ruta.slice(19) : null} className='btn btn-sm btn-primary' onClick={() => handleDownloadArchivo(documento.attributes.id_actuacion.uuid)}>Descargar documento <i className=\"fas fa-download\"></i> </button>          \r\n            ,\r\n            sortable: true,\r\n        },\r\n        {\r\n            name: 'Estado',\r\n            cell: documento => \r\n                <div>\r\n                    {documento.attributes.estado == global.Constants.ESTADO_FIRMA_MECANICA.PENDIENTE_FIRMA ? \"Pendiente firma\" : null}\r\n                    {documento.attributes.estado == global.Constants.ESTADO_FIRMA_MECANICA.FIRMADO ? \"Firmado\" : null}\r\n                </div>\r\n            ,\r\n            selector: documento => (documento.attributes.estado == global.Constants.ESTADO_FIRMA_MECANICA.PENDIENTE_FIRMA ? \r\n                \"Pendiente firma\" \r\n                : \r\n                documento.attributes.estado == global.Constants.ESTADO_FIRMA_MECANICA.FIRMADO ?\r\n                \"Firmado\" \r\n                : \r\n                null\r\n                \r\n            ),\r\n            sortable: true,\r\n        },\r\n        {\r\n            name: 'Acciones',\r\n            width: \"15%\",\r\n            cell: documento => (\r\n                documento.attributes.estado == global.Constants.ESTADO_FIRMA_MECANICA.PENDIENTE_FIRMA ? \r\n                (\r\n                    documento.attributes.id_user.firma_mecanica != null ? \r\n                    (\r\n                        <Link className=\"btn btn-primary\" to=\"/FirmaActuaciones\" state={{FirmaActuacion: documento }}>\r\n                            <span className=\"nav-main-link-name\">Firmar</span>\r\n                        </Link>\r\n                    ) : \r\n                    (\r\n                        <button type=\"button\" className=\"btn btn-rounded btn-primary\" onClick={() => setModalState({ title: \"Firmar Actuacion\", message: \"Falta agregar firma mecanica en su perfil\", show: true, redirect: \"/perfil/\", alert: global.Constants.TIPO_ALERTA.ERROR })}>Firmar</button>\r\n                        \r\n                    )\r\n                ) : null)\r\n        }\r\n    ];\r\n\r\n    const handlePageChange = page => {\r\n        setPageActual(page);\r\n    }\r\n\r\n    const handlePerRowsChange = async (newPerPage, page) => {\r\n        setPerPage(newPerPage);\r\n        setPageActual(page);\r\n    }\r\n\r\n    return (\r\n        <>\r\n            {<Spinner />}\r\n            {<ModalGen data={getModalState} />}\r\n            {<InfoErrorApi error={errorApi} />}\r\n            <Formik>\r\n                <Form>\r\n                    <div className=\"w2d_block\">\r\n                        <nav aria-label=\"breadcrumb\">\r\n                            <ol className=\"breadcrumb breadcrumb-alt push\">\r\n                                <li className=\"breadcrumb-item\"><Link underline=\"hover\" className=\"text-dark\" to={`/Perfil`}><small>Perfil</small></Link></li>\r\n                                <li className=\"breadcrumb-item\"><small>Documentos firmados y/o pendientes de firma</small></li>\r\n                            </ol>\r\n                        </nav>\r\n                    </div>\r\n                    <div className=\"row\">\r\n                        <div className=\"col-md-12\"> \r\n                            <div className=\"block block-themed\">\r\n                                <div className=\"block-header\">\r\n                                    <h3 className=\"block-title\">Documentos firmados o pendientes de firma</h3>\r\n                                </div>\r\n                                <div className=\"block-content\">\r\n                                    <div className=\"row\">\r\n                                        <div className='col-md-3'>\r\n                                            <div className=\"form-group \">\r\n                                                <Field type=\"text\" id=\"search\" value={getSeach} onChange={e => setSeach(e.target.value)} onKeyPress={(e) => { e.key === 'Enter' && e.preventDefault(); }} name=\"search\" className=\"form-control border border-success\" placeholder=\"Buscar\" />\r\n                                            </div>\r\n                                        </div>\r\n                                        <div className='col-md-9 text-right'>\r\n                                            <Link to={`/Perfil/`} title='Regresar al perfil'>\r\n                                                <button type=\"button\" className=\"btn btn-success\"><i class=\"fas fa-backward\"></i></button>\r\n                                            </Link>\r\n                                        </div>\r\n                                    </div>\r\n                                    <div className=\"row\">\r\n                                        <div className='col-md-12 mt-2 mb-2'>\r\n                                            <DataTable className=\"table table-bordered table-striped table-vcenter js-dataTable-full\"\r\n                                                columns={columns}\r\n                                                data={getListaFirmasMecanicas.data.filter((suggestion) => {\r\n                                                    // console.log(getListaFirmasMecanicas);\r\n                                                    if (getSeach === \"\") {\r\n                                                        return suggestion;\r\n                                                    } else if (\r\n\r\n                                                        ((quitarAcentos(suggestion.attributes.proceso_disciplinario.radicado)\r\n                                                        + quitarAcentos(suggestion.attributes.proceso_disciplinario.vigencia) \r\n                                                        + quitarAcentos(suggestion.attributes.created_at) \r\n                                                        + quitarAcentos(suggestion.attributes.id_user.nombre) \r\n                                                        + quitarAcentos(suggestion.attributes.id_user.apellido) \r\n                                                        + quitarAcentos(suggestion.attributes.id_user.name) \r\n                                                        + (suggestion.attributes.estado == global.Constants.ESTADO_FIRMA_MECANICA.PENDIENTE_FIRMA ? \"Pendiente firma\" : suggestion.attributes.estado == global.Constants.ESTADO_FIRMA_MECANICA.FIRMADO ? \"Firmado\" : null)\r\n                                                        \r\n                                                        ).toLowerCase().includes(getSeach.toLowerCase()))\r\n\r\n                                                    ) {\r\n                                                        return suggestion;\r\n                                                    }\r\n                                                })}\r\n                                                perPage={perPage}\r\n                                                page={pageActual}\r\n                                                pagination\r\n                                                noDataComponent={global.Constants.DATA_TABLE.SIN_DATOS_TABLE}\r\n                                                paginationTotalRows={getListaFirmasMecanicas.data.length}\r\n                                                onChangePage={handlePageChange}\r\n                                                onChangeRowsPerPage={handlePerRowsChange}\r\n                                                striped\r\n                                                paginationComponentOptions={{ rowsPerPageText: global.Constants.DATA_TABLE.FILTRO_POR_PAGINA }}\r\n                                            />\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>    \r\n                    </div>\r\n                </Form>\r\n            </Formik>\r\n        </>\r\n    )\r\n\r\n}\r\n\r\nexport default DocumentosFirmadosOPendientesDeFirma;","\r\nexport default global.Constants = {\r\n    ESTADOS: {\r\n        ACTIVO: '1',\r\n        INACTIVO: '0'\r\n    },\r\n\r\n    //MIS PENDIENTES\r\n    RANGOS: {\r\n        TODOS: '-1'\r\n    },\r\n\r\n    SEMAFORIZACION_FASES: {\r\n        GREEN: 'far fa-dot-circle txt-green',\r\n        RED: 'far fa-dot-circle txt-red',\r\n        ORANGE: 'far fa-dot-circle txt-orange'\r\n    },\r\n\r\n    BOTONES_CARPETAS: {\r\n        PRIMARY: 'btn btn-sm btn-primary mr-1 mb-3',\r\n        SUCCESS: 'btn btn-sm btn-success mr-1 mb-3',\r\n    },\r\n\r\n    COLOR_FASE: {\r\n        COMPLETA: '#8BCD50',\r\n    },\r\n\r\n    //PARA LOS TEXT_AREA\r\n    TEXT_AREA: {\r\n        CARACTERES_ESPECIALES: `^[]{}'|<>~`,\r\n        //CARACTERES_ESPECIALES: `\\`!@#$%^&*()_+\\-=\\[\\]{};':\"\\\\|<>\\/?~`,\r\n        CANTIDAD_MINIMA_DESCRIPCION: 100 //Cantidad mínima que se muestra en las descripciones\r\n    },\r\n\r\n    //CARACTERES ESECIALES\r\n    CARACTERES_ESPECIALES: {\r\n        formatOnlyLetters: /^[A-Za-z\\s]+$/,\r\n        formatOnlyLe