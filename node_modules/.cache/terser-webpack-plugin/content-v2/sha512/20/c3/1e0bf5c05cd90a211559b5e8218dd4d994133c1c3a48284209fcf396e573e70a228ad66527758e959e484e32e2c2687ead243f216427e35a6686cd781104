            codigoEntidadInvestigado: '',\r\n                            nombreInvestigado: '',\r\n                            cargoInvestigado: '',\r\n                            observaciones: '',\r\n                        }}\r\n                        enableReinitialize\r\n                        validate={(valores) => {\r\n                            let errores = {}\r\n                            setCountTextArea(observaciones.length)\r\n\r\n                            if (!valores.idEntidad && idEntidad == '') {\r\n                                errores.idEntidad = 'Debe seleccionar una entidad'\r\n                            }\r\n                            if (!valores.nombreInvestigado && !nombreInvestigado) {\r\n                                errores.nombreInvestigado = 'Debe agregar un nombre del investigado'\r\n                            }\r\n                            if (!valores.cargoInvestigado && !cargoInvestigado) {\r\n                                errores.cargoInvestigado = 'Debe agregar un cargo'\r\n                            }\r\n                            if (!valores.observaciones && !observaciones) {\r\n                                errores.observaciones = 'Debe agregar observaciones'\r\n                            }\r\n                            else if (observaciones.length <= getMinimoTextArea && observaciones != 'NO_APLICA') {\r\n                                errores.observaciones = 'Las observaciones deben tener almenos ' + getMinimoTextArea + ' caracteres'\r\n                            }\r\n\r\n                            if (observaciones) {\r\n                                if (containsSpecialChars(observaciones))\r\n                                    errores.observaciones = 'Tiene caracteres inválidos'\r\n                            }\r\n\r\n                            //console.log(errores);\r\n                            return errores;\r\n                        }}\r\n\r\n                        onSubmit={(valores, { resetForm }) => {\r\n                            //console.log (valores);\r\n                            enviarDatos(valores);\r\n                        }}\r\n                    >\r\n                        {({ errors }) => (\r\n                            <Form>\r\n                                <div className=\"row\">\r\n                                    <div className=\"block block-themed\">\r\n                                        <div className=\"block-content\">\r\n                                            <div className=\"row\">\r\n                                                <div className=\"col-md-12\">\r\n                                                    <div className=\"form-group\">\r\n                                                        <label htmlFor=\"idEntidad\">Entidad del investigado <span className=\"text-danger\">*</span></label>\r\n                                                        <Autocomplete suggestions={listaEntidad} parentCallback={handleCallback} dataPersoneria={getInformacionPersoneria} />\r\n                                                        <ErrorMessage name=\"idEntidad\" component={() => (<span className=\"text-danger\">{errors.idEntidad}</span>)} />\r\n                                                    </div>\r\n                                                </div>\r\n                                            </div>\r\n                                            <div className=\"row\">\r\n                                                <div className=\"block-content\">\r\n                                                    <>\r\n                                                        {\r\n                                                            (esAnonimo == true) ? (\r\n                                                                <div className='text-center'>\r\n                                                                    <div className=\"custom-control custom-switch custom-control-lg mb-2\">\r\n                                                                        <input type=\"checkbox\" checked onChange={e => asignarAnonimoCheck(e.target.checked)} className=\"custom-control-input\" id=\"example-sw-custom-lg11\" name=\"example-sw-custom-lg11\" />\r\n                                                                        <label className=\"custom-control-label\" htmlFor=\"example-sw-custom-lg11\">No conozco los datos</label>\r\n                                                                    </div>\r\n                                                                </div>\r\n                                                            ) : null\r\n                                                        }\r\n\r\n                                                        {\r\n                                                            (esAnonimo == false) ? (\r\n                                                                <div className='text-center'>\r\n                                                                    <div className=\"custom-control custom-switch custom-control-lg mb-2\">\r\n                                                                        <input type=\"checkbox\" onChange={e => asignarAnonimoCheck(e.target.checked)} className=\"custom-control-input\" id=\"example-sw-custom-lg12\" name=\"example-sw-custom-lg12\" />\r\n                                                                        <label className=\"custom-control-label\" htmlFor=\"example-sw-custom-lg12\">No conozco los datos</label>\r\n                                                                    </div>\r\n                                                                </div>\r\n                                                            ) : null\r\n                                                        }\r\n                                                    </>\r\n                                                </div>\r\n\r\n                          