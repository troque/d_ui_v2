das\"].split(',').map((rol, i) => {\r\n                setFasesPermitidas(oldArray => [...oldArray, rol]);\r\n            });\r\n        }\r\n        //verificamos a cuales fases tiene acceso el resultado\r\n        else if (attributes[\"cantidad_evaluacion\"]) {\r\n            if (attributes[\"cantidad_evaluacion\"][0][\"attributes\"][\"fases_permitidas\"]) {\r\n\r\n                attributes[\"cantidad_evaluacion\"][0][\"attributes\"][\"fases_permitidas\"].split(',').map((rol, i) => {\r\n                    setFasesPermitidas(oldArray => [...oldArray, rol]);\r\n                });\r\n\r\n            }\r\n        }\r\n\r\n\r\n\r\n\r\n\r\n        // Etapa: Evaluacin Fase: remision queja\r\n        if (attributes[\"muestra_remision_queja\"] === true && attributes[\"muestra_evaluacion\"] === true) {\r\n            setColorRemisionQueja(getBotonColorSuccess);\r\n            setHabilitaGestorRespuesta(true);\r\n        } else {\r\n            //setHabilitaGestorRespuesta(false);\r\n        }\r\n        // Etapa: Evaluacin Fase: gestor respuesta\r\n        if (attributes[\"muestra_gestor_respuesta\"] === true && attributes[\"muestra_remision_queja\"] === true) {\r\n            setColorGestorRespuesta(getBotonColorSuccess);\r\n            if (attributes[\"muestra_comunicacion_interesado\"] === true) {\r\n                setHabilitadaComunicacionInteresado(true);\r\n            }\r\n        } else {\r\n            //setHabilitadaComunicacionInteresado(false);\r\n        }\r\n\r\n        //si lo muestra revisamos si debe cambiar el color dependiendo si ya hay registros\r\n        if (attributes[\"comunicacion_interesado\"] === true && attributes[\"muestra_gestor_respuesta\"] === true) {\r\n            setColorComunicacionInteresado(getBotonColorSuccess);\r\n            setHabilitaDocumentoCierre(true);\r\n        }\r\n        else {\r\n            //setHabilitaDocumentoCierre(false);\r\n        }\r\n\r\n\r\n        if (attributes[\"muestra_documento_cierre\"] === true && at