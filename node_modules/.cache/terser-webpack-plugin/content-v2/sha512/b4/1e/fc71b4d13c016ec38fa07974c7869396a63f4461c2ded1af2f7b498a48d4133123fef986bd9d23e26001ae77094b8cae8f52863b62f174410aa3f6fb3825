ange={e => selectChangeTipoRespuesta(e.target.value)}>\r\n                                            <option value=\"\">Por favor seleccione</option>\r\n                                            {respuestaTipoRespuesta ? selectTipoRespuesta() : null}\r\n                                        </select>\r\n                                    </div>\r\n                                    \r\n                                </div>\r\n                            </div>\r\n                        :null}\r\n\r\n\r\n                    {/*SI APLICA RECLASIFIFCACION*/}\r\n                    {selectedTipoRespuesta === '1' ? componenteTipoRespuesta(1) : ''}\r\n                    {/*NO APLICA RECLASIFIFCACION*/}\r\n                    {selectedTipoRespuesta === '2' ? componenteTipoRespuesta(2) : ''}\r\n                </div>\r\n            </div>\r\n        </>\r\n    );\r\n\r\n\r\n}\r\n\r\nexport default RequerimientoJuzgadoForm;","\r\nexport default global.Constants = {\r\n    ESTADOS: {\r\n        ACTIVO: '1',\r\n        INACTIVO: '0'\r\n    },\r\n\r\n    //MIS PENDIENTES\r\n    RANGOS: {\r\n        TODOS: '-1'\r\n    },\r\n\r\n    SEMAFORIZACION_FASES: {\r\n        GREEN: 'far fa-dot-circle txt-green',\r\n        RED: 'far fa-dot-circle txt-red',\r\n        ORANGE: 'far fa-dot-circle txt-orange'\r\n    },\r\n\r\n    BOTONES_CARPETAS: {\r\n        PRIMARY: 'btn btn-sm btn-primary mr-1 mb-3',\r\n        SUCCESS: 'btn btn-sm btn-success mr-1 mb-3',\r\n    },\r\n\r\n    COLOR_FASE: {\r\n        COMPLETA: '#8BCD50',\r\n    },\r\n\r\n    //PARA LOS TEXT_AREA\r\n    TEXT_AREA: {\r\n        CARACTERES_ESPECIALES: `^[]{}'|<>~`,\r\n        //CARACTERES_ESPECIALES: `\\`!@#$%^&*()_+\\-=\\[\\]{};':\"\\\\|<>\\/?~`,\r\n        CANTIDAD_MINIMA_DESCRIPCION: 100 //Cantidad mínima que se muestra en las descripciones\r\n    },\r\n\r\n    //CARACTERES ESECIALES\r\n    CARACTERES_ESPECIALES: {\r\n        formatOnlyLetters: /^[A-Za-z\\s]+$/,\r\n        formatOnlyLettersWhitAccent: /^[a-zA-Z