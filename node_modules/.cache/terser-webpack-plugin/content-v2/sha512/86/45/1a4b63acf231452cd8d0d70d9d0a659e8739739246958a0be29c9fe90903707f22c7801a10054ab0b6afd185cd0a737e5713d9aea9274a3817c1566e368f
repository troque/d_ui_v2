                   setModalState({ title: \"SINPROC No \"+radicado+\" :: Evaluación del radicado\", message: 'El proceso disciplinario es asignado a ' + datos.usuario_remitido, show: true, redirect: '/MisPendientes', from: {from}, alert: global.Constants.TIPO_ALERTA.EXITO});\r\n                        }\r\n                        else {\r\n                            setModalState({ title: \"SINPROC No \"+radicado+\" :: Evaluación del radicado\", message: 'En estos momentos no es posible traer la información.', show: true, redirect: null, from: {from}, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                        }\r\n                    }\r\n                    else {\r\n                        setModalState({ title: \"SINPROC No \"+radicado+\" :: Evaluación del radicado\", message: datos.error.toString(), show: true, redirect: null, from: {from}, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                    }\r\n                    window.showSpinner(false);\r\n                }\r\n            )\r\n\r\n        }\r\n\r\n        \r\n    }\r\n\r\n    const componentEvaluacionForm = () => {\r\n        if(disable != true){\r\n            return (\r\n                <>\r\n                    <Formik\r\n                        initialValues={{\r\n                            ResulEvaluacion: '',\r\n                            NoticiaPrio: '',\r\n                            tipoConducta: '',\r\n                            Justificacion: ''\r\n                        }}\r\n                        validate={(valores) => {\r\n                            let errores = {};\r\n                            setCountTextArea(valores.Justificacion.length)\r\n                            if (!valores.ResulEvaluacion) {\r\n                                errores.ResulEvaluacion = 'Debe ingresar un resultado'\r\n                            }\r\n    \r\n                            if (!valores.NoticiaPrio) {\r\n                                errores.NoticiaPrio = 'Debe ingresar si es una noticia priorizada'\r\n                            }\r\n    \r\n                            if (!valores.tipoConducta) {\r\n                                errores.tipoConducta = 'Debe ingresar tipo de conducta'\r\n                            }\r\n    \r\n    \r\n                            if (!valores.Justificacion) {\r\n                                errores.Justificacion = 'Debe ingresar una justificación'\r\n                            }\r\n    \r\n                            if (valores.Justificacion) {\r\n                                if (valores.Justificacion.length < getMinimoTextArea) {\r\n                                    errores.Justificacion = 'Debe ingresar mínimo ' + getMinimoTextArea + ' caracteres'\r\n                                }\r\n    \r\n                                if (containsSpecialChars(valores.Justificacion))\r\n                                    errores.Justificacion = 'Tiene caracteres inválidos'\r\n                            }\r\n                            return errores\r\n                        }}\r\n                        onSubmit={(valores, { resetForm }) => {\r\n                            enviarDatos(valores);\r\n                        }}>\r\n    \r\n                        {({ errors }) => (\r\n                            <Form>\r\n                                <div className='row'>\r\n    \r\n                                    <div className='col-md-12'>\r\n                                        <div className='block-content block-content-full'>\r\n                                            <div className='alert alert-success' role='alert'>\r\n                                                <p className='mb-0'>Para continuar con la gestión de la presente queja, el resultado de la evaluación indicado por usted deberá ser previamente aprobado y avalado por el/la Delegado(a) de la dependencia.\r\n                                                    Por lo anterior al registrar la información del presente formulario, el SINPROC le será remitido automáticamente a dicho funcionario(a). </p>\r\n                                            </div>\r\n                                        </div>\r\n                                    </div>\r\n    \r\n                                    <div className='col-md-6'>\r\n                                        <label htmlFor='ResulEvaluacion'>Resultado de la Evaluación<span className='text-danger'></span></label>\r\n                                        <Field as='select' className='form-control' id='ResulEvaluacion' name='ResulEvaluacion'>\r\n                                            <option>Por favor seleccione</option>\r\n                                            {getListaResultadoEvaluacion.data.length > 0 ? componentSelectResultadoEvaluacion() : null}\r\n                                        </Field>\r\n                                        <ErrorMessage name='ResulEvaluacion' component={() => (<span className='text-danger'>{errors.ResulEvaluacion}</span>)} />\r\n                                    </div>\r\n    \r\n                                    <div className='col-md-6'>\r\n                                        <label htmlFor='NoticiaPrio'>Noticia Priorizada<span className='text-danger'></span></label>\r\n                                        <Field as='select' className='form-control' id='NoticiaPrio' name='NoticiaPrio'>\r\n                                            <option value=''>Por favor seleccione</option>\r\n                                            <option value='1'>SI</option>\r\n                                            <option value='2'>NO</option>\r\n    \r\n                                        </Field>\r\n                                        <ErrorMessage name='NoticiaPrio' component={() => (<span className='text-danger'>{errors.NoticiaPrio}</span>)} />\r\n                                    </div>\r\n    \r\n                                    <div className='col-md-12'>\r\n                                        <label htmlFor='tipoConducta'>Tipo de Conducta<span className='text-danger'></span></label>\r\n                                        <Field as='select' className='form-control' id='tipoConducta' name='tipoConducta'>\r\n                                            <option value=''>Por favor seleccione</option>\r\n                                            {getListaTipoConducta.data.length > 0 ? componentSelectTipoConducta() : null}\r\n    \r\n                                        </Field>\r\n                                        <ErrorMessage name='tipoConducta' component={() => (<span className='text-danger'>{errors.tipoConducta}</span>)} />\r\n                                    </div>\r\n    \r\n    \r\n                                    <div className='col-md-12'>\r\n                                        <label htmlFor='Justificacion'>Justificación y observaciones de la evaluación seleccionada *</label>\r\n                                        <Field as=\"textarea\" className=\"form-control\" id=\"Justificacion\" name=\"Justificacion\" rows=\"4\"\r\n                                            placeholder=\"Escriba en este espacio las observaciones\" maxLength={getMaximoTextArea} minLength={getMinimoTextArea} ></Field>\r\n                                        <div className=\"text-right\">\r\n                                            <span className=\"text-primary\"> {countTextArea} / {getMaximoTextArea} </span>\r\n                                        </div>\r\n                                        <ErrorMessage name=\"Justificacion\" component={() => (<span className=\"text-danger\">{errors.Justificacion}</span>)} />\r\n                                    </div>\r\n    \r\n                                    <div className=\"block-content block-content-full text-right\">\r\n    \r\n                                        <button type=\"submit\" className=\"btn btn-rounded btn-primary\">Registrar</button>\r\n                                        <Link to={`/RamasProceso/`} state={{ from: from }} >\r\n                                            <button type=\"button\" className=\"btn btn-rounded btn-outline-primary\">Cancelar</button>\r\n                                        </Link>\r\n                                    </div>\r\n    \r\n    \r\n                                </div>\r\n                            </Form>\r\n                        )}\r\n                    </Formik>\r\n                </>\r\n            );\r\n        }else{\r\n            return null;\r\n        }\r\n        \r\n    }\r\n\r\n    const enviarDatosEvalucion = (datos) => {\r\n        window.showSpinner(true);\r\n        const data = {\r\n            \"data\": {\r\n                \"type\": 'evaluacion',\r\n                \"attributes\": {\r\n                    \"id_proceso_disciplinario\": procesoDisciplinarioId,\r\n                    \"estado\": getSelectedConcepto,\r\n                    \"resultado_evaluacion\": datos.ReclasiEvaluacion,\r\n                    \"justificacion\": datos.DetalEvaluacion,\r\n                }\r\n            }\r\n        }\r\n        console.log(JSON.stringify(data));\r\n\r\n        GenericApi.addGeneric('evaluacion-crear', data).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    console.log(datos.usuario_remitido);\r\n                    if (datos.usuario_remitido) {\r\n\r\n                        setModalState({ title: \"SINPROC No \"+radicado+\" :: Evaluación del radicado\", message: 'El proceso disciplinario es asignado a ' + datos.usuario_remitido, show: true, redirect: '/MisPendientes', from: {from}, alert: global.Constants.TIPO_ALERTA.EXITO});         \r\n                    }\r\n                    else {\r\n                        setModalState({ title: \"SINPROCNo \"+radicado+\" :: Evaluación del radicado\", message: 'En estos momentos no es posible traer la información.', show: true, redirect: null, from: {from}, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                    }\r\n                }\r\n                else {\r\n                    setModalState({ title: \"SINPROCNo \"+radicado+\" :: Evaluación del radicado\", message: datos.error.toString(), show: true, redirect: null, from: {from}, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                }\r\n                window.showSpinner(false);\r\n            }\r\n        )\r\n    }\r\n\r\n\r\n\r\n    const handleInputChange = (event) => {\r\n\r\n        const target = event.target;\r\n        const value = target.type === 'checkbox' ? target.checked : target.value;\r\n        const name = target.name;\r\n\r\n        if (name == \"ConcepEvaluacion\") {\r\n\r\n            setSelectedConcepto(value);\r\n        }\r\n    }\r\n    \r\n    const componentEvaluacionAprobacionForm = () => {\r\n        return (\r\n            <>\r\n\t\t\t{<InfoExitoCierreEtapa success='' />}\r\n                <Formik\r\n                    initialValues={{\r\n                        ConcepEvaluacion: '',\r\n                        ReclasiEvaluacion: '',\r\n                        DetalEvaluacion: ''\r\n                    }}\r\n                    validate={(valores) => {\r\n                        let errores = {};\r\n                        setCountTextArea(valores.DetalEvaluacion.length)\r\n                        if (!getSelectedConcepto) {\r\n                            errores.ConcepEvaluacion = 'Debe ingresar el concepto'\r\n                        }\r\n                        else {\r\n                            if (getSelectedConcepto == '3' && !valores.ReclasiEvaluacion) {\r\n                                errores.ReclasiEvaluacion = 'Debe ingresar un Resultado de Evaluación'\r\n                            }\r\n                        }\r\n                        if (!valores.DetalEvaluacion) {\r\n                            errores.DetalEvaluacion = 'Debe ingresar una justificación'\r\n                        }\r\n\r\n                        if (valores.DetalEvaluacion) {\r\n                            if (valores.DetalEvaluacion.length < getMinimoTextArea) {\r\n                                errores.DetalEvaluacion = 'Debe ingresar mínimo ' + getMinimoTextArea + ' caracteres'\r\n                            }\r\n\r\n                            if (containsSpecialChars(valores.DetalEvaluacion))\r\n                                errores.DetalEvaluacion = 'Tiene caracteres inválidos'\r\n                        }\r\n                        return errores\r\n\r\n\r\n                    }}\r\n                    onSubmit={(valores, { resetForm }) => {\r\n                        enviarDatosEvalucion(valores);\r\n                    }}>\r\n\r\n\r\n                    {({ errors }) => (\r\n                        <Form>\r\n                            <div className='row'>\r\n                                <div className='col-md-12 '>\r\n\r\n\r\n                                    <div className='col-md-12 mt-2 mb-2'>\r\n                                        <DataTable className=\"table table-bordered table-striped table-vcenter js-dataTable-full\"\r\n                                            columns={columns}\r\n                                            data={getDatosEvaluacion.data}\r\n                                            perPage={perPage}\r\n                                            page={pageActual}\r\n                                            pagination\r\n                                            noDataComponent={global.Constants.DATA_TABLE.SIN_DATOS_TABLE}\r\n                                            paginationTotalRows={getDatosEvaluacion.data.length}\r\n                                            onChangePage={handlePageChange}\r\n                                            onChangeRowsPerPage={handlePerRowsChange}\r\n                                            striped\r\n                                            paginat