a(true);\r\n                    } else {\r\n\r\n                        // Se redeclara la variable en false\r\n                        setExisteFirmaMecanica(false);\r\n                    }\r\n                } else {\r\n\r\n                    // Se redeclara la variable en false\r\n                    setExisteFirmaMecanica(false);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const getSpan = () => {\r\n        return (\r\n            <>\r\n                <br />\r\n                <span className=\"txt-blue-primary\">\r\n                    <strong>JEFE DEPENDENCIA: </strong>\r\n                </span>\r\n                {getDatosJefeDependencia.nombre + \" \" + getDatosJefeDependencia.apellido}\r\n            </>\r\n        )\r\n    }\r\n\r\n    // Se retorna el HTML\r\n    return (\r\n        <>\r\n            {<Spinner />}\r\n            <div className=\"row\">\r\n                <div className=\"col-md-12\">\r\n                    <div className=\"block block-themed\">\r\n                        <div className=\"block-header\">\r\n                            <h3 className=\"block-title\">PERFIL USUARIO</h3>\r\n                        </div>\r\n                        <div className=\"block-content\">\r\n                            <div className=\"row\">\r\n                                <div className=\"col-md-6 mb-2\">\r\n                                    <span className=\"txt-blue-primary\">\r\n                                        <strong>NOMBRE: </strong>\r\n                                    </span>\r\n                                    {getExistenDatosUsuario ?\r\n                                        getUser() ? getUser().nombre.toUpperCase() : null\r\n                                        : null}\r\n                                </div>\r\n                                <div className=\"col-md-6 mb-2\">\r\n                                    <span className=\"txt-blue-primary\">\r\n                             