                   downloadBase64File(datos.content_type, datos.base_64, nombre_documento, extension);\r\n                    }\r\n                    else {\r\n                        setModalState({ title: \"SINPROCNo \"+radicado+\" :: Gestor Respuesta\", message: datos.error.toString(), show: true, redirect: null, from: {from}, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                    }\r\n                }\r\n            )\r\n        } catch (error) {\r\n            console.error(error);\r\n        }\r\n    };\r\n\r\n    function downloadBase64File(contentType, base64Data, fileName, extension) {\r\n        const linkSource = `data:${contentType};base64,${base64Data}`;\r\n        const downloadLink = document.createElement(\"a\");\r\n        downloadLink.href = linkSource;\r\n        downloadLink.download = fileName;\r\n        downloadLink.click();\r\n    }\r\n\r\n    const componentRegistro = () => {\r\n        return (\r\n            <>\r\n            {\r\n                getGestorRespuestaApi?.data.length > 0\r\n                ?\r\n                    <div className=\"block block-themed block-transparent mb-0\">\r\n                        <div className=\"block-content\">\r\n                            <div className='row'>\r\n                                <div className=\"col-md-12\">\r\n                                    <div className='row'>\r\n                                        <div className=\"col-md-4\">\r\n                                            <label>Versión: </label> { getGestorRespuestaApi.data[0].attributes.version }\r\n                                        </div>\r\n                                        <div className=\"col-md-4\">\r\n                                            <label>Fecha: </label> { getGestorRespuestaApi.data[0].attributes.fecha }\r\n                                        </div>\r\n                                        <div className=\"col-md-4\">\r\n                                            <label>Concepto: </label> { getGestorRespuestaApi.data[0].attributes.aprobado == 1 ? 'APROBADO' : 'RECHAZADO' }\r\n                                        </div>\r\n                                    </div>\r\n                                    <div className='row'>\r\n                                        <div className=\"col-md-12\">\r\n                                            <label>Por: </label> { getGestorRespuestaApi.data[0].attributes.usuario }\r\n                                        </div>\r\n                                    </div>\r\n                                    <div className='row'>\r\n                                        <div className=\"col-md-10\">\r\n                                            <label>Observación: </label> { getGestorRespuestaApi.data[0].attributes.descripcion }\r\n                                        </div>\r\n                                        <div className=\"col-md-2\">\r\n                                            <button type='button' title='Descargar documento' className='btn btn-sm btn-primary' onClick={() => handleClicArchivo(getGestorRespuestaApi.data[0].attributes.documento_sirius)}><i className=\"fas fa-download\"></i></button>\r\n                                        </div>\r\n                                    </div>\r\n                                    <div className='row'>\r\n                                        <div className=\"col-md-6\">\r\n                                            <label>Evaluación Inicial: </label> { getGestorRespuestaApi?.data[0]?.attributes?.evaluaciones[0]?.nombre }\r\n                                        </div>\r\n                                        <div className=\"col-md-6\">\r\n                                            <label>Evaluación Final: </label> { getGestorRespuestaApi?.data[0]?.attributes?.evaluaciones[1]?.nombre }\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                :\r\n                null\r\n            }                \r\n                <div className=\"col-md-12\">\r\n                    <div className=\"alert alert-warning alert-dismissable\" role=\"alert\">\r\n                        <h3 className=\"alert-heading font-size-h4 my-2\">Alerta</h3>\r\n                        <p className=\"mb-0\"> Para cualquier archivo que requiera adjuntar valide previamente lo siguiente: </p>\r\n                        <p className=\"mb-0\">\r\n                            NO puede superar QUINCE (15) Mb de peso/tamaño.\r\n                            Tipo/Formato permitido: \r\n                            {getFormatosApi ? (getFormatosApi.map((suggestion) => {\r\n                                if (suggestion.attributes.estado == true)\r\n                                    return suggestion.attributes.nombre;\r\n\r\n                            })).join(' - '):null}\r\n                        </p>\r\n                        <p className=\"mb-0\">\r\n                            El nombre del archivo debe ser de máximo 40 caracteres.\r\n                        </p>\r\n                    </div>\r\n                </div>\r\n                <div className=\"col-md-12\">\r\n                    { componentForm() }\r\n                </div>\r\n            </>\r\n        )\r\n    }\r\n\r\n    /*Componentes */\r\n    const componentForm = () => {\r\n        return (\r\n            <Formik\r\n                initialValues={{\r\n                    descripcion: '',\r\n                }}\r\n                enableReinitialize\r\n                validate={(valores) => {\r\n\r\n                    let errores = {}\r\n                    setCountTextArea(valores.descripcion.length);\r\n\r\n                    if (valores.descripcion && valores.descripcion.length < getMinimoTextArea) {\r\n                        errores.