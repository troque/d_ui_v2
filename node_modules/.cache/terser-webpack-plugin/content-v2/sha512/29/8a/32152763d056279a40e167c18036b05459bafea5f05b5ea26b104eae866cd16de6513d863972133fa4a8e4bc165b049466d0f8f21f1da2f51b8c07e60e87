                                    onChangePage={handlePageChange}\r\n                                                onChangeRowsPerPage={handlePerRowsChange}\r\n                                                defaultSortFieldId=\"Nombre\"\r\n                                                striped\r\n                                                paginationComponentOptions={{ rowsPerPageText: 'Filas por pgina' }}\r\n                                                defaultSortAsc={false}\r\n                                            />\r\n                                        </div>\r\n                                    </div>\r\n                                    {getEventoInicio == 1 || getEventoInicio == 2 || getEventoInicio == 3 ? (\r\n                                    <div className='row mt-5'>\r\n                                        <div className=\"col-md-6\">\r\n                                            <div className=\"form-group\">\r\n                                                <label htmlFor=\"etapaDeLaActuacionInicia\">ETAPA DE LA ACTUACIN QUE FINALIZA EL SEMFORO</label>\r\n                                                <Field as=\"select\" value={getEtapaDeLaActuacionFinaliza} onChange={changeActuacionIniciaSemaforo} className=\"form-control\" id=\"etapaDeLaActuacionFinaliza\" name=\"etapaDeLaActuacionFinaliza\" placeholder=\" Etapa de la actuacin que finaliza\">\r\n                                                    <option value=\"\">{global.Constants.MENSAJE_INFORMATIVO.SELECCIONE_UNA_OPCION}</option>\r\n                                                    {getrespuestaMasEtapas ? selectEtapas() : null}\r\n                                                </Field>\r\n                                                <ErrorMessage name=\"etapaDeLaActuacionFinaliza\" component={() => (<span className=\"text-danger\">{errors.etapaDeLaActuacionFinaliza}</span>)} />\r\n                                            </div>\r\n                    