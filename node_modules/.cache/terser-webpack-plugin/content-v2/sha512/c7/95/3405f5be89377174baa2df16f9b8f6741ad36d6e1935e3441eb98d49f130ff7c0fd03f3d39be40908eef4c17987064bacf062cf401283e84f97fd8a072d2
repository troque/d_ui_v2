-striped table-vcenter js-dataTable-full\">\r\n                                            <thead>\r\n                                                <tr>\r\n                                                    <th>Etapa</th>\r\n                                                    <th>Fecha registro</th>\r\n                                                    <th>Funcionario Registra</th>\r\n                                                    <th>Dependencia</th>\r\n                                                    <th>Detalle de cierre</th>\r\n                                                </tr>\r\n                                            </thead>\r\n                                            <tbody>\r\n                                                {getRtaCierreEtapa_EV ? listaCierreEtapaEV() : null}\r\n                                            </tbody>\r\n                                        </table>\r\n                                    </div>\r\n                                </div>\r\n                                <div className=\"block-content block-content-full text-right bg-light\">\r\n                                    <button type=\"button\" className=\"btn btn-rounded btn-primar\" data-dismiss=\"modal\">Cerrar</button>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n        </>\r\n    );\r\n}\r\n\r\nexport default ModalCierreEtapaInfo;\r\n","import React, { useEffect, useState } from 'react';\r\nimport { Link } from \"react-router-dom\";\r\nimport { Field, Form, Formik } from 'formik';\r\nimport Spinner from '../Utils/Spinner';\r\nimport moment from 'moment';\r\nimport { useLocation } from 'react-router-dom';\r\nimport { hasAccess } from '../../components/Utils/Common';\r\nimport '../Utils/Constants';\r\nimport GenericApi from '../Api/Services/GenericApi';\r\nimport { getUser } from '../../components/Utils/Common';\r\nimport ModalCierreEtapaInfo from '../Utils/Modals/ModalCierreEtapaInfo';\r\nimport ModalGen from '../Utils/Modals/ModalGeneric';\r\n\r\nfunction RamasProceso() {\r\n\r\n    const [getRespuestaAntecedente, setRespuestaAntecedente] = useState(false);\r\n    const [getRtaJefe, setRtaJefe] = useState(false);\r\n    const [getListaFasesHabilitadas, setListaFasesHabilitadas] = useState({ data: [] });\r\n    const [getModalState, setModalState] = useState({ title: \"\", message: \"\", show: false });\r\n    const [getFase, setFase] = useState(false);\r\n    const [getEtapa, setEtapa] = useState(0);\r\n    const [getIdEtapaAnterior, setIdEtapaAnterior] = useState(\"\");\r\n    const [getFuncionarioAsignado, setFuncionarioAsignado] = useState(\"\");\r\n    const [getUsuarioComisionado, setUsuarioComisionado] = useState(\"\");\r\n    const [getDependenciaComisionado, setDependenciaComisionado] = useState(\"\");\r\n    const [getExistenActuaciones, setExistenActuaciones] = useState(false);\r\n    \r\n    \r\n    \r\n    \r\n    const [getColorText, setColorText] = useState(\"\");\r\n\r\n    const location = useLocation();\r\n    const { from , disable} = location.state;\r\n    let radicado = from.radicado;\r\n    let procesoDisciplinarioId = from.procesoDisciplinarioId;\r\n    let vigencia = from.vigencia;\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n\r\n            window.showSpinner(true);\r\n\r\n            // VALIDA SI ES JEFE\r\n            jefeDependencia();\r\n            listaFasesHabilitadas();\r\n            etapaActual();\r\n            \r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n    /**\r\n     * Se consulta si el usuario que se encuentra en sesión es jefe de la dependencia\r\n     */\r\n    const jefeDependencia = () => {\r\n\r\n        const data = {\r\n            \"data\": {\r\n                \"type\": \"usuario\",\r\n                \"attributes\": {\r\n                    \"\": \"\"\r\n                }\r\n            }\r\n        }\r\n\r\n        GenericApi.getByDataGeneric(\"usuario/get-jefe-dependencia\", data).then(\r\n\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setRtaJefe(true)\r\n                }\r\n                else {\r\n                    setRtaJefe(false);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const etapaActual = () => {\r\n        GenericApi.getGeneric(\"proceso-diciplinario/\" + procesoDisciplinarioId).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    from.idEtapa = datos.data.attributes.etapa.id;\r\n                    setEtapa(datos.data.attributes.etapa.id);\r\n                    cargarActuaciones(datos.data.attributes.etapa.id);\r\n                    funcionarioAsignado(datos.data.attributes.etapa.id);\r\n                    if(datos.data.attributes.usuario_comisionado != null){\r\n                        GenericApi.getGeneric(\"mas-dependencia-origen/\" + datos.data.attributes.usuario_comisionado.id_dependencia).then(\r\n                            datos => {\r\n                                if (!datos.error) {\r\n                                    setDependenciaComisionado(datos.data.attributes.nombre);\r\n                                }\r\n                            }\r\n                        )\r\n                        setUsuarioComisionado(datos.data.attributes.usuario_comisionado.nombre+\" \"+datos.data.attributes.usuario_comisionado.apellido);\r\n                    }\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const listaFasesHabilitadas = () => {\r\n\r\n        GenericApi.getGeneric(\"proceso-diciplinario/get-fases-registradas/\" + procesoDisciplinarioId).then(\r\n\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    setListaFasesHabilitadas(datos);\r\n                    setFase(true);\r\n                    setRespuestaAntecedente(true);\r\n                }\r\n                else {\r\n\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const funcionarioAsignado = (etapa) => {\r\n        const dataCierreEtapa = {\r\n            \"data\": {\r\n                \"type\": \"cerrar_etapa\",\r\n                \"attributes\": {\r\n                    \"id_proceso_disciplinario\": procesoDisciplinarioId,\r\n                    \"id_etapa\": etapa - 1\r\n                }\r\n            }\r\n        }\r\n        GenericApi.getByDataGeneric(\"cierre-etapa/get-cierre-etapa\",dataCierreEtapa).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    console.log(datos);\r\n                    if(datos.data.length > 0){\r\n                        setIdEtapaAnterior(datos.data[0].attributes.id_etapa);\r\n                        setFuncionarioAsignado(datos.data[0].attributes.funcionario_asignado.email);\r\n                    }\r\n                    \r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    //Funciones\r\n    const handleClicSoporteRadicado = (id_etapa, id_fase, es_soporte) => {\r\n        from.id_etapa = id_etapa;\r\n        from.id_fase = id_fase;\r\n        changeValueEsFuncion(es_soporte);\r\n    };\r\n\r\n    function changeValueEsFuncion(es_funcion) {\r\n        from.es_soporte = es_funcion;\r\n    }\r\n\r\n    const cerrarEtapa = (datos) => {\r\n\r\n        window.showSpinner(true);\r\n\r\n        let data\r\n\r\n        data = {\r\n            \"data\": {\r\n                \"type\": \"cerrar_etapa\",\r\n                \"attributes\": {\r\n                    \"id_proceso_disciplinario\": procesoDisciplinarioId,\r\n                    \"id_etapa\": datos.inputEtapa,\r\n                    \"descripcion\": \"Cierre de etapa\",\r\n                    \"created_user\": getUser().nombre,\r\n                }\r\n            }\r\n        }\r\n\r\n\r\n        GenericApi.addGeneric('cierre-etapa', data).then(\r\n            datos => {\r\n\r\n                if (!datos.error) {\r\n                    setModalState({ title: \"Cierre de etapa\", message: \"El proceso disciplinario se asigna a: \" + datos.data.attributes.funcionario_asignado.nombre + \" \" + datos.data.attributes.funcionario_asignado.apellido, show: true, redirect: '/MisPendientes', from: { from }, alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                }\r\n                else {\r\n                    setModalState({ title: \"Cierre de etapa\", message: datos.error.toString(), show: true, redirect: '/RamasProceso', from: { from }, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n\r\n                }\r\n                window.showSpinner(false);\r\n            }\r\n        )\r\n    }\r\n\r\n    const componentAntecedente = () => {\r\n        return (\r\n            <>\r\n\r\n                <div className=\"col-lg-12 push\">\r\n                    <div className=\"alert alert-info\" role=\"alert\">\r\n                        <h4 className=\"txt-blue-primary\">RADICADO: {radicado}</h4>\r\n                        <span className=\"txt-blue-primary\"><strong>Antecedente: </strong></span><br/><br/>\r\n                        <span className='txt-black-primary' title={(from.antecedente) ? from.antecedente : \"\"}>{from.antecedente ? from.antecedente.substring(0, 300) : \"\"}...</span><br />\r\n\r\n                        <br />\r\n                        <div className=\"row\">\r\n                            <div className=\"col-lg-4\"><span><strong>Registrado Por: </strong>{from.registradoPor ? from.registradoPor : \"Sin Información\"}</span></div>\r\n                            <hr />\r\n                            <div className=\"col-lg-4\"><span><strong>Dependencia registro: </strong> {from.dependendencia ? from.dependendencia : \"Sin Información\"}</span></div>\r\n                            <hr />\r\n                            <div className=\"col-lg-4\">\r\n                                {getUsuarioComisionado != \"\" ? (\r\n                                    <span><strong>Usuario comisionado: </strong> {getUsuarioComisionado +\" (\"+getDependenciaComisionado+\")\"} </span>\r\n                                ) : null}\r\n                            </div>\r\n                            <hr />\r\n                        </div>\r\n\r\n                        <br></br>\r\n                        \r\n                        <div className=\"row\">\r\n                            <div className=\"col-lg-4\"><span><strong className=\"txt-blue-primary\">Vigencia: </strong>{vigencia}</span></div>\r\n                            <div className=\"col-lg-4\"><span><strong className=\"txt-blue-primary\">Fecha de registro: </strong> {moment(from.fechaRegistro, 'DD/MM/YYYY, h:mm:ss a').format(\"DD/MM/YYYY, h:mm:ss a\")}</span></div>\r\n                            <div className=\"col-lg-4\"><span><strong className=\"txt-blue-primary\">Fecha de ingreso: </strong> {moment(from.fechaIngreso, 'YYYY/MM/DD').format(\"DD/MM/YYYY\")}</span></div>\r\n\r\n\r\n                        </div>\r\n\r\n                    </div>\r\n                </div>\r\n            </>\r\n        )\r\n    }\r\n\r\n    const cargarFase = (id_etapa, id_fase, es_soporte, redirect) => {\r\n        for (var i = 0; i < getListaFasesHabilitadas.data.length; i++) {\r\n\r\n            if (getListaFasesHabilitadas.data[i].id == id_fase\r\n                && (getListaFasesHabilitadas.data[i].attributes.visible\r\n                    || (id_etapa >= 3 && id_etapa == from.idEtapa) //Validación para fases de Evaluación en PD en adelante\r\n                )) {\r\n\r\n                if (getListaFasesHabilitadas.data[i].attributes.estado) {\r\n                    return (\r\n                        <>\r\n                            <div className='w2d-form-group'>\r\n                                <Link to={redirect} state={{ from: from, selected_id_etapa: id_etapa, disable: disable }}>\r\n                                    <button type=\"button\" className=\"btn btn-sm btn-secundary w2d_btn-large mr-1 mb-3 text-left\" onClick={() => changeValueEsFuncion(false)}>\r\n                                        <i className={colorFase(getListaFasesHabilitadas.data[i].attributes.semaforizacion)}></i> {getListaFasesHabilitadas.data[i].attributes.nombre}</button>\r\n                                </Link>\r\n\r\n                                {getListaFasesHabilitadas.data[i].attributes.etapa == global.Constants.ETAPAS.CAPTURA_REPARTO ? (\r\n                                    <Link to={`/SoporteradicadoForm/`} state={{ from: from , disable: disable }}>\r\n                                        <button type=\"button\" className=\"btn btn-sm btn-secundary mr-1 mb-3\" onClick={() => handleClicSoporteRadicado(id_etapa, id_fase, es_soporte)}><i class=\"far fa-folder-open\"></i></button>\r\n                                    </Link>\r\n                                ) : null\r\n                                }\r\n                            </div>\r\n                        </>\r\n                    )\r\n                }\r\n\r\n                else {\r\n                    return (\r\n                        <>\r\n                            <div className='w2d-form-group'>\r\n                                <Link to={redirect} state={{ from: from, selected_id_etapa: id_etapa , disable: disable }}>\r\n                                    <button type=\"button\" className=\"btn btn-sm btn-secundary w2d_btn-large mr-1 mb-3 text-left\" onClick={() => changeValueEsFuncion(false)}>\r\n                                    <i className={colorFase(getListaFasesHabilitadas.data[i].attributes.semaforizacion)}></i> {getListaFasesHabilitadas.data[i].attributes.nombre}</button>\r\n                                </Link>\r\n\r\n                                {getListaFasesHabilitadas.data[i].attributes.etapa == global.Constants.ETAPAS.CAPTURA_REPARTO ? (\r\n                                    <Link to={`/SoporteradicadoForm/`} state={{ from: from , disable: disable }}>\r\n                                        <button type=\"button\" className=\"btn btn-sm btn-secundary mr-1 mb-3\" onClick={() => handleClicSoporteRadicado(id_etapa, id_fase, es_soporte)}>\r\n                                            <i className=\"far fa-folder-open\"></i></button>\r\n                                    </Link>\r\n                                ) : null\r\n                                }\r\n\r\n                            </div>\r\n                        </>\r\n                    )\r\n                }\r\n            }\r\n\r\n        }\r\n\r\n    }\r\n\r\n    const colorFase = (estado_fase) => {\r\n\r\n        if (estado_fase === 1)\r\n            return (global.Constants.SEMAFORIZACION_FASES.RED)\r\n        else if (estado_fase === 2)\r\n            return (global.Constants.SEMAFORIZACION_FASES.ORANGE)\r\n        else\r\n            return (global.Constants.SEMAFORIZACION_FASES.GREEN)\r\n    }\r\n\r\n    const cargarSoportesRadicado = (id_etapa, id_fase, es_soporte, redirect) => {\r\n\r\n        for (var i = 0; i < getListaFasesHabilitadas.data.length; i++) {\r\n\r\n            if (getListaFasesHabilitadas.data[i].id == id_fase && getListaFasesHabilitadas.data[i].attributes.estado) {\r\n                return (\r\n                    <div className='w2d-form-group'>\r\n                        <Link to={redirect} state={{ from: from , disable: disable }}>\r\n                            <button type=\"button\" className=\"btn btn-sm btn-secundary w2d_btn-large mr-1 mb-3 text-left\" onClick={() => handleClicSoporteRadicado(id_etapa, id_fase, es_soporte)}>\r\n                            <i className={colorFase(getListaFasesHabilitadas.data[i].attributes.semaforizacion)}></i> {getListaFasesHabilitadas.data[i].attributes.nombre}</button>\r\n                        </Link>\r\n                    </div>\r\n                )\r\n            }\r\n            else if (getListaFasesHabilitadas.data[i].id == id_fase && !getListaFasesHabilitadas.data[i].attributes.estado) {\r\n                return (\r\n                    <div className='w2d-form-group'>\r\n                        <Link to={redirect} state={{ from: from , disable: disable }}>\r\n                            <button type=\"button\" className=\"btn btn-sm btn-secundary w2d_btn-large mr-1 mb-3 text-left\" onClick={() => handleClicSoporteRadicado(id_etapa, id_fase, es_soporte)}>\r\n                            <i className={colorFase(getListaFasesHabilitadas.data[i].attributes.semaforizacion)}></i> {getListaFasesHabilitadas.data[i].attributes.nombre}</button>\r\n                        </Link>\r\n                    </div>\r\n                )\r\n            }\r\n        }\r\n    }\r\n\r\n    const cargarCierreEtapa = (id_etapa, id_fase_lista_para_cierre, id_fase_cerrada) => {\r\n\r\n        for (var i = 0; i < getListaFasesHabilitadas.data.length; i++) {\r\n\r\n            // valida cierre de captura y reparto\r\n            if (getListaFasesHabilitadas.data[i].id == id_fase_lista_para_cierre) {\r\n\r\n                console.log(\"ID_ETAPA: \" + id_etapa + \" ID_FASE_LISTA_PARA_CIERRE: \" + id_fase_lista_para_cierre + \" ID_FASE_CERRADA: \" + id_fase_cerrada)\r\n                return (\r\n                    <div className=\"form-group\">\r\n\r\n                        <Formik\r\n                            initialValues={{\r\n                                inputCierreEtapa: '',\r\n                                inputEtapa: id_etapa,\r\n                            }}\r\n                            enableReinitialize\r\n                            validate={(valores) => {\r\n\r\n                                let errores = {}\r\n                                if (!valores.inputCierreEtapa) {\r\n                                    errores.inputCierreEtapa = 'Debe seleccionar la opción cierre de etapa'\r\n                                }\r\n                                return errores\r\n                            }}\r\n\r\n                            onSubmit={(valores, { resetForm }) => {\r\n                                cerrarEtapa(valores);\r\n                            }}\r\n                        >\r\n\r\n                            <Form>\r\n                                <div className=\"col-md-12\">\r\n                                    <Field type=\"checkbox\" className=\"form-check-input\" id=\"inputCierreEtapa\" name=\"inputCierreEtapa\" />\r\n                                    <label className=\"form-check-label\" htmlFor=\"example-checkbox-inline1\">¿Con la información actual se puede dar cierre a esta etapa?</label>\r\n                                </div>\r\n\r\n                                <div className=\"col-md-12\">\r\n                                    <>\r\n                                        {\r\n                                            (hasAccess('CR_CierreEtapa', 'Crear')) ? (\r\n                                                <div className=\"block-content block-content-full text-center\">\r\n                                                    <button type=\"submit\" className=\"btn btn-rounded btn-outline-primary\"> Cerrar Etapa</button>\r\n                                                </div>\r\n                                            ) : null\r\n                                        }\r\n                                    </>\r\n                                </div>\r\n                            </Form>\r\n                        </Formik>\r\n                    </div>\r\n                )\r\n            }\r\n\r\n            // valida cierre de captura y reparto\r\n            if (getListaFasesHabilitadas.data[i].id == id_fase_cerrada && getListaFasesHabilitadas.data[i].attributes.estado) {\r\n\r\n                return (\r\n                    <div className=\"col-md-12 text-center\">\r\n                        <button type='button' title='Consultar cierre de etapa' className='btn btn-sm'\r\n                            data-toggle=\"modal\" data-target={'#modal-consultar-detalle-' + id_etapa}> <i className=\"fas fa-clipboard-list fa-2x txt-blue-secundary\"></i></button>\r\n                    </div>\r\n                )\r\n            }\r\n        }\r\n    }\r\n\r\n    const cargarActuaciones = (etapa) => {\r\n        GenericApi.getGeneric('actuaciones/get-actuaciones-discipl-etapa/' + procesoDisciplinarioId + '/' + etapa + '/1').then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                        if(datos.data.length > 0){\r\n                            setExistenActuaciones(true);\r\n                        }else{\r\n                            setExistenActuaciones(false);\r\n                        }\r\n                } else {\r\n                    window.showModal(1)\r\n                }\r\n                window.showSpinner(false);\r\n            }\r\n            \r\n        )\r\n    }\r\n\r\n    return (\r\n        <>\r\n            {<Spinner />}\r\n            {<ModalCierreEtapaInfo object={1} />}\r\n            {<ModalGen data={getModalState} />}\r\n\r\n            <div className=\"row\">\r\n\r\n                <div className=\"col-md-12\">\r\n                    <div className=\"w2d_block\">\r\n                        <nav aria-label=\"breadcrumb\">\r\n                            <ol className=\"breadcrumb breadcrumb-alt push\">\r\n\r\n                                <li className=\"breadcrumb-item\">\r\n                                    <Link underline=\"hover\" className=\"text-dark\" to={`/MisPendientes`}><small>Mis Pendientes</small></Link></li>\r\n                                <li className=\"breadcrumb-item\"><small>Ramas del proceso</small></li>\r\n                            </ol>\r\n                        </nav>\r\n                    </div>\r\n                </div>\r\n                {getRespuestaAntecedente ? componentAntecedente() : null}\r\n\r\n                {disable ? null : (\r\n                <d