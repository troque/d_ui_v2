r\n        headers: headers\r\n    })\r\n    .then(response => {\r\n        return response.data;\r\n    })\r\n    .catch(error => {\r\n        handleError(error);\r\n        return {estado: false, error: error}\r\n    });\r\n}\r\n\r\nasync function updateGeneric(serviceName, id, data){\r\n\r\n    const url = baseUrl + serviceName + \"/\" + id;\r\n    headers['Authorization'] = getBearerToken();\r\n\r\n    return await axios.put(url, data, {\r\n        headers: headers\r\n    })\r\n    .then(response => {\r\n        return response.data;\r\n    })\r\n    .catch(error => {\r\n        handleError(error);\r\n        return {estado: false, error: error}\r\n    });\r\n}\r\n\r\n\r\n\r\n\r\nexport default { getGeneric, getAllGeneric, getByIdGeneric, addGeneric, updateGeneric, getByDataGeneric}\r\n","\r\nexport default global.Constants = {\r\n    ESTADOS: {\r\n        ACTIVO: '1',\r\n        INACTIVO: '0'\r\n    },\r\n\r\n    //MIS PENDIENTES\r\n    RANGOS: {\r\n        TODOS: '-1'\r\n    },\r\n\r\n    BOTONES_RAMAS: {\r\n        PRIMARY: 'btn btn-sm btn-primary w2d_btn-large mr-1 mb-3 text-left',\r\n        SUCCESS: 'btn btn-sm btn-success w2d_btn-large mr-1 mb-3 text-left',\r\n        WARNING: 'btn btn-sm btn-warning w2d_btn-large mr-1 mb-3 text-left'\r\n    },\r\n\r\n    COLOR_FASE: {\r\n        COMPLETA: '#8BCD50'\r\n    },\r\n\r\n    //PARA LOS TEXT_AREA\r\n    TEXT_AREA: {\r\n        CARACTERES_ESPECIALES: `\\`!@#$%^&*()_+\\-=\\[\\]{};':\"\\\\|<>\\/?~`,\r\n        CANTIDAD_MINIMA_DESCRIPCION: 100 //Cantidad mínima que se muestra en las descripciones\r\n    },\r\n\r\n    //CARACTERES ESECIALES\r\n    CARACTERES_ESPECIALES: {\r\n        formatOnlyLetters: /^[A-Za-z\\s]+$/,\r\n        formatOnlyLettersOrnUmbers: /^[0-9a-zA-Z\\s]+$/,\r\n        formatWithOutSpecials: /^[A-Za-z0-9#-\\s]+$/g\r\n    },\r\n\r\n    //ETAPAS\r\n    ETAPAS: {\r\n        NINGUNA: 0, // SE ASIGNO PARA LA PARTE ADMISNITRATIVA - QUE CIERTAS FUNCIUONALIDADES SE MOSTRARAN INDIFERNTES A LA ETAPA EN LA 