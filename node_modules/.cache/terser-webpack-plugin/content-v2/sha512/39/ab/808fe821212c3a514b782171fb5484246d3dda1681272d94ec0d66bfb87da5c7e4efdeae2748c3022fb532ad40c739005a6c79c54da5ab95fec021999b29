AllDepartamentosPaginacion(url).then(\r\n            datos => !datos.error ? setDepartamentosLista(datos) : window.showModal()\r\n        )\r\n    }\r\n\r\n    return (          \r\n        <div className=\"block block-rounded block-bordered\">\r\n            { <InfoErrorApi /> }\r\n            <div className=\"block-header block-header-default\">\r\n                <h3 className=\"block-title\">Lista de departamentos</h3>\r\n                <Link to={`${'agregar'}`}>\r\n                    <button type=\"button\" className=\"btn btn-primary mr-1 mb-3\">\r\n                        <i className=\"fa fa-fw fa-plus mr-1\"></i> Agregar\r\n                    </button>\r\n                </Link>\r\n            </div>                \r\n            <div className=\"block-content block-content-full\">\r\n                <div className=\"row\">\r\n                    <div className=\"col-sm-12\">\r\n                        <table className=\"table table-bordered table-striped table-vcenter js-dataTable-full\">\r\n                            <thead>\r\n                                <tr>\r\n                                    <th className=\"text-center\">No</th>\r\n                                    <th>Código DANE</th>\r\n                                    <th>Departamento</th>\r\n                                    <th>Acciones</th>\r\n                                </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                                { departamentos() }\r\n                            </tbody>\r\n                        </table>\r\n                    </div>\r\n                </div>\r\n                { paginacion() }\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default DepartamentosListaEjemplo;","import React, { Component } from 'react';\r\n\r\nimport DepartamentosListaEjemplo from './DepartamentosListaEjemplo';\r\n\r\nclass DepartamentosEjemplo extends Component {\r\n\r\n    render(){\r\n    