{\r\n\r\n        }\r\n\r\n    }\r\n\r\n    const columns = [\r\n        {\r\n            name: 'Nombre',\r\n            cell: usuario => <div>{(usuario.attributes.nombre ? usuario.attributes.nombre : \"\") + ' ' + (usuario.attributes.apellido ? usuario.attributes.apellido : \"\")}</div>,\r\n            selector: usuario => usuario.attributes.nombre,\r\n            sortable: true,\r\n            width: \"16%\"\r\n        },\r\n        {\r\n            name: 'Usuario',\r\n            selector: usuario => usuario.attributes.name,\r\n            sortable: true,\r\n            width: \"16%\"\r\n        },\r\n        {\r\n            name: 'Correo',\r\n            selector: usuario => usuario.attributes.email,\r\n            sortable: true,\r\n            width: \"22%\"\r\n        },\r\n        {\r\n            name: 'Dependencia',\r\n            selector: usuario => (usuario.attributes.dependencia ? usuario.attributes.dependencia.nombre : ''),\r\n            sortable: true,\r\n            width: \"25%\"\r\n\r\n        },\r\n        {\r\n            name: 'Estado',\r\n            selector: usuario => (usuario.attributes.estado == \"1\" ? 'Activo' : 'Inactivo'),\r\n            sortable: true,\r\n            width: \"10%\"\r\n        },\r\n        {\r\n            name: 'Acciones',\r\n\r\n            cell: row => <div><Link to={`${row.id}`}>\r\n                <button type=\"button\" className=\"btn btn btn-primary\" title='Editar'>\r\n                    <i className=\"fa fa-fw fa-edit\"></i>\r\n                </button>\r\n            </Link></div>\r\n\r\n\r\n        }\r\n    ];\r\n\r\n    const redirectToRoutes = () => {\r\n        return <Navigate to={`/UsuarioLista/`} />;\r\n    }\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n            window.showSpinner(true);\r\n            cargarUsuarios(1, paganationPerPages);\r\n\r\n\r\n\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n    const cargarUsuarios = (page, perPage) => {\r\n       