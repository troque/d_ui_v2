nst baseUrl = process.env.REACT_APP_API_URL;\r\n\r\nconst headers = {\r\n    'Content-Type': 'application/vnd.api+json',\r\n    'Accept': 'application/vnd.api+json'\r\n}\r\n\r\nasync function consultarRadicadoSirius(data){\r\n    \r\n    const url = baseUrl + \"proceso-diciplinario/validar-sirius\";\r\n    const token = getToken();\r\n    headers['Authorization'] = `Bearer ${token}`;\r\n\r\n    return await axios.post(url, data, {\r\n        headers: headers\r\n    })\r\n    .then(response => {\r\n        console.log(response)\r\n        return response.data;\r\n    })\r\n    .catch(error => {\r\n        console.log(error);\r\n        return {estado: false, error: error.message}\r\n    });\r\n}\r\n\r\n\r\nasync function consultarRadicadoDesgloce(data){\r\n\r\n    const url = baseUrl + \"proceso-diciplinario/validar-desglose\";\r\n    const token = getToken();\r\n    headers['Authorization'] = `Bearer ${token}`;\r\n   \r\n    \r\n    return await axios.post(url, data, {\r\n        headers: headers\r\n    })\r\n    .then(response => {\r\n        console.log(response)\r\n        return response.data;\r\n    })\r\n    .catch(error => {\r\n        console.log(error);\r\n        return {estado: false, error: error.message}\r\n    });\r\n}\r\n\r\nasync function consultarRadicadoSinproc(data){\r\n    \r\n    const url = baseUrl + \"proceso-diciplinario/validar-sinproc\";\r\n    const token = getToken();\r\n    headers['Authorization'] = `Bearer ${token}`;\r\n   \r\n    console.log(data)\r\n\r\n    return await axios.post(url, data, {\r\n        headers: headers\r\n    })\r\n    .then(response => {\r\n        console.log(response)\r\n        return response.data;\r\n    })\r\n    .catch(error => {\r\n        console.log(error);\r\n        return {estado: false, error: error.message}\r\n    });\r\n}\r\n\r\n\r\nasync function addProcesoDiciplinario(data){\r\n\r\n    const url = baseUrl + \"proceso-diciplinario\";\r\n    const token = getToken();\r\n    headers['Authorization'] = `Bearer ${token}`;\r\n   \r\n    console.log(data)\r\n\r\n    return await axios.post(url, data, {\r\n        headers: headers\r\n    })\r\n    .then(response => {\r\n        console.log(response)\r\n        return response.data;\r\n    })\r\n    .catch(error => {\r\n        //console.log(error);\r\n        return {estado: false, error: error}\r\n    });\r\n}\r\n\r\n\r\nasync function getProcesoDisciplinarioById(id){\r\n\r\n    const url = baseUrl + \"proceso-diciplinario/\"+id;\r\n    const token = getToken();\r\n    headers['Authorization'] = `Bearer ${token}`;\r\n    return await axios.get(url, {\r\n        headers: headers\r\n    })\r\n    .then(response => {\r\n        console.log(response.data);\r\n        return response.data;\r\n    })\r\n    .catch(error => {\r\n        console.log(error);\r\n        return {estado: false, error: error}\r\n    });\r\n}\r\n\r\nasync function getConsultarFasesProcesoDisciplinario(data, $id_proceso_disciplinario){\r\n    \r\n    const url = baseUrl + \"proceso-diciplinario/get-fases-registradas/\"+$id_proceso_disciplinario;\r\n    const token = getToken();\r\n    headers['Authorization'] = `Bearer ${token}`;\r\n\r\n    return await axios.post(url, data, {\r\n        headers: headers\r\n    })\r\n    .then(response => {\r\n        console.log(response.data)\r\n        return response.data;\r\n    })\r\n    .catch(error => {\r\n        console.log(error);\r\n        return {estado: false, error: error.message}\r\n    });\r\n}\r\n\r\n\r\nexport default { addProcesoDiciplinario, consultarRadicadoSirius, consultarRadicadoDesgloce, consultarRadicadoSinproc, getProcesoDisciplinarioById, getConsultarFasesProcesoDisciplinario }\r\n","function InfoExitoApiConsulta() {\r\n\r\n    return (\r\n        <>\r\n            <div className=\"modal fade\" id=\"modal-block-popout-exito-consulta\" tabIndex=\"-1\" role=\"dialog\" aria-labelledby=\"modal-block-popout\" aria-hidden=\"true\">\r\n                <div className=\"modal-dialog modal-di