n'\r\n}\r\n\r\nasync function getGeneric(relativeUrl){\r\n    const url = baseUrl + relativeUrl;\r\n    headers['Authorization'] = getBearerToken();\r\n    return await axios.get(url, {\r\n        headers: headers\r\n    })\r\n    .then(response => {\r\n        //console.log(response.data);\r\n        return response.data;\r\n    })\r\n    .catch(error => {\r\n        //console.log(error);\r\n        return {estado: false, error: error}\r\n    });\r\n}\r\n\r\nasync function getAllGeneric(serviceName){\r\n    const url = serviceName;\r\n    return getGeneric(url);\r\n}\r\n\r\nasync function getByIdGeneric(serviceName, id){\r\n    const url = serviceName + \"/\" + id;\r\n    return getGeneric(url);\r\n}\r\n\r\nasync function getByDataGeneric(serviceName, data){\r\n   \r\n    const url = baseUrl + serviceName;\r\n    headers['Authorization'] = getBearerToken();\r\n\r\n    console.log(\"URL: \"+serviceName);\r\n    console.log(\"DATA: \"+data);\r\n\r\n    return await axios.post(url, data, {\r\n        headers: headers\r\n    })\r\n    .then(response => {\r\n        console.log(\"RTA: \"+response.data);\r\n        return response.data;\r\n    })\r\n    .catch(error => {\r\n        console.log(\"RTA: \"+error);\r\n        return {estado: false, error: error}\r\n    });\r\n}\r\n\r\nasync function addGeneric(serviceName, data){\r\n    const url = baseUrl + serviceName;\r\n    headers['Authorization'] = getBearerToken();\r\n    return await axios.post(url, data, {\r\n        headers: headers\r\n    })\r\n    .then(response => {\r\n        return response.data;\r\n    })\r\n    .catch(error => {\r\n        return {estado: false, error: error}\r\n    });\r\n}\r\n\r\nasync function updateGeneric(serviceName, id, data){\r\n    const url = baseUrl + serviceName + \"/\" + id;\r\n    headers['Authorization'] = getBearerToken();\r\n    return await axios.put(url, data, {\r\n        headers: headers\r\n    })\r\n    .then(response => {\r\n        return response.data;\r\n    })\r\n    .catch(error => {\r\n        return {estado: false, error: error}\r\n    });\r\n}\r\n\r\nexport default { getGeneric, getAllGeneric, getByIdGeneric, addGeneric, updateGeneric, getByDataGeneric}\r\n","\r\nexport default global.Constants = {\r\n    ESTADOS: {\r\n        ACTIVO: '1',\r\n        INACTIVO: '0'\r\n    },\r\n\r\n    //MIS PENDIENTES\r\n    RANGOS: {\r\n        TODOS: '-1'\r\n    },\r\n\r\n    BOTONES_RAMAS: {\r\n        PRIMARY: 'btn btn-sm btn-secondary w2d_btn-large mr-1 mb-3 text-left',\r\n        SUCCESS: 'btn btn-sm btn-success w2d_btn-large mr-1 mb-3 text-left'\r\n    },\r\n\r\n    COLOR_FASE: {\r\n        COMPLETA: '#8BCD50'\r\n    },\r\n\r\n    //PARA LOS TEXT_AREA\r\n    TEXT_AREA: {\r\n        CARACTERES_ESPECIALES: `\\`!@#$%^&*()_+\\-=\\[\\]{};':\"\\\\|<>\\/?~`,\r\n        CANTIDAD_MINIMA_DESCRIPCION: 100 //Cantidad mínima que se muestra en las descripciones\r\n    },\r\n\r\n    //CARACTERES ESECIALES\r\n    CARACTERES_ESPECIALES: {\r\n        formatOnlyLetters: /^[A-Za-z\\s]+$/,\r\n        formatOnlyLettersOrnUmbers: /^[0-9a-zA-Z\\s]+$/,\r\n        formatWithOutSpecials: /^[A-Za-z0-9#-\\s]+$/g\r\n    },\r\n\r\n    //ETAPAS\r\n    ETAPAS: {\r\n        CAPTURA_REPARTO: '1',\r\n        EVALUACION: '2',\r\n        EVALUACION_PD: '3'\r\n    },\r\n\r\n    //FASES\r\n    FASES: {\r\n        ANTECEDENTES: 1,\r\n        INTERESADO: 2,\r\n        CLASIFICACION: 3,\r\n        ENTIDAD_INVESTIGADO: 4,\r\n        SOPORTE_RADICADO: 5\r\n\r\n\r\n    },\r\n\r\n    TIPO_INGRESSO: {\r\n        SIRIUS: 1,\r\n        DESGLOSE: 2,\r\n        SINRPOC: 3\r\n    },\r\n\r\n    //TABLAS\r\n    DATA_TABLE: {\r\n        SIN_DATOS_TABLE: 'Sin datos',\r\n        FILTRO_POR_PAGINA: 'Filas por página'\r\n    },\r\n\r\n    CLASIFICACION_RADICADO: {\r\n        TIPO_QUEJA_EXTERNA: 1,\r\n        TIPO_QUEJA_INTERNA: 2\r\n    },\r\n\r\n    TIPOS_EXPEDIENTES: {\r\n        DERECHO_PETICION: '1',\r\n        PODER_REFERENTE: '2',\r\n        QUEJA: '3',\r\n        TUTELA: '4'\r\n    }