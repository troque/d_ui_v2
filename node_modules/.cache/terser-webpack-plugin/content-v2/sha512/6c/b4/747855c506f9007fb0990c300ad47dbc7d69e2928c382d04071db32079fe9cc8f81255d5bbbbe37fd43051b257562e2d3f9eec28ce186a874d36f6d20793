                                   \"oficina_control_interno\": radicacionDesglose.data.attributes.clasificacion.oficina_control_interno,\r\n                                    \"created_user\": getUser().nombre,\r\n                                    \"reclasificacion\": radicacionDesglose.data.attributes.clasificacion.reclasificacion,\r\n                                }\r\n                            }\r\n                        }\r\n                        GenericApi.addGeneric(\"clasificacion-radicado\", dataClasificacion);\r\n\r\n                        let dataValidarCLasificado = {\r\n                            \"data\": {\r\n                                \"type\": \"validar_clasificacion\",\r\n                                \"attributes\": {\r\n                                    \"id_proceso_disciplinario\": datos.data.id,\r\n                                    \"id_etapa\": 2,\r\n                                    \"estado\": true,\r\n                                }\r\n                            }\r\n                        }\r\n                        GenericApi.addGeneric(\"validar-clasificacion\", dataValidarCLasificado);\r\n                        \r\n                        console.log(datos);\r\n                        GenericApi.getByIdGeneric(\"remision-queja\", getIdProcesoDisciplinarioPadre).then(\r\n                            datosRemision => {\r\n                                if (!datosRemision.error) {\r\n                                    console.log(datosRemision);\r\n                                    let data;\r\n                                    if(datosRemision.data.attributes.Idevaluacion == 1){\r\n                                        data = {\r\n                                            \"data\": {\r\n                                                \"type\": \"remision_queja\",\r\n                                                \"attributes\": {\r\n                                                    \"consulta\": false,\r\n                                                    \"id_proceso_disciplinario\": datos.data.id,\r\n                                                    \"id_tipo_evaluacion\": datosRemision.data.attributes.Idevaluacion,\r\n                                                    \"id_tipo_dependencia_acceso\": global.Constants.ACCESO_DEPENDENCIA.REMISION_QUEJA_COMISORIO_EJE,\r\n                                                    \"vigencia\": vigenciaRadicado.toString(),\r\n                                                }\r\n                                            }\r\n                                        }\r\n                                    }else if (datosRemision.data.attributes.Idevaluacion == 3) {\r\n                                        data = {\r\n                                            \"data\": {\r\n                                                \"type\": \"remision_queja\",\r\n                                                \"attributes\": {\r\n                                                    \"consulta\": false,\r\n                                                    \"id_proceso_disciplinario\": datos.data.id,\r\n                                                    \"id_tipo_evaluacion\": datosRemision.data.attributes.Idevaluacion,\r\n                                                    \"id_tipo_dependencia_acceso\": global.Constants.ACCESO_DEPENDENCIA.REMISION_QUEJA_INCORPORACION,\r\n                                                }\r\n                                            }\r\n                                        }\r\n                                    }else if (datosRemision.data.attributes.Idevaluacion == 5) {\r\n                                        data = {\r\n                                            \"data\": {\r\n                                                \"type\": \"remision_queja\",\r\n                                                \"attributes\": {\r\n                                                    \"consulta\": false,\r\n                                                    \"id_proceso_disciplinario\": datos.data.id,\r\n                                                    \"id_tipo_evaluacion\": datosRemision.data.attributes.Idevaluacion,\r\n                                                    \"id_tipo_dependencia_acceso\": global.Constants.ACCESO_DEPENDENCIA.REMISION_QUEJA_REMISORIO_INTERNO,\r\n                                                }\r\n                                            }\r\n                                        }\r\n                                    }\r\n\r\n                                    RemisionQuejaApi.addIncorporacionExpediente(data).then(\r\n                                        datosSubidos => {\r\n                                            if (!datosSubidos.error) {\r\n                                                console.log(datosSubidos);\r\n                                            }else {\r\n                                                setModalState({ title: \"Error\", message: datosSubidos.error.toString(), show: true, alert: global.Constants.TIPO_ALERTA.ERROR }); \r\n                                            }\r\n                                        }\r\n                                    );\r\n                                }\r\n                            }\r\n                        );\r\n                        \r\n\r\n                    }\r\n\r\n                    GenericApi.getByIdGeneric('get-semaforo-evento', 1).then(\r\n                        datosSemaforo => {\r\n                            if (!datosSemaforo.error) {\r\n                                datosSemaforo.data.forEach(element => {\r\n                                    if(element.attributes.id_mas_evento_inicio.id == 1){\r\n                                        let date = new Date();\r\n                                        let datapdxsemaforo = {\r\n                                            \"data\": {\r\n                                                \"type\": \"pdxsemaforo\",\r\n                                                \"attributes\": {\r\n                                                    \"id_semaforo\": element.id,\r\n                                                    \"id_proceso_disciplinario\": datos.data.id,\r\n                                                    \"id_actuacion\": \"\",\r\n                                                    \"fecha_inicio\": date.toLocaleDateString('zh-Hans-CN'),\r\n                                                    \"estado\": 1,\r\n                                                }\r\n                                            }\r\n                                        }\r\n                                        GenericApi.addGeneric('pdxsemaforo', datapdxsemaforo)\r\n                                    }\r\n                                });\r\n                            } else {\r\n                                setModalState({ title: \"Proceso Disciplinario\", message: \" A ocurrido un error con la semaforización\", show: true, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                            }\r\n                        }\r\n                    )\r\n\r\n                    setModalState({ title: \"Proceso Disciplinario\", message: \"Proceso registrado con número de registro \" + datos[\"data\"][\"attributes\"][\"radicado\"] + \".\", show: true, redirect: '/MisPendientes', alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                }\r\n                else {\r\n                    setModalState({ title: \"Proceso Disciplinario\", message: datos.error.toString(), show: true, redirect: 'ProcesoDisciplinario', alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                }\r\n                window.showSpinner(false);\r\n            }\r\n        )\r\n    }\r\n\r\n    /*\r\n    * Componentes\r\n    */\r\n\r\n    const selectOrigenRadicado = () => {\r\n        return (\r\n            listaOrigenRadicado.data.map((origen, i) => {\r\n                return (\r\n                    <option key={origen.id} value={origen.id}>{origen.attributes.nombre}</option>\r\n                )\r\n            })\r\n        )\r\n    }\r\n\r\n    const selectTipoProceso = () => {\r\n        return (\r\n            listaTipoProceso.data.map((proceso, i) => {\r\n                return (\r\n                    <option key={proceso.id} value={proceso.id}>{proceso.attributes.nombre}</option>\r\n                )\r\n            })\r\n        )\r\n    }\r\n\r\n\r\n    const selectEtapaPoderPreferente = () => {\r\n        return (\r\n            getListaEtapaActivaPoderPreferente.data.map((etapa, i) => {\r\n                return (\r\n                    <option key={etapa.id} value={etapa.id}>{etapa.attributes.nombre}</option>\r\n                )\r\n            })\r\n        )\r\n    }\r\n\r\n    const selectDependenciaOrigen = () => {\r\n        return (\r\n            getListaDependenciaOrigenDesglose.data.map((dependencia, i) => {\r\n                return (\r\n                    <option key={dependencia.id} value={dependencia.id}>{dependencia.attributes.nombre}</option>\r\n                )\r\n            })\r\n        )\r\n    }\r\n\r\n    const componentSirius = (paso) => {\r\n        if (paso === 1) {\r\n            return (\r\n                <>\r\n                    <Formik\r\n                        initialValues={{\r\n                            codigoSirius: '',\r\n                            origenRadicado: ''\r\n                        }}\r\n                        validate={(valores) => {\r\n                            let errores = {}\r\n                            if (!valores.codigoSirius) {\r\n                                errores.codigoSirius = 'Debe ingresar un código de SIRIUS'\r\n                            }\r\n                            if (!valores.origenRadicado) {\r\n                                errores.origenRadicado = 'Debe seleccionar un origen'\r\n 