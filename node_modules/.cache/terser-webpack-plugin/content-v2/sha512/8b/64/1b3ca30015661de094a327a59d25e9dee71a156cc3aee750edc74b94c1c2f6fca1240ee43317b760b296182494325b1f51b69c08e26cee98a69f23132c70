ADA_PDF_DEFINITIVO","ACTUALIZACION_DOCUMENTO","SOLICITUD_INACTIVACION_ACEPTADA","SOLICITUD_INACTIVACION_RECHAZADA","DOCUMENTO_FIRMADO","ACTUACION_INACTIVADA","REMITIDA","CAMBIO_ETAPA","CAMBIO_LISTA_ACTUACIONES_INACTIVAR","SOLICITUD_ACTIVACION","SOLICITUD_ACTIVACION_ACEPTADA","SOLICITUD_ACTIVACION_RECHAZADA","ESTADO_ACTUACION","TIPO_ARCHIVO_ACTUACIONES","DOCUMENTO_INICIAL","DOCUMENTO_DEFINITIVO","VISIBILIDAD","VISIBLE_TODOS","VISIBLE_DEPENDENCIA","VISIBLE_PARA_MI_Y_JEFE","OCULTO_TODOS","baseUrl","process","headers","relativeUrl","url","getBearerToken","console","log","axios","get","response","catch","handleError","getAllGeneric","serviceName","getByIdGeneric","getByDataGeneric","JSON","stringify","post","put","Spinner","style1","mensajeModal","mensajeCargando","ModalGen","props","navigate","useNavigate","setShow","handleClose","getAlerta","id_alerta","InfoErrorApi"],"mappings":";;;;;AAAA,oBAAoB,mBAAO,CAAC,EAAoB;AAChD;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,KAAK;AACL,GAAG;AACH;AACA;AACA;AACA;AACA,+G;;;;;;;ACfA;AACA;AACA;AACA,+G;;;;;;;ACHA;AACA;AACA;AACA,gH;;;;;;;ACHA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,OAAO,YAAY,6EAA6E;AAChG;AACA;AACA,KAAK;AACL;AACA,KAAK;AACL;AACA;AACA,OAAO;AACP;AACA;AACA;AACA;AACA;AACA;AACA,qH;;;;;;;;;;;;;;;;;;;;;;;;;;AClBe,QAASA,wBAAuB,EAAG,CAE9C;AACA,GAAMC,SAAQ,CAAGC,4EAAW,EAAE,CAC9B,GAAQC,KAAI,CAAKF,QAAQ,CAACG,KAAK,CAAvBD,IAAI,CAEZ;AACA,cAAgCE,sDAAQ,CAAC,EAAE,CAAC,wKAArCC,QAAQ,eAAEC,WAAW,eAC5B,eAAuCF,sDAAQ,CAAC,CAAEG,KAAK,CAAE,EAAE,CAAEC,OAAO,CAAE,EAAE,CAAEC,IAAI,CAAE,KAAM,CAAC,CAAC,yKAAjFC,aAAa,eAAEC,aAAa,eAEnC;AACA,eAAmCP,sDAAQ,EAAE,yKAAtCQ,WAAW,eAAEC,WAAW,eAC/B,eAAyCT,sDAAQ,CAAC,EAAE,CAAC,yKAA9CU,cAAc,eAAEC,cAAc,eACrC,eAAqDX,sDAAQ,CAAC,KAAK,CAAC,0KAA7DY,oBAAoB,gBAAEC,oBAAoB,gBACjD,gBAAyDb,sDAAQ,CAAC,KAAK,CAAC,2KAAjEc,sBAAsB,gBAAEC,sBAAsB,gBAErD;AACA,gBAAmEf,sDAAQ,CAAC,EAAE,CAAC,2KAAxEgB,2BAA2B,gBAAEC,2BAA2B,gBAC/D,gBAAuDjB,sDAAQ,CAAC,EAAE,CAAC,2KAA5DkB,qBAAqB,gBAAEC,qBAAqB,gBAEnD;AACA,gBAA2CnB,sDAAQ,CAAC,EAAE,C