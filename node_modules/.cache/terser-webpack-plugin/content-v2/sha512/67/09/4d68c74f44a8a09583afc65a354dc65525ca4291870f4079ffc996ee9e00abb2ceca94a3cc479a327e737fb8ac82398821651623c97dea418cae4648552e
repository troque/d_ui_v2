if (!datos.error) {\r\n\r\n                    // Se valida que haya informacion del usuario\r\n                    if (datos.data[0]) {\r\n\r\n                        // Se redeclara en true\r\n                        setExistenDatosUsuario(true);\r\n\r\n                        // Se redeclara los datos del usuario\r\n                        setDatosUsuario(datos.data[0]);\r\n                    } else {\r\n\r\n                        // Se redeclara en false\r\n                        setExistenDatosUsuario(false);\r\n                    }\r\n                }\r\n\r\n                // Se quita el cargando\r\n                window.showSpinner(false);\r\n            }\r\n        )\r\n    }\r\n\r\n\r\n    return (\r\n        <>\r\n            {<Spinner />}\r\n            <div className=\"row\">\r\n                <div className=\"col-md-12\">\r\n                    <div className=\"block block-themed\">\r\n                        <div className=\"block-header\">\r\n                            <h3 className=\"block-title\">Perfil Usuario</h3>\r\n                        </div>\r\n                        <div className=\"block-content\">\r\n                            <div className=\"row\">\r\n                                <div className=\"col-md-6 mb-2\">\r\n                                    <span className=\"txt-blue-primary\"><strong >Nombre: </strong></span>\r\n                                    {getExistenDatosUsuario == true ?\r\n                                        getDatosUsuario.attributes ? getDatosUsuario.attributes.nombre : null\r\n                                        : null}\r\n                                </div>\r\n                                <div className=\"col-md-6 mb-2\">\r\n                                    <span className=\"txt-blue-primary\"><strong >Apellido: </strong></span>\r\n                                    {getExistenDatosUsuario == true ?\r\n                                        getDatosUsuario.attributes ? getDatosUsuario.attributes.apellido : null\r\n                                        : null}\r\n                                </div>\r\n                                <div className=\"col-md-6 mb-2\">\r\n                                    <span className=\"txt-blue-primary\"><strong >Usuario: </strong></span>\r\n                                    {getExistenDatosUsuario == true ?\r\n                                        getDatosUsuario.attributes ? getDatosUsuario.attributes.name : null\r\n                                        : null}\r\n                                </div>\r\n                                <div className=\"col-md-6 mb-4\">\r\n                                    <span className=\"txt-blue-primary\"><strong >Correo: </strong></span>\r\n                                    {getExistenDatosUsuario == true ?\r\n                                        getDatosUsuario.attributes ? getDatosUsuario.attributes.email : null\r\n                                        : null}\r\n                                </div>\r\n\r\n                                <div className=\"col-md-6 my-2\">\r\n                                    <Link to={`/DocumentosFirmadosOPendientesDeFirma/`}>\r\n                                        <button type=\"button\" className=\"btn btn-rounded btn-primary\">\r\n                                            Ver documentos firmados o pendientes de firma\r\n                                        </button>\r\n                                    </Link>\r\n\r\n                                </div>\r\n\r\n                                <div className=\"col-md-6 my-2\">\r\n                                    <Link to={`/AgregarFirma/`}>\r\n                                        <button type=\"button\" className=\"btn btn-rounded btn-primary\">\r\n                                            {getExistenDatosUsuario == true ?\r\n                                                getDatosUsuario.attributes ? getDatosUsuario.attributes.firma_mecanica ? \"Cambiar\" : \"Agregar\" : null\r\n                                                : null\r\n                                            } firma mecánica\r\n                                        </button>\r\n                                    </Link>\r\n                                </div>\r\n\r\n\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </>\r\n    )\r\n\r\n}\r\n\r\nexport default Perfil;","import axios from 'axios'\r\nimport { getBearerToken, handleError } from '../../../components/Utils/Common';\r\n\r\nconst baseUrl = process.env.REACT_APP_API_URL;\r\n\r\nconst headers = {\r\n    'Content-Type': 'application/vnd.api+json',\r\n    'Accept': 'application/vnd.api+json'\r\n}\r\n\r\nasync function getGeneric(relativeUrl) {\r\n    const url = baseUrl + relativeUrl;\r\n    headers['Authorization'] = getBearerToken();\r\n\r\n    console.log(url);\r\n\r\n    return await axios.get(url, {\r\n        headers: headers\r\n    })\r\n        .then(response => {\r\n            return response.data;\r\n        })\r\n        .catch(error => {\r\n            handleError(error);\r\n            return { estado: false, error: error }\r\n        });\r\n}\r\n\r\nasync function getAllGeneric(serviceName) {\r\n    const url = serviceName;\r\n    return getGeneric(url);\r\n}\r\n\r\nasync function getByIdGeneric(serviceName, id) {\r\n    const url = serviceName + \"/\" + id;\r\n    return getGeneric(url);\r\n}\r\n\r\nasync function getByDataGeneric(serviceName, data) {\r\n\r\n    const url = baseUrl + serviceName;\r\n    headers['Authorization'] = getBearerToken();\r\n\r\n    console.log(url);\r\n    console.log(JSON.stringify(data));\r\n\r\n    return await axios.post(url, data, {\r\n        headers: headers\r\n    })\r\n        .then(response => {\r\n            return response.data;\r\n        })\r\n        .catch(error => {\r\n            handleError(error);\r\n            return { estado: false, error: error }\r\n        });\r\n}\r\n\r\nasync function addGeneric(serviceName, data) {\r\n    const url = baseUrl + serviceName;\r\n    headers['Authorization'] = getBearerToken();\r\n\r\n    console.log(url);\r\n    console.log(JSON.stringify(data));\r\n\r\n    return await axios.post(url, data, {\r\n        headers: headers\r\n    })\r\n        .then(response => {\r\n            return response.data;\r\n        })\r\n        .catch(error => {\r\n            handleError(error);\r\n            return { estado: false, error: error }\r\n        });\r\n}\r\n\r\nasync function updateGeneric(serviceName, id, data) {\r\n\r\n    const url = baseUrl + serviceName + \"/\" + id;\r\n    headers['Authorization'] = getBearerToken();\r\n\r\n    console.log(url);\r\n    console.log(JSON.stringify(data));\r\n\r\n    return await axios.put(url, data, {\r\n        headers: headers\r\n    })\r\n        .then(response => {\r\n            return response.data;\r\n        })\r\n        .catch(error => {\r\n            handleError(error);\r\n            return { estado: false, error: error }\r\n        });\r\n}\r\n\r\n\r\nexport default { getGeneric, getAllGeneric, getByIdGeneric, addGeneric, updateGeneric, getByDataGeneric }\r\n","function Spinner() {\r\n\r\n    const style1 = {\r\n        \"display\": \"flex\"\r\n    }\r\n\r\n    return (\r\n        <div className=\"modal\" id=\"modal-block-spinner\" tabIndex=\"-1\" role=\"dialog\" aria-labelledby=\"modal-block-spinner\" aria-hidden=\"true\" data-keyboard=\"false\" data-backdrop=\"static\">\r\n            <div className=\"modal-dialog modal-dialog-centered\" role=\"document\">\r\n                <div className=\"modal-content\">\r\n                    <div className=\"block block-themed block-transparent mb-0\">                        \r\n                        <div className=\"block-content\">\r\n                            <div className=\"row\">\r\n                                <div className=\"swal2-icon swal2-warning swal2-animate-warning-icon\" style={style1}></div>\r\n                            </div>\r\n                            <div className=\"row-w2d\">\r\n                                <h3 className=\"text-center\">Cargando la información, espera un momento...</h3>\r\n                            </div>\r\n                            <div className=\"text-center\">\r\n                                <div className=\"spinner-border text-primary\" role=\"status\">\r\n                                    <span className=\"sr-only\">Cargando...</span>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n\r\n//Cargando la información, espera un momento...\r\n\r\nexport default Spinner;\r\n"]}}