bleCustomDt,\r\n            state: {\r\n                userInput\r\n            }\r\n        } = this;\r\n\r\n        return (\r\n            <Fragment>\r\n\r\n\r\n                <DatePicker locale='es' dateFormat=\"DD/MM/YYYY\"\r\n                    closeOnSelect={true} placeholder=\"dd/mm/yyyy\"\r\n                    timeFormat={false}\r\n                    onChange={e => onChange(e)}\r\n                    isValidDate={disableCustomDt}\r\n                    value={userInput} />\r\n            </Fragment>\r\n        );\r\n    }\r\n}\r\n\r\nexport default DatePerson;","\r\nexport default global.Constants = {\r\n    ESTADOS: {\r\n        ACTIVO: '1',\r\n        INACTIVO: '0'\r\n    },\r\n\r\n    //MIS PENDIENTES\r\n    RANGOS: {\r\n        TODOS: '-1'\r\n    },\r\n\r\n    SEMAFORIZACION_FASES: {\r\n        GREEN: 'far fa-dot-circle txt-green',\r\n        RED: 'far fa-dot-circle txt-red',\r\n        ORANGE: 'far fa-dot-circle txt-orange'\r\n    },\r\n\r\n    BOTONES_CARPETAS: {\r\n        PRIMARY: 'btn btn-sm btn-primary mr-1 mb-3',\r\n        SUCCESS: 'btn btn-sm btn-success mr-1 mb-3',\r\n    },\r\n\r\n    COLOR_FASE: {\r\n        COMPLETA: '#8BCD50',\r\n    },\r\n\r\n    //PARA LOS TEXT_AREA\r\n    TEXT_AREA: {\r\n        CARACTERES_ESPECIALES: `^[]{}'|<>~`,\r\n        //CARACTERES_ESPECIALES: `\\`!@#$%^&*()_+\\-=\\[\\]{};':\"\\\\|<>\\/?~`,\r\n        CANTIDAD_MINIMA_DESCRIPCION: 100 //Cantidad mnima que se muestra en las descripciones\r\n    },\r\n\r\n    //CARACTERES ESECIALES\r\n    CARACTERES_ESPECIALES: {\r\n        formatOnlyLetters: /^[A-Za-z\\s]+$/,\r\n        formatOnlyLettersWhitAccent: /^[a-zA-Z0-9]+$/,\r\n        formatOnlyLettersOrnUmbers: /^[0-9a-zA-Z\\s]+$/,\r\n        formatWithOutSpecials: /^[A-Za-z0-9#-\\s]+$/g\r\n    },\r\n\r\n    //ETAPAS\r\n    ETAPAS: {\r\n        NINGUNA: 0, // SE ASIGNO PARA LA PARTE ADMISNITRATIVA - QUE CIERTAS FUNCIUONALIDADES SE MOSTRARAN INDIFERNTES A LA ETAPA EN LA QUE EST\r\n        CAPTURA_REPARTO: 1,\r\n      