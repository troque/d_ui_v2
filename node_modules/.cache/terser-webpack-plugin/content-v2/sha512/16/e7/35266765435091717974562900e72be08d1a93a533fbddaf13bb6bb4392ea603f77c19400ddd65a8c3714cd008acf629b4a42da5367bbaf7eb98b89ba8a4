n            sortable: true,\r\n            width: \"65%\"\r\n        },\r\n        {\r\n            name: 'CÓDIGO DANE',\r\n            selector: departamento => departamento.attributes.codigo_dane,\r\n            sortable: true,\r\n            width: \"15%\"\r\n        },\r\n        {\r\n            name: 'ESTADO',\r\n            selector: departamento => (departamento.attributes.estado == \"1\" ? 'ACTIVO' : 'INACTIVO'),\r\n            sortable: true,\r\n            width: \"10%\"\r\n        },\r\n        {\r\n            name: 'ACCIONES',\r\n\r\n            cell: row => <div><Link to={`${row.id}`}>\r\n                <button type=\"button\" className=\"btn btn btn-primary\" title='Editar'>\r\n                    <i className=\"fa fa-fw fa-edit\"></i>\r\n                </button>\r\n            </Link></div>\r\n            ,width: \"10%\"\r\n        } \r\n    ];\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n            cargarDepartamentos(1);\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n\r\n    const cargarDepartamentos = (estado) => {\r\n        window.showSpinner(true);\r\n        GenericApi.getAllGeneric('getDepartamentos/'+estado).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setDepartamentoLista(datos);\r\n                    window.showSpinner(false);\r\n                }else {\r\n                    window.showSpinner(false);\r\n                    setModalState({ title: \"LISTA DE DEPARTAMENTOS :: ERROR\", message: datos.error.toString(), show: true, alert: global.Constants.TIPO_ALERTA.ERROR, button: true });\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const handlePageChange = page => {\r\n        setPageActual(page);\r\n    }\r\n\r\n    const handlePerRowsChange = async (newPerPage, page) => {\r\n        setPerPage(newPerPage);\r\n        setPageActual(page);\r\n    }\r\n\r\n    return (\r\n        <div>\r\n\r\n            <Spinner />\r\n            <ModalInfo data={getModalState} />\r\n\r\n            <div className=\"col-md-12\">\r\n                <div className=\"w2d_block let\">\r\n                    <nav aria-label=\"breadcrumb\">\r\n                        <ol className=\"breadcrumb breadcrumb-alt push\">\r\n                            <li className=\"breadcrumb-item\"> <small>Administrador</small></li>\r\n                            <li className=\"breadcrumb-item\"> <small>Otros</small></li>\r\n                            <li className=\"breadcrumb-item\"> <small>Localización</small></li>\r\n                            <li className=\"breadcrumb-item\"> <small>Departamentos</small></li>\r\n                        </ol>\r\n                    </nav>\r\n                </div>\r\n            </div>\r\n            \r\n            <div className=\"block block-themed\">\r\n                <div className=\"block-header\">\r\n                    <h3 className=\"block-title\">ADMINISTRACIÓN :: DEPARTAMENTOS {getEstadoLista == global.Constants.ESTADOS.INACTIVO ? \"INACTIVOS\" : \"ACTIVOS\"}</h3>\r\n                </div>\r\n                <div className=\"block-content\">\r\n                    <div className='row'>\r\n                        <div className='col-md-3'>\r\n                            <div className=\"form-group \">\r\n                                <input type=\"text\" id=\"search\" name=\"search\" value={getSeach} onChange={e => setSeach(e.target.value)} onKeyPress={(e) => { e.key === 'Enter' && e.preventDefault(); }} className=\"form-control border border-success\" placeholder=\"Buscar\" />\r\n                            </div>\r\n                        </div>\r\n                        <ListaBotones getRoutes={getRoutes} parentCallback={handleCallback} id=\"botonesNavegacion\" name=\"botonesNavegacion\" />\r\n                       \r\n                    </div>\r\n                    <div className=\"row\">\r\n                        <div className=\"col-sm-12\">\r\n                            <DataTable className=\"table table-bordered table-striped table-vcenter js-dataTable-full\"\r\n                                columns={columns}\r\n                                data={departamentoLista.data.filter((suggestion) => {\r\n                                    if (getSeach === \"\") {\r\n                                        if(suggestion.attributes.estado == getEstadoLista){\r\n                                            return suggestion;\r\n                                        }\r\n\t\t\t\t\t\t\t\t\t\t\r\n                                    } else if (\r\n                                        ((suggestion.id\r\n                                            + quitarAcentos(suggestion.attributes.nombre)\r\n                                            + suggestion.attributes.codigo_dane\r\n                                            + (suggestion.attributes.estado == \"1\" ? 'ACTIVO' : 'INACTIVO')).toLowerCase().includes(quitarAcentos(getSeach.toLowerCase()))\r\n                                            && (suggestion.attributes.estado == getEstadoLista))) {\r\n                                        return suggestion;\r\n                                    }\r\n                                })}\r\n                                perPage={perPage}\r\n                                page={pageActual}\r\n                                pagination\r\n                                noDataComponent=\"Sin datos\"\r\n                                paginationTotalRows={departamentoLista.data.length}\r\n                                onChangePage={handlePageChange}\r\n                                onChangeRowsPerPage={handlePerRowsChange}\r\n                                defaultSortFieldId=\"Nombre\"\r\n                                striped\r\n                                paginationComponentOptions={{ rowsPerPageText: 'Filas por página' }}\r\n                                defaultSortAsc={false}\r\n                            />\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default DepartamentoLista;"]}}