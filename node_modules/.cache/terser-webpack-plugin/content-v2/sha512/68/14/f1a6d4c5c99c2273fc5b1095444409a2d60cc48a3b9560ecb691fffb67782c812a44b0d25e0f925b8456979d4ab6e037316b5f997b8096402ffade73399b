}\r\n\r\n    };\r\n\r\n\r\n    const handleInputChange = (event, i) => {\r\n     \r\n        if(i===1){\r\n            setNombres(event.currentTarget.value);\r\n        }\r\n\r\n        else if(i===2){\r\n            setApellidos(event.currentTarget.value)\r\n        }\r\n\r\n        else if(i===3){\r\n            setTipoIdentificacion(event.currentTarget.value)\r\n        }\r\n\r\n        else if(i===4){\r\n            setIdentificacion(event.currentTarget.value)\r\n        }\r\n\r\n        else if(i===5){\r\n            setTipoUsuario(event.currentTarget.value)\r\n        }\r\n\r\n        else if(i===6){\r\n            setGenero(event.currentTarget.value)\r\n        }\r\n\r\n        return true;\r\n\r\n        //return true;\r\n    }\r\n\r\n\r\n\r\n    const infoInteresado = () => {\r\n\r\n        if (getLista.data != null && typeof (getLista.data) != 'undefined') {\r\n            return (\r\n\r\n                getLista.data.map((item, i) => {\r\n                    return (\r\n                        <>\r\n                        {/* NOMBRES */}   \r\n                        <div className=\"col-md-5\">\r\n                            <div className=\"form-group\">\r\n                                <label htmlFor=\"interesado_nombres_origen\">Nombres</label>\r\n                                <Field value={item.attributes.interesado} as=\"input\" type=\"text\" autoComplete=\"off\" className=\"form-control\" id=\"interesado_nombres_origen\" name=\"interesado_nombres_origen\"></Field>\r\n                            </div>\r\n                        </div>\r\n                        \r\n                        <div className=\"col-md-2\">\r\n                            <div className=\"form-group\">\r\n                                <Center>\r\n                                    <button type=\"button\" className=\"btn btn-success\" onClick={event => handleClick(event, 1)}><i class=\"fas fa-angle-double-right\"></i></button>\r\n                                </Ce