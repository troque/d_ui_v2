ion, setIdentificacion] = useState('');\r\n    const [tipoUsuario, setTipoUsuario] = useState('');\r\n    const [genero, setGenero] = useState('');\r\n\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n            window.showSpinner(true);\r\n            lista();\r\n            \r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n    \r\n    function containsSpecialChars(str) {\r\n\r\n        const result = global.Constants.TEXT_AREA.CARACTERES_ESPECIALES.split('').some(specialChar => {\r\n            if (str.includes(specialChar)) {\r\n                return true;\r\n            }\r\n\r\n            return false;\r\n        });\r\n\r\n        return result;\r\n    }\r\n\r\n    const lista= () => {\r\n        try {\r\n\r\n            //buscamos el parametro\r\n            GenericApi.getGeneric(\"migracion-interesado/\"+radicado+\"/\"+version+\"/\"+id+\"/\"+item).then(\r\n                datos => {\r\n\r\n                    if (!datos.error) {\r\n                        setLista(datos);\r\n                        setExisteInformacion(true);\r\n                        obtenerParametros();\r\n                    } else {\r\n                        setModalState({ title: \"Antecedentes\", message: datos.error.toString(), show: true, redirect: '/AntecedentesForm', alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                    }\r\n                   \r\n                }\r\n            )\r\n        } catch (error) {\r\n            // console.log(error);\r\n        }\r\n    }\r\n\r\n    const obtenerParametros = () => {\r\n        try {\r\n\r\n            const data = {\r\n                \"data\": {\r\n                    \"type\": 'mas_parametro',\r\n                    \"attributes\": {\r\n                        \"nombre\": \"minimo_caracteres_textarea|maximo_caracteres_textarea\"\r\n                    }\r\n                }\r\n            }\r\n\r\n            //buscamos el parametro\r\n            GenericApi.getByDataGeneric(\"par