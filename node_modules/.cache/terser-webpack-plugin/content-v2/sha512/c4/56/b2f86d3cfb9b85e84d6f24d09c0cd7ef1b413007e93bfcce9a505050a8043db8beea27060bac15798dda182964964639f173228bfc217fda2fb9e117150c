r\n    const [getModalState, setModalState] = useState({ title: \"\", message: \"\", show: false });\r\n    const [getListaSemaforos, setListaSemaforos] = useState({ data: [] });\r\n    const [ConfirmarListaSemaforos, SetConfirmarListaSemaforos] = useState(0);\r\n\r\n    const location = useLocation();\r\n    const { from } = location.state;\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n            window.showSpinner(true);\r\n            GenericApi.getByIdGeneric('getSemaforosPorProceso', from.procesoDisciplinarioId).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        datos.data.forEach(element => {\r\n                            GenericApi.getByIdGeneric('semaforo', element.attributes.id_semaforo.id).then(\r\n                                datosSemaforo => {\r\n                                    if (!datosSemaforo.error) {\r\n                                        getListaSemaforos.data.push(datosSemaforo.data);\r\n                                        SetConfirmarListaSemaforos(getListaSemaforos.data.length);\r\n                                    }else{\r\n                                        setModalState({ title: \"Lista de Semáforos\", message: datos.error.toString(), show: true, redirect: '/MisPendientes', alert: global.Constants.TIPO_ALERTA.ERROR});\r\n                                    }\r\n                                    \r\n                                }\r\n                            )\r\n                        });\r\n                        window.showSpinner(false);\r\n                    }else{\r\n                        setModalState({ title: \"Lista de Semáforos\", message: datos.error.toString(), show: true, redirect: '/MisPendientes', alert: global.Constants.TIPO_ALERTA.ERROR});\r\n                    }\r\n                }\r\n            )\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n    const handlePageChange = page => {\r\n        setPageActual(page);\r\n    }\r\n\r\n    const handlePerRowsChange = async (newPerPage, page) => {\r\n        setPerPage(newPerPage);\r\n        setPageActual(page);\r\n    }\r\n\r\n    console.log(getListaSemaforos);\r\n    const columns = [\r\n        {\r\n            name: 'Nombre',\r\n            selector: semaforo => semaforo.attributes.nombre,\r\n            sortable: true\r\n        },\r\n        {\r\n            name: 'Evento de inicio',\r\n            selector: semaforo => semaforo.attributes.id_mas_evento_inicio.nombre,\r\n            sortable: true\r\n        },\r\n        {\r\n            name: 'Actuación que inicia el semáforo',\r\n            selector: semaforo => semaforo.attributes.id_mas_actuacion_inicia ? semaforo.attributes.id_mas_actuacion_inicia.nombre_actuacion : null,\r\n            sortable: true\r\n        },\r\n        {\r\n            name: 'Días',\r\n            selector: semaforo => semaforo.id,\r\n            sortable: true\r\n        },\r\n        {\r\n            name: 'Color',\r\n            selector: semaforo => semaforo.id,\r\n            sortable: true\r\n        }\r\n    ];\r\n\r\n    return (\r\n        <>\r\n            {<Spinner />}\r\n            {<ModalGen data={getModalState} />}\r\n            <Formik\r\n                initialValues={{\r\n                    rango: ''\r\n                }}\r\n                enableReinitialize\r\n                validate={(valores) => {\r\n                    let errores = {}\r\n                    return errores\r\n                }}\r\n                onSubmit={(valores, { resetForm }) => {\r\n                }}\r\n            >\r\n\r\n                {({ errors }) => (\r\n                    <Form>\r\n\r\n                        <div className=\"w2d_block\">\r\n                            <nav aria-label=\"breadcrumb\">\r\n                                <ol className=\"breadcrumb breadcrumb-alt push\">\r\n                                    <li className=\"breadcrumb-item\"> <Link underl