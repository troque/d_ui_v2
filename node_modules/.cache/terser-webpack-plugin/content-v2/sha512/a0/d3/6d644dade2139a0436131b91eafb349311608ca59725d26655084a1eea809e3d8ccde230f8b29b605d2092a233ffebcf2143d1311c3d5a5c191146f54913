uros){\r\n            return (!resultDiasNoLaborales.includes(current.format('YYYY-MM-DD')) && moment(current).isAfter(pastDate) && moment(current).isBefore(new Date()));\r\n        }\r\n        else{\r\n            return (!resultDiasNoLaborales.includes(current.format('YYYY-MM-DD')) && moment(current).isAfter(pastDate));\r\n        }\r\n\r\n        \r\n    }\r\n    \r\n    const columns = [\r\n        {\r\n            name: 'Nombre',\r\n            selector: semaforo => <spam title={semaforo.attributes.pdxsemaforo.attributes.id_semaforo.nombre.toUpperCase()}>{semaforo.attributes.pdxsemaforo.attributes.id_semaforo.nombre.toUpperCase()}</spam>,\r\n            sortable: true,\r\n            width: \"24  %\"\r\n        },\r\n        {\r\n            name: 'Fecha de inicio',\r\n            selector: semaforo => <spam>{\r\n                (\r\n                    <Formik\r\n                    initialValues={{\r\n                        rango: ''\r\n                    }}\r\n                    enableReinitialize\r\n                    validate={(valores) => {\r\n                        let errores = {}\r\n                        return errores\r\n                    }}\r\n                    onSubmit={(valores, { resetForm }) => {\r\n                    }}\r\n                >\r\n\r\n                    {({ errors }) => (\r\n                        \r\n                        <Form>\r\n                                <div className=\"form-group\" id=\"contenedorFechaSemaforo\"> \r\n                                    <DatePicker \r\n                                        id=\"fechaInicioSemaforo\" \r\n                                        locale='es' \r\n                                        name=\"fechaInicioSemaforo\" \r\n                                        dateFormat=\"YYYY-MM-DD\" \r\n                                        closeOnSelect={true} \r\n                                        placeholder=\"yyyy-mm-dd\" \r\n                                      