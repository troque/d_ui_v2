r\n                        ? \r\n                            <i className=\"fas fa-solid fa-eye\" title='Visible en el portal'></i>\r\n                        : \r\n                        null\r\n                    }\r\n                </div >,\r\n            width: \"8%\"\r\n        }\r\n    ];\r\n\r\n    const handleClicArchivo = (id_documento, extension, nombre_documento) => {\r\n\r\n        // Se usa el cargando\r\n        window.showSpinner(true);\r\n\r\n        try {\r\n\r\n            // Se consume la API\r\n            GenericApi.getGeneric(\"archivo-actuaciones/get-documento/\" + id_documento + \"/\" + extension).then(\r\n\r\n                // Se inicializa la variable de respuesta\r\n                datos => {\r\n\r\n                    // Se valida que no haya error\r\n                    if (!datos.error) {\r\n\r\n                        // Se manda al metodo para generar el pdf\r\n                        downloadBase64File(datos.content_type, datos.base_64, nombre_documento);\r\n                    } else {\r\n\r\n                        // Se quita el cargando\r\n                        window.showSpinner(false);\r\n\r\n                        setErrorApi(datos.error.toString())\r\n                        window.showModal(1)\r\n                    }\r\n                }\r\n            )\r\n        } catch (error) {\r\n            console.error(error);\r\n        }\r\n    };\r\n\r\n    function downloadBase64File(contentType, base64Data, fileName) {\r\n\r\n        // Se inicializan las constantes del documento en base 64\r\n        const linkSource = `data:${contentType};base64,${base64Data}`;\r\n        const downloadLink = document.createElement(\"a\");\r\n\r\n        // Se crear el open file en una new tab\r\n        downloadLink.href = linkSource;\r\n        downloadLink.download = fileName;\r\n        downloadLink.click();\r\n\r\n        // Se quita el cargando\r\n        window.showSpinner(false);\r\n    }\r\n\r\n    const redirectToRoutes = () => {\r\n        return <Navigate to={`/ActuacionesLista/`} state={{ from: from, selected_id_etapa: selected_id_etapa }} />;\r\n    }\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n            window.showSpinner(true);\r\n            setEstadoLista(\"Activos\");\r\n            nombreProceso();\r\n            cargarUsuarioComisionado();\r\n            cargarFuncionarioAsignado();\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n\r\n\r\n    const nombreProceso = () => {\r\n        GenericApi.getByIdGeneric(\"nombre-proceso\", from.procesoDisciplinarioId).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setNombreProceso(datos.data.attributes.nombre);\r\n                    validarPermisosCrearActuaciones();\r\n                    cargarActuaciones(1, paganationPerPages, global.Constants.ESTADOS.ACTIVO);\r\n                    obtenerDatosDeLaEtapaSeleccionada();\r\n                }\r\n            }\r\n        )\r\n    }\r\n    const cargarFuncionarioAsignado = () => {\r\n        GenericApi.getByIdGeneric(\"proceso-diciplinario\", procesoDisciplinarioId).then(\r\n\r\n            // Se inicializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n\r\n                    // Se valida que haya una dependencia dueña del proceso\r\n                    if (datos.data.attributes.dependencia_duena != null) {\r\n\r\n                        // Se consume la API\r\n                        GenericApi.getByIdGeneric(\"usuario\", datos.data.attributes.dependencia_duena.id_usuario_jefe).then(\r\n\r\n                            // Se inicializa la variable de respuesta\r\n                            datos => {\r\n\r\n                                // Se valida que no haya error\r\n                                if (!datos.error) {\r\n\r\n                                    // Se setea la informacion\r\n                                    setFuncionarioAsignado(datos.data.attributes.email);\r\n                                }\r\n                            }\r\n                        )\r\n                    } else {\r\n                        window.showSpinner(false);\r\n                    }\r\n                } else {\r\n                    window.showSpinner(false);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const cargarUsuarioComisionado = (page, perPage, estado) => {\r\n        GenericApi.getGeneric('actuaciones/get-usuario-comisionado/' + procesoDisciplinarioId).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    console.log(\"USuario COmisionado\");\r\n                    console.log(datos);\r\n                    setUsuarioComisionado(datos);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    // Metodo encargado de cargar las actuaciones por estado activo o inactivo\r\n    const cargarActuaciones = (page, perPage, estado) => {\r\n\r\n        // Se activa el cargando\r\n        window.showSpinner(true);\r\n\r\n        // Se consume la API\r\n        GenericApi.getGeneric('actuaciones/get-actuaciones-discipl-etapa-documento-final/' + procesoDisciplinarioId + '/' + selected_id_etapa + '/' + estado).then(\r\n\r\n            // Se inicializa la variable de respuesta\r\n            datos => {\r\n\r\n                // Se valida que no haya error\r\n                if (!datos.error) {\r\n                    \r\n                    // Se setean los datos\r\n                    setActuacionesListaSearch(datos);\r\n                    \r\n                    if(datos.data[datos.data.length - 1].attributes.actuacion.attributes.nombre_estado_actuacion == global.Constants.ESTADOS_ACTUACION.APROBADA_PDF_DEFINITIVO){\r\n                        from.impedimentoAprobado = true;\r\n                    }\r\n                    else{\r\n                        from.impedimentoAprobado = false;\r\n                    }\r\n\r\n                    // Se valida que haya data\r\n                    if (datos[\"data\"] != \"\") {\r\n\r\n                        // Se obtiene los datos de la ultima actuacion y se setea\r\n                        obtenerLaUltimaActuacion(datos.data[0].attributes.actuacion.attributes.id_estado_actuacion, datos.data[0].attributes.actuacion.attributes.id_actuacion);\r\n                    } else {\r\n\r\n                        // Se realiza la validacion para el boton de agregar con usuario comisionado\r\n                        validacionDeBotonAgregar(0, 0, 0, getLaDependenciaDelUsuarioTieneUnComisorioAprobado);\r\n                    }\r\n                } else {\r\n\r\n                    // Se setea el error\r\n                    setErrorApi(datos.error.toString());\r\n\r\n                    // Se muestra el modal\r\n                    window.showModal(1);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const obtenerLaUltimaActuacion = (estado, id) => {\r\n        GenericApi.getByIdGeneric('mas_actuaciones', id).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    if (datos[\"data\"] != \"\") {\r\n                        // console.log(datos);\r\n                        validacionDeBotonAgregar(datos.data.attributes.tipo_actuacion, estado, datos.data.attributes.excluyente, getLaDependenciaDelUsuarioTieneUnComisorioAprobado);\r\n                    }\r\n                } else {\r\n                    window.showModal(1);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const obtenerDatosDeLaEtapaSeleccionada = () => {\r\n        GenericApi.getGeneric('mas-etapa/' + selected_id_etapa).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setNombreEtapaSeleccionada(datos.data.attributes.nombre);\r\n                } else {\r\n                    // console.log(\"Error en la consulta de obtener los datos de la etapa seleccionada\")\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const handlePageChange = page => {\r\n\r\n        // Se usa el cargando\r\n        window.showSpinner(true);\r\n\r\n        // Se setea la pagina\r\n        setPageActual(page);\r\n\r\n        // Se cargan las actuaciones\r\n        cargarActuaciones(page, perPage , getEstadoLista === \"Inactivos\" ? 0 : 1);\r\n\r\n        // Se quita el cargando\r\n        window.showSpinner(false);\r\n    }\r\n\r\n    const handlePerRowsChange = async (newPerPage, page) => {\r\n\r\n        // Se usa el cargando\r\n        setPerPage(newPerPage);\r\n\r\n        // Se setea la pagina\r\n        setPageActual(page);\r\n\r\n        // Se cargan las actuaciones\r\n        cargarActuaciones(page, newPerPage , getEstadoLista === \"Inactivos\" ? 0 : 1);\r\n\r\n        // Se quita el cargando\r\n        window.showSpinner(false);\r\n    }\r\n\r\n    const validarPermisosCrearActuaciones = () => {\r\n        GenericApi.getGeneric('validar-permiso-crear-actuaciones').then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setPermisoActuaciones(datos?.data?.attributes?.crear_actuacion);\r\n                }\r\n            }\r\n        )\r\n    }\r\n\r\n    const validacionDeBotonAgregar = (tipo_actuacion, estado, excluyente) => {\r\n        GenericApi.getGeneric('actuaciones/dependencia-tiene-comisorio-aprobado/' + getUser().id_dependencia + '/' + from.procesoDisciplinarioId).then(\r\n            datos => {\r\n                if (tipo_actuacion == 0) {\r\n                    if (from.idEtapa == selected_id_etapa && (excluyente != 1 || estado != 3)) {\r\n                        setMostrarBotonAgregar(true);\r\n                    } else {\r\n                        setMostrarBotonAgregar(false);\r\n                    }\r\n                } else if (tipo_actuacion == 1) {\r\n\r\n                    if (datos == \"Si tiene\" || (hasAccess('SeleccionarDependenciaDuenaDelProceso', 'Gestionar'))) {\r\n                        setMostrarBotonAgregar(true);\r\n                    } else {\r\n                        setMostrarBotonAgregar(false);\r\n                    }\r\n\r\n                    // Se valida cuando el tipo es comisorio\r\n                } else if (tipo_actuacion == 2) {\r\n                    // Se valida que la etapa actual sea igual a la etapa seleccionada y que no sea excluyente y que sea diferente de pendiente de aprobacion\r\n                    if (from.idEtapa == selected_id_etapa && excluyente != 1 && estado != 3) {\r\n                        // Se habilita el boton de agregado\r\n                        setMostrarBotonAgregar(true);\r\n\r\n                        // Se valida que el estado rechazado\r\n                        if (estado == 2) {\r\n                            // Se setea la ruta en actuacion \r\n                            setRutaParametrizada(\"Comisorio\");\r\n                        } else {\r\n                            // Se setea la ruta en comisorio\r\n                            setRutaParametrizada(\"Actuación\");\r\n                        }\r\n                    } else {\r\n                        // Se deshabilita el boton de agregado\r\n                        setMostrarBotonAgregar(false);\r\n                    }\r\n\r\n                }\r\n\r\n                else {\r\n\r\n                    // Se habilita el boton de agregado\r\n                    setMostrarBotonAgregar(true);\r\n                }\r\n\r\n                // Se setean los botones generales\r\n                setmuestrameBotones(true);\r\n\r\n                // Se deshabilita el cargando\r\n                window.showSpinner(false);\r\n            }\r\n        )\r\n    }\r\n\r\n\r\n\r\n\r\n    return (\r\n        <>\r\n            {isRedirect ? redirectToRoutes() : null}\r\n            {<Spinner />}\r\n            {<InfoErrorApi error={errorApi} />}\r\n            {<InfoExitoApi />}\r\n          