amento']}\r\n        ddlServicesFunction={itemSelectResolve}\r\n        model={CiudadModel}\r\n        successRedirect='/Ciudad'\r\n    />\r\n    );\r\n};","\r\nexport default global.Constants = {\r\n    ESTADOS: {\r\n        ACTIVO: '1',\r\n        INACTIVO: '0'\r\n    },\r\n\r\n    //MIS PENDIENTES\r\n    RANGOS: {\r\n        TODOS: '-1'\r\n    },\r\n\r\n    BOTONES_RAMAS: {\r\n        PRIMARY: 'btn btn-sm btn-primary w2d_btn-large mr-1 mb-3 text-left',\r\n        SUCCESS: 'btn btn-sm btn-success w2d_btn-large mr-1 mb-3 text-left',\r\n        WARNING: 'btn btn-sm btn-warning w2d_btn-large mr-1 mb-3 text-left'\r\n    },\r\n\r\n    BOTONES_CARPETAS: {\r\n        PRIMARY: 'btn btn-sm btn-primary mr-1 mb-3',\r\n        SUCCESS: 'btn btn-sm btn-success mr-1 mb-3',\r\n    },\r\n\r\n    COLOR_FASE: {\r\n        COMPLETA: '#8BCD50'\r\n    },\r\n\r\n    //PARA LOS TEXT_AREA\r\n    TEXT_AREA: {\r\n        CARACTERES_ESPECIALES: `\\`^_\\[\\]{}'\\\\|<>\\/~`,\r\n        //CARACTERES_ESPECIALES: `\\`!@#$%^&*()_+\\-=\\[\\]{};':\"\\\\|<>\\/?~`,\r\n        CANTIDAD_MINIMA_DESCRIPCION: 100 //Cantidad mínima que se muestra en las descripciones\r\n    },\r\n\r\n    //CARACTERES ESECIALES\r\n    CARACTERES_ESPECIALES: {\r\n        formatOnlyLetters: /^[A-Za-z\\s]+$/,\r\n        formatOnlyLettersOrnUmbers: /^[0-9a-zA-Z\\s]+$/,\r\n        formatWithOutSpecials: /^[A-Za-z0-9#-\\s]+$/g\r\n    },\r\n\r\n    //ETAPAS\r\n    ETAPAS: {\r\n        NINGUNA: 0, // SE ASIGNO PARA LA PARTE ADMISNITRATIVA - QUE CIERTAS FUNCIUONALIDADES SE MOSTRARAN INDIFERNTES A LA ETAPA EN LA QUE ESTÁ\r\n        CAPTURA_REPARTO: 1,\r\n        EVALUACION: 2,\r\n        EVALUACION_PD: 3\r\n    },\r\n\r\n    //FASES\r\n    FASES: {\r\n        ANTECEDENTES: 1,\r\n        INTERESADO: 2,\r\n        CLASIFICACION: 3,\r\n        ENTIDAD_INVESTIGADO: 4,\r\n        SOPORTE_RADICADO: 5,\r\n\r\n        //evaluacion\r\n        VALIDAR_CLASIFICACION: '10',\r\n        EVALUACION: '11',\r\n        REMISION_QUEJA: '6',\r\n        