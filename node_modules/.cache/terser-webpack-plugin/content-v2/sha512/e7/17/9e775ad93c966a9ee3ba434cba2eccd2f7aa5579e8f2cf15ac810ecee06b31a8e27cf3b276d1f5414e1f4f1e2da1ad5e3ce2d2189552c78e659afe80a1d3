inner(true);\r\n\r\n            // Se genera la peticion para descargar el archivo\r\n            GenericApi.getGeneric(\"archivo-actuaciones/get-documento/\" + id_documento + \"/\" + extension).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        downloadBase64File(datos.content_type, datos.base_64, nombre_documento);\r\n                    } else {\r\n                        setErrorApi(datos.error.toString())\r\n                        window.showModal(1)\r\n                    }\r\n                    window.showSpinner(false);\r\n                }\r\n            )\r\n        } catch (error) {\r\n            console.error(error);\r\n        }\r\n    };\r\n\r\n    function downloadBase64File(contentType, base64Data, fileName) {\r\n        const linkSource = `data:${contentType};base64,${base64Data}`;\r\n        const downloadLink = document.createElement(\"a\");\r\n        downloadLink.href = linkSource;\r\n        downloadLink.download = fileName;\r\n        downloadLink.click();\r\n    }\r\n\r\n    const redirectToRoutes = () => {\r\n        return <Navigate to={`/ActuacionesLista/`} state={{ from: from, selected_id_etapa: selected_id_etapa }} />;\r\n    }\r\n\r\n    useEffect(() => {\r\n        async function fetchData() {\r\n            setEstadoLista(\"Activos\");\r\n            cargarActuaciones(1, paganationPerPages, global.Constants.ESTADOS.ACTIVO);\r\n            obtenerDatosDeLaEtapaSeleccionada();\r\n        }\r\n        fetchData();\r\n    }, []);\r\n\r\n\r\n    const cargarActuaciones = (page, perPage, estado) => {\r\n        window.showSpinner(true);\r\n        GenericApi.getGeneric('actuaciones/get-actuaciones-discipl-etapa-documento-final/' + procesoDisciplinarioId + '/' + selected_id_etapa + '/' + estado).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setActuacionesListaSearch(datos);\r\n                    if (datos[\"data\"] != \"\") {\r\n       