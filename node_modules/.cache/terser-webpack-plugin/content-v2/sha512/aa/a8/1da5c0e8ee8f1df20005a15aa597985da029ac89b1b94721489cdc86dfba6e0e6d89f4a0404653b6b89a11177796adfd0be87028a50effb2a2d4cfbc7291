ve double digit day\r\n        let month = (\"0\" + (result.getMonth() + 1)).slice(-2); //get your zero in front of single month digits so you have 2 digit months\r\n        let date = result.getFullYear() + '-' + month + '-' + day;\r\n        setFechaFiltro(date);\r\n\r\n        const data = {\r\n            \"data\": {\r\n                \"type\": \"pendietes\",\r\n                \"attributes\": {\r\n                    \"fecha\": (selectedRango != global.Constants.RANGOS.TODOS) ? date : '',\r\n                    'per_page': paganationPerPages,\r\n                    'current_page': '1',\r\n                    \"usuario_actual\": nombreUsuario\r\n                }\r\n            }\r\n        }\r\n\r\n        console.log(JSON.stringify(data));\r\n\r\n        PendientesApi.getAllPendientesFilter(data).then(\r\n            datos => {\r\n                if (!datos.error) {\r\n                    setListaPendientes(datos)\r\n                    console.log(datos)\r\n                }\r\n                else {\r\n                    setErrorApi(datos.error.toString())\r\n                    window.showModal()\r\n                }\r\n                window.showSpinner(false);\r\n            }\r\n        )\r\n\r\n\r\n    }\r\n    return (\r\n        <>\r\n\r\n            {<InfoErrorApi error={errorApi} />}\r\n            {<InfoExitoApi />}\r\n            {<Spinner />}\r\n            {<ModalRemitirExpediente object={getDatosParaRemitir} />}\r\n            <Formik\r\n                initialValues={{\r\n                    rango: ''\r\n                }}\r\n                enableReinitialize\r\n                validate={(valores) => {\r\n                    let errores = {}\r\n                    return errores\r\n                }}\r\n                onSubmit={(valores, { resetForm }) => {\r\n\r\n                    enviarDatos(valores);\r\n                }}\r\n            >\r\n\r\n                {({ errors }) => (\r\n                    <Form>\r\n\r\n                  