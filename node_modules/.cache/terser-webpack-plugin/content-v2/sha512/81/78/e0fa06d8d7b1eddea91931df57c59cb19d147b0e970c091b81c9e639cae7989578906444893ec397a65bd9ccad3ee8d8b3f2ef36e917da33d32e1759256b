\n                \"attributes\": {\r\n                    \"id_proceso_disciplinario\": procesoDisciplinarioId,\r\n                    \"noticia_priorizada\": datos.NoticiaPrio,\r\n                    \"justificacion\": datos.Justificacion,\r\n                    \"estado\": global.Constants.ESTADOS_EVALUACION.REGISTRADO,\r\n                    \"resultado_evaluacion\": datos.ResulEvaluacion,\r\n                    \"tipo_conducta\": datos.tipoConducta,\r\n                    \"estado_evaluacion\": true\r\n                }\r\n            }\r\n        }\r\n\r\n        // console.log(JSON.stringify(data));\r\n\r\n        if (getTipoProcesoDisciplinario == global.Constants.TIPO_INGRESO.PODER_PREFERENTE) {\r\n\r\n            GenericApi.addGeneric('evaluacion', data).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        setModalState({ title: getNombreProceso + \" :: Evaluación del radicado\", message: 'Registro exitoso', show: true, redirect: '/RamasProceso', from: { from }, alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                    }\r\n                    else {\r\n                        setModalState({ title: getNombreProceso + \" :: Evaluación del radicado\", message: datos.error.toString(), show: true, redirect: null, from: { from }, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                    }\r\n                    window.showSpinner(false);\r\n                }\r\n            )\r\n\r\n        }\r\n        else {\r\n\r\n            GenericApi.addGeneric('evaluacion', data).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        if (datos.usuario_remitido) {\r\n                            setModalState({ title: getNombreProceso + \" :: Evaluación del radicado\", message: 'El proceso disciplinario es asignado a ' + datos.usuario_remitido, show: true, redirect: '/MisPendientes', from: { from }, alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                        }\r\n                        else {\r\n                            setModalState({ title: getNombreProceso + \" :: Evaluación del radicado\", message: 'En estos momentos no es posible traer la información.', show: true, redirect: null, from: { from }, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                        }\r\n                    }\r\n                    else {\r\n                        setModalState({ title: getNombreProceso + \" :: Evaluación del radicado\", message: datos.error.toString(), show: true, redirect: null, from: { from }, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                    }\r\n                    window.showSpinner(false);\r\n                }\r\n            )\r\n\r\n        }\r\n\r\n\r\n    }\r\n\r\n    const componentEvaluacionForm = () => {\r\n     \r\n            return (\r\n                <>\r\n                    <Formik\r\n                        initialValues={{\r\n                            ResulEvaluacion: '',\r\n                            NoticiaPrio: '',\r\n                            tipoConducta: '',\r\n                            Justificacion: ''\r\n                        }}\r\n                        validate={(valores) => {\r\n                            let errores = {};\r\n                            setCountTextArea(valores.Justificacion.length)\r\n                            if (!valores.ResulEvaluacion) {\r\n                                errores.ResulEvaluacion = 'Debe ingresar un resultado'\r\n                            }\r\n\r\n                            if (!valores.NoticiaPrio) {\r\n                                errores.NoticiaPrio = 'Debe ingresar si es una noticia priorizada'\r\n                            }\r\n\r\n                            if (!valores.tipoConducta) {\r\n                                errores.tipoConducta = 'Debe ingresar tipo de conducta'\r\n                            }\r\n\r\n\r\n                            if (!valores.Justificacion) {\r\n                                errores.Justificacion = 'Debe ingresar una justificación'\r\n                            }\r\n\r\n                            if (valores.Justificacion) {\r\n                                if (valores.Justificacion.length < getMinimoTextArea) {\r\n                                    errores.Justificacion = 'Debe ingresar mínimo ' + getMinimoTextArea + ' caracteres'\r\n                                }\r\n\r\n                                if (containsSpecialChars(valores.Justificacion))\r\n                                    errores.Justificacion = 'Tiene caracteres inválidos'\r\n                            }\r\n                            return errores\r\n                        }}\r\n                        onSubmit={(valores, { resetForm }) => {\r\n                            enviarDatos(valores);\r\n                        }}>\r\n\r\n                        {({ errors }) => (\r\n                            <Form>\r\n                                <div className='row'>\r\n\r\n                                    {getEsJefe && (getEstadoEvaluacion == global.Constants.ESTADOS_EVALUACION.APROBADO_POR_JEFE || getEstadoEvaluacion == global.Constants.ESTADOS_EVALUACION.RECHAZADO_POR_JEFE)?\r\n\r\n                                    <div className='col-md-12'>\r\n                                        \r\n                                        <div class=\"alert alert-warning\" role=\"alert\">\r\n                                            <strong>RECLASIFICACIÓN DE LA EVALUACIÓN</strong><br/><br/>\r\n                                            Recuerde que una vez realice la reclasificación deberá iniciar nuevamente el proceso en la fases subsecuentes de la etapa evaluación.\r\n                                        </div>\r\n\r\n                                    </div>\r\n                                    :\r\n                                    <div className='col-md-12'>\r\n                                        <d