                   window.showSpinner(false);\r\n                }\r\n            )\r\n\r\n        }\r\n        else {\r\n\r\n            GenericApi.addGeneric('evaluacion', data).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        if (datos.usuario_remitido) {\r\n                            setModalState({ title: getNombreProceso + \" :: Evaluación del radicado\", message: 'El proceso disciplinario es asignado a ' + datos.usuario_remitido, show: true, redirect: '/MisPendientes', from: { from }, alert: global.Constants.TIPO_ALERTA.EXITO });\r\n                        }\r\n                        else {\r\n                            setModalState({ title: getNombreProceso + \" :: Evaluación del radicado\", message: 'En estos momentos no es posible traer la información.', show: true, redirect: null, from: { from }, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                        }\r\n                    }\r\n                    else {\r\n                        setModalState({ title: getNombreProceso + \" :: Evaluación del radicado\", message: datos.error.toString(), show: true, redirect: null, from: { from }, alert: global.Constants.TIPO_ALERTA.ERROR });\r\n                    }\r\n                    window.showSpinner(false);\r\n                }\r\n            )\r\n\r\n        }\r\n\r\n\r\n    }\r\n\r\n    const componentEvaluacionForm = () => {\r\n     \r\n            return (\r\n                <>\r\n                    <Formik\r\n                        initialValues={{\r\n                            ResulEvaluacion: '',\r\n                            NoticiaPrio: '',\r\n                            tipoConducta: '',\r\n                            Justificacion: ''\r\n                        }}\r\n                        validate={(valores) => {\r\n                            let errores = {};\r\n                            setCountTextArea(valores.Justificacion.length)\r\n                           