}\r\n                else {\r\n                    // console.log(\"Error en la consulta de la los datos de las transacciones\");\r\n                }\r\n\r\n            }\r\n        )\r\n    }\r\n\r\n    const handleClicArchivoManual = () => {\r\n        try {\r\n            window.showSpinner(true);\r\n\r\n            const data = {\r\n                \"data\": {\r\n                    \"type\": \"mas_actuaciones\",\r\n                    \"attributes\": {\r\n                        \"nombre_actuacion\": \"\",\r\n                        \"nombre_plantilla\": \"\",\r\n                        \"id_etapa\": \"\",\r\n                        \"estado\": \"\",\r\n                        \"id_etapa_despues_aprobacion\": \"\",\r\n                        \"despues_aprobacion_listar_actuacion\": \"\",\r\n                        \"nombre_plantilla_manual\": \"\",\r\n                        \"fileBase64\": \"\",\r\n                        \"fileBase64_manual\": \"\"\r\n                    }\r\n                }\r\n            }\r\n\r\n            GenericApi.getByDataGeneric('mas_actuaciones/manual/' + idActu, data).then(\r\n                datos => {\r\n                    if (!datos.error) {\r\n                        downloadBase64FileManual(datos.content_type, datos.base_64, datos.file_name);\r\n                    }\r\n                    else {\r\n\r\n                    }\r\n                    window.showSpinner(false);\r\n                }\r\n            )\r\n\r\n        } catch (error) {\r\n            console.error(error);\r\n        }\r\n    };\r\n\r\n    function downloadBase64FileManual(contentType, base64Data, fileName) {\r\n        const linkSource = `data:${contentType};base64,${base64Data}`;\r\n        const downloadLink = document.createElement(\"a\");\r\n        downloadLink.href = linkSource;\r\n        downloadLink.download = fileName;\r\n        downloadLink.click();\r\n    }\r\n\r\n    return (\r\n\r\n        <>\r\n            {<Spinner />}\r\n\r\n            <div className=\"w2d_block\">\r\n                <nav aria-label=\"breadcrumb\">\r\n                    <ol className=\"breadcrumb breadcrumb-alt push\">\r\n                        <li class=\"breadcrumb-item\"> <Link underline=\"hover\" className=\"text-dark\" to={`/MisPendientes`}><small>Mis Pendientes</small></Link></li>\r\n                        <li class=\"breadcrumb-item\"> <Link underline=\"hover\" className=\"text-dark\" to={`/RamasProceso/`} state={{ from: from }}><small>Ramas del proceso</small></Link></li>\r\n                        <li class=\"breadcrumb-item\"> <Link underline=\"hover\" className=\"text-dark\" to={`/ActuacionesLista/`} state={{ from: from, selected_id_etapa: selected_id_etapa }}><small>Actuaciones</small></Link></li>\r\n                        <li class=\"breadcrumb-item\"> <small>Transacciones</small></li>\r\n                    </ol>\r\n                </nav>\r\n            </div>\r\n\r\n            <div className=\"block block-themed\">\r\n                <div className=\"block-header\">\r\n                    <h3 className=\"block-title\">{getNombreProceso} :: <strong>Transacciones</strong></h3>\r\n                </div>\r\n                <div className=\"block-content row\">\r\n                    <table className=\"table table-striped table-borderless table-vcenter\">\r\n                        <thead className=\"thead-light\">\r\n                            <tr>\r\n                                <th colSpan=\"2\">\r\n                                    {idActu != null ? (\r\n                                        \"En el siguiente boton puedes descargar una guia :\"\r\n                                    ) : null}\r\n                                    {idActu != null ? (\r\n\r\n                                        <button type='button' title='Descargar documento' className='btn btn-rounded btn-primary ml-3' onClick={() => handleClicArchivoManual()}>\r\n                                            Descargar Manual\r\n                                        </button>\r\n\r\n                                    ) : null}\r\n\r\n                                    <div className=\"text-muted mt-2 mb-1\">Donde deseas asignar el proceso con radicado {from.radicado} ?</div>\r\n                                </th>\r\n                            </tr>\r\n                        </thead>\r\n                        <tbody>\r\n\r\n                            <tr>\r\n                                <td className=\"text-center\">\r\n                                    <i className=\"fa fa-check text-success fa-2x\"></i>\r\n                                </td>\r\n                                <td>\r\n                                    <Link to={'/MisPendientes'} className=\"font-size-h5 font-w600\">Dejar en mis pendientes</Link>\r\n                                    <div className=\"text-muted mt-2 mb-1\">{getTextoTransaccion1}</div>\r\n                                </td>\r\n                            </tr>\r\n                            {(hasAccess('Transacciones', 'EnviarUsuarioDependencia')) ? (\r\n                                <tr>\r\n                                    <td className=\"text-center\">\r\n                                        <i className=\"fa fa-user text-success fa-2x\"></i>\r\n                                    </td>\r\n                                    <td>\r\n                                        <Link to={'/EnviaraAlguienDeMiDependencia'} state={{ from: from, selected_id_etapa: selected_id_etapa }} className=\"font-size-h5 font-w600\" >Enviar a alguien de mi dependencia</Link>\r\n                                        <div className=\"text-muted mt-2 mb-1\">{getTextoTransaccion2}</div>\r\n                                    </td>\r\n                                </tr>\r\n                            ) : null}\r\n                            {(hasAccess('Transacciones', 'EnviarAJefe')) && getRtaJefe == false ? (\r\n                                <tr>\r\n                                    <td className=\"text-center\">\r\n                                        <i className=\"fa fa-check-double text-success fa-2x\"></i>\r\n                                    </td>\r\n                                    <td>\r\n                                        <Link to={`/EnviarAlJefeDeMiDependencia`} state={{ from: from, selected_id_etapa: selected_id_etapa }} className=\"font-size-h5 font-w600\" >Enviar a jefe de la dependencia</Link>\r\n                                        <div className=\"text-muted mt-2 mb-1\">{getTextoTransaccion3}</div>\r\n                                    </td>\r\n                                </tr>\r\n                            ) : null}\r\n                            {(hasAccess('Transacciones', 'EnviarOtraDependencia')) && getRtaJefe == true ? (\r\n                                <tr>\r\n                                    <td className=\"text-center\">\r\n                                        <i className=\"fa fa-paper-plane text-success fa-2x\"></i>\r\n                                    </td>\r\n                                    <td>\r\n                                        <Link to={`/EnviarAotraDependencia`} state={{ from: from, selected_id_etapa: selected_id_etapa }} className=\"font-size-h5 font-w600\" >Enviar a otra dependencia</Link>\r\n                                        <div className=\"text-muted mt-2 mb-1\">{getTextoTransaccion4}</div>\r\n                                    </td>\r\n                                </tr>\r\n                            ) : null}\r\n                            {(hasAccess('Transacciones', 'RegresarUltimoUsuario')) ? (\r\n                                <tr>\r\n                                    <td className=\"text-center\">\r\n                                        <i className=\"fa fa-undo-alt text-success fa-2x\"></i>\r\n                                    </td>\r\n                                    <td>\r\n                                        <Link to={'/EnviarAlAnteriorUsuario'} state={{ from: from, selected_id_etapa: selected_id_etapa }} className=\"font-size-h5 font-w600\" >Regresar proceso al último usuario</Link>\r\n                                        <div className=\"text-muted mt-2 mb-1\">{getTextoTransaccion5}</div>\r\n                                    </td>\r\n                                </tr>\r\n                            ) : null}\r\n                            {(hasAccess('Transacciones', 'EnviarDirigidoSecretariaComun')) ? (\r\n                                <tr>\r\n                                    <td className=\"text-center\">\r\n                                        <i className=\"fa fa-arrow-right text-success fa-2x\"></i>\r\n                                    </td>\r\n                                    <td>\r\n                                        <Link to={'/EnviaraAlguienDeSecretariaComunDirigido'} state={{ from: from, selected_id_etapa: selected_id_etapa }} className=\"font-size-h5 font-w600\" >Enviar a alguien de secretaria común dirigidamente</Link>\r\n                                        <div className=\"text-muted mt-2 mb-1\">{getTextoTransaccion6}</div>\r\n                                    </td>\r\n                                </tr>\r\n                            ) : null}\r\n                            {(hasAccess('Transacciones', 'EnviarAleatorioSecretariaComun')) ? (\r\n                                <tr>\r\n                                    <td className=\"text-center\">\r\n                                        <i className=\"fa fa-arrows-alt text-success fa-2x\"></i>\r\n                                    </td>\r\n                                    <td>\r\n                                        <Link to={'/EnviaraAlguienDeSecretariaComunAleatorio'} state={{ from: from, selected_id_etapa: selected_id_etapa }} className=\"font-size-h5 font-w600\" >Enviar a alguien de secretaria común aleatoriamente</Link>\r\n           