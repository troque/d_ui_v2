ALES.formatOnlyLetters.test(value)) {\r\n            setValue('titulo', value);\r\n        }\r\n        else{\r\n            setValue('titulo', value.substring(0, value.length -1));\r\n        }\r\n        \r\n    }\r\n\r\n    const model = createSchema('TransaccionesModel', {\r\n        titulo: {\r\n            type: 'string',\r\n            required: true,\r\n            maxLength: 255,\r\n            onChange: (handleChange)\r\n        },\r\n        descripcion: {\r\n            type: 'string',\r\n            \r\n            maxLength: 255\r\n        }\r\n    });\r\n\r\n    return model;\r\n}\r\n\r\n\r\nsetLanguageByName('es')\r\naddTranslations({\r\n    models: {\r\n        TransaccionesModel: {\r\n            titulo: 'Titulo',\r\n            descripcion: 'Descripción',\r\n        }\r\n    },\r\n})\r\n\r\n","import React from 'react';\r\nimport GenericForm from './GenericForm';\r\nimport TransaccionesModel from './Models/TransaccionesModel';\r\n\r\nexport default function CiudadForm() {\r\n\r\n    const itemSelectResolve = (serviceName, value) => {\r\n        return { value: value.id, label: value.attributes.nombre }\r\n    }\r\n\r\n    return (<GenericForm\r\n        formName='TransaccionesAdmin'\r\n        service='TransaccionesAdmin'\r\n        ddlServicesFunction={itemSelectResolve}\r\n        model={TransaccionesModel}\r\n        successRedirect='/TransaccionesAdmin'\r\n    />\r\n    );\r\n};","\r\nexport default global.Constants = {\r\n    ESTADOS: {\r\n        ACTIVO: '1',\r\n        INACTIVO: '0'\r\n    },\r\n\r\n    //MIS PENDIENTES\r\n    RANGOS: {\r\n        TODOS: '-1'\r\n    },\r\n\r\n    BOTONES_RAMAS: {\r\n        PRIMARY: 'btn btn-sm btn-primary w2d_btn-large mr-1 mb-3 text-left',\r\n        SUCCESS: 'btn btn-sm btn-success w2d_btn-large mr-1 mb-3 text-left',\r\n        WARNING: 'btn btn-sm btn-warning w2d_btn-large mr-1 mb-3 text-left'\r\n    },\r\n\r\n    BOTONES_CARPETAS: {\r\n        PRIMARY: 'btn btn-sm btn-primary mr-1 mb-3',\r\n        SUC