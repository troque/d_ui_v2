                  {getRespuestaEstapasCambiar ? selectEtapaCambiar() : null}\r\n                                                    </Field>\r\n                                                    <ErrorMessage name=\"cambiarEtapa\" component={() => (<span className=\"text-danger\">{errors.cambiarEtapa}</span>)} />\r\n                                                </div>\r\n                                            </div>\r\n                                            : null\r\n                                        }\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n\r\n                            <div className=\"block block-themed mt-2\">\r\n                                <div className=\"block-header\">\r\n                                    <h3 className=\"block-title\">Textos informativos de transacciones</h3>\r\n                                </div>\r\n                                <div className=\"block-content\">\r\n                                    <div className=\"col-md-12\">\r\n                                        <div className=\"form-group\">\r\n                                            <i className=\"fa fa-check text-success fa-2x mr-3\"></i>\r\n                                            <label htmlFor=\"dejarEnMisPendientes\">Dejar en mis pendientes</label>\r\n                                            <Field as=\"textarea\" className=\"form-control\" id=\"dejarEnMisPendientes\" rows=\"3\" name=\"dejarEnMisPendientes\" placeholder=\"Información para dejar en mis pendientes....\" value={from != null ? getTextoTransaccion1 : null} onChange={changeTextoTransaccion1}></Field>\r\n                                            <div className=\"text-right\">\r\n                                                <span className=\"text-primary\">{countTextArea1} / {getMaximoTextArea}</span>\r\n                                            </div>\r\n                        